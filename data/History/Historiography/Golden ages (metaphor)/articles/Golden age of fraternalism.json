inks":["http://www.cornerstonepublishers.com/ssoc.pdf","http://harpers.org/archive/1964/11/the-paranoid-style-in-american-politics/","http://phoenixmasonry.org/Golden%20Age%20of%20Fraternalism.pdf","https://books.google.com/books?id=9VMdUMAhfW8C&pg=PT171","http://www.phoenixmasonry.org/masonicmuseum/fraternalism/fraternalism_in_america.htm","http://www.msana.com/msastats.asp"],"sections":[{"toclevel":1,"level":"2","line":"Beginnings","number":"1","index":"1","fromtitle":"Golden_age_of_fraternalism","byteoffset":766,"anchor":"Beginnings"},{"toclevel":1,"level":"2","line":"Rise and peak membership","number":"2","index":"2","fromtitle":"Golden_age_of_fraternalism","byteoffset":2619,"anchor":"Rise_and_peak_membership"},{"toclevel":1,"level":"2","line":"Decline","number":"3","index":"3","fromtitle":"Golden_age_of_fraternalism","byteoffset":5103,"anchor":"Decline"},{"toclevel":1,"level":"2","line":"References","number":"4","index":"4","fromtitle":"Golden_age_of_fraternalism","byteoffset":6611,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Golden age of fraternalism","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q24963936"}]}}