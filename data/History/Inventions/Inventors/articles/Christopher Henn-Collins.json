,{"ns":0,"exists":"","*":"Guerrilla war"},{"ns":0,"exists":"","*":"HMS Shropshire (73)"},{"ns":0,"exists":"","*":"Institute of Electrical and Electronics Engineers"},{"ns":0,"exists":"","*":"Institution of Electrical Engineers"},{"ns":0,"exists":"","*":"Institution of Engineering and Technology"},{"ns":0,"exists":"","*":"Invasion of Poland (1939)"},{"ns":0,"exists":"","*":"Lieutenant-Colonel"},{"ns":0,"exists":"","*":"Lieutenant-Colonel (United Kingdom)"},{"ns":0,"exists":"","*":"Lieutenant-colonel"},{"ns":0,"exists":"","*":"Lord Collins"},{"ns":0,"exists":"","*":"Louis Essen"},{"ns":0,"exists":"","*":"Luftwaffe"},{"ns":0,"exists":"","*":"Lvov"},{"ns":0,"exists":"","*":"Major (United Kingdom)"},{"ns":0,"exists":"","*":"Marseilles"},{"ns":0,"exists":"","*":"Master of the Rolls"},{"ns":0,"exists":"","*":"Ministry of Supply"},{"ns":0,"exists":"","*":"North African Campaign"},{"ns":0,"exists":"","*":"Order of St Michael and St George"},{"ns":0,"exists":"","*":"Palestine (region)"},{"ns":0,"exists":"","*":"Poland"},{"ns":0,"exists":"","*":"Polish Campaign"},{"ns":0,"exists":"","*":"Quartz clock"},{"ns":0,"exists":"","*":"Richard Henn Collins"},{"ns":0,"exists":"","*":"Romania"},{"ns":0,"exists":"","*":"Royal Military Academy, Woolwich"},{"ns":0,"exists":"","*":"Russians"},{"ns":0,"exists":"","*":"Second World War"},{"ns":0,"exists":"","*":"Shrewsbury"},{"ns":0,"exists":"","*":"Staff College, Camberley"},{"ns":0,"exists":"","*":"Tarnopol"},{"ns":0,"exists":"","*":"Thermionic valve"},{"ns":0,"exists":"","*":"Transistor"},{"ns":0,"exists":"","*":"Turkey"},{"ns":0,"exists":"","*":"Victoria Cross"},{"ns":0,"exists":"","*":"Warsaw"},{"ns":0,"exists":"","*":"Woolwich"},{"ns":0,"*":"Henn-Collins Associates"}],"templates":[{"ns":10,"exists":"","*":"Template:Use dmy dates"},{"ns":10,"exists":"","*":"Template:DMCA"},{"ns":10,"exists":"","*":"Template:Dated maintenance category"},{"ns":10,"exists":"","*":"Template:FULLROOTPAGENAME"},{"ns":10,"exists":"","*":"Template:Ns has subpages