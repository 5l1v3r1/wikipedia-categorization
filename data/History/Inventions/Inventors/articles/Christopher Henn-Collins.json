al life","number":"5","index":"5","fromtitle":"Christopher_Henn-Collins","byteoffset":6949,"anchor":"Personal_life"},{"toclevel":1,"level":"2","line":"References","number":"6","index":"6","fromtitle":"Christopher_Henn-Collins","byteoffset":7079,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"7","index":"7","fromtitle":"Christopher_Henn-Collins","byteoffset":7234,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"Christopher Henn-Collins","iwlinks":[],"properties":[{"name":"defaultsort","*":"Henn-Collins, Christopher"},{"name":"wikibase_item","*":"Q5112528"}]}}