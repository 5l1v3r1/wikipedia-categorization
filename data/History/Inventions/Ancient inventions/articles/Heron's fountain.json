nchor":"Construction"},{"toclevel":1,"level":"2","line":"Motion","number":"2","index":"2","fromtitle":"Heron's_fountain","byteoffset":1981,"anchor":"Motion"},{"toclevel":1,"level":"2","line":"Geological phenomena","number":"3","index":"3","fromtitle":"Heron's_fountain","byteoffset":3917,"anchor":"Geological_phenomena"},{"toclevel":1,"level":"2","line":"In popular culture","number":"4","index":"4","fromtitle":"Heron's_fountain","byteoffset":4532,"anchor":"In_popular_culture"},{"toclevel":1,"level":"2","line":"See also","number":"5","index":"5","fromtitle":"Heron's_fountain","byteoffset":4831,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"Notes","number":"6","index":"6","fromtitle":"Heron's_fountain","byteoffset":4905,"anchor":"Notes"},{"toclevel":1,"level":"2","line":"References","number":"7","index":"7","fromtitle":"Heron's_fountain","byteoffset":4932,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"8","index":"8","fromtitle":"Heron's_fountain","byteoffset":5227,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"Heron's fountain","iwlinks":[{"prefix":"commons","url":"https://commons.wikimedia.org/wiki/Category:Heron%27s_fountain","*":"commons:Category:Heron's fountain"}],"properties":[{"name":"defaultsort","*":"Heron's Fountain"},{"name":"wikibase_item","*":"Q1613860"}]}}