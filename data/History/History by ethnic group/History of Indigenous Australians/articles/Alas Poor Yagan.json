ia"},{"sortkey":"","*":"Cartoon_controversies"}],"links":[{"ns":14,"exists":"","*":"Category:Use Australian English from April 2018"},{"ns":14,"exists":"","*":"Category:Use dmy dates from April 2018"},{"ns":0,"exists":"","*":"Activist"},{"ns":0,"exists":"","*":"Cemetery"},{"ns":0,"exists":"","*":"Dean Alston"},{"ns":0,"exists":"","*":"Death"},{"ns":0,"exists":"","*":"Editing"},{"ns":0,"exists":"","*":"Editorial cartoon"},{"ns":0,"exists":"","*":"European ethnic groups"},{"ns":0,"exists":"","*":"Exhumation of Yagan's head"},{"ns":0,"exists":"","*":"Federal Court of Australia"},{"ns":0,"exists":"","*":"Human Rights and Equal Opportunity Commission"},{"ns":0,"exists":"","*":"Indigenous Australians"},{"ns":0,"exists":"","*":"Indigenous Law Bulletin"},{"ns":0,"exists":"","*":"International Standard Book Number"},{"ns":0,"exists":"","*":"Ken Colbung"},{"ns":0,"exists":"","*":"Museum"},{"ns":0,"exists":"","*":"Newspaper"},{"ns":0,"exists":"","*":"Nyungar"},{"ns":0,"exists":"","*":"Paul Murray (journalist)"},{"ns":0,"exists":"","*":"Racial Discrimination Act 1975"},{"ns":0,"exists":"","*":"Religion"},{"ns":0,"exists":"","*":"Robert Bropho"},{"ns":0,"exists":"","*":"Stereotype"},{"ns":0,"exists":"","*":"Supreme Court of Western Australia"},{"ns":0,"exists":"","*":"The West Australian"},{"ns":0,"exists":"","*":"United Kingdom"},{"ns":0,"exists":"","*":"Wagyl"},{"ns":0,"exists":"","*":"Western Australia"},{"ns":0,"exists":"","*":"Yagan"},{"ns":0,"*":"Hannah McGlade"}],"templates":[{"ns":10,"exists":"","*":"Template:Use Australian English"},{"ns":10,"exists":"","*":"Template:DMCA"},{"ns":10,"exists":"","*":"Template:Dated maintenance category"},{"ns":10,"exists":"","*":"Template:FULLROOTPAGENAME"},{"ns":10,"exists":"","*":"Template:Ns has subpages"},{"ns":10,"exists":"","*":"Template:Use dmy dates"},{"ns":10,"exists":"","*":"Template:Main"},{"ns":10,"exists":"","*":"Template:Reflist"},{"ns":10,"exists":"","*":"Template:Cite journal"},{"ns":10,"exists":"","*":"Template:Cite book"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":828,"exists":"","*":"Module:Unsubst"},{"ns":828,"exists":"","*":"Module:Ns has subpages"},{"ns":828,"exists":"","*":"Module:Main"},{"ns":828,"exists":"","*":"Module:Hatnote"},{"ns":828,"exists":"","*":"Module:Hatnote list"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:Citation/CS1"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Configuration"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Whitelist"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Utilities"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Date validation"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Identifiers"},{"ns":828,"exists":"","*":"Module:Citation/CS1/COinS"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"}],"images":["Alas_Poor_Yagan.jpg"],"externallinks":["http://www.austlii.edu.au/au/cases/cth/FCAFC/2004/16.html"],"sections":[{"toclevel":1,"level":"2","line":"Background","number":"1","index":"1","fromtitle":"Alas_Poor_Yagan","byteoffset":1089,"anchor":"Background"},{"toclevel":1,"level":"2","line":"Publication","number":"2","index":"2","fromtitle":"Alas_Poor_Yagan","byteoffset":2421,"anchor":"Publication"},{"toclevel":1,"level":"2","line":"Complaint","number":"3","index":"3","fromtitle":"Alas_Poor_Yagan","byteoffset":3044,"anchor":"Complaint"},{"toclevel":1,"level":"2","line":"Analysis","number":"4","index":"4","fromtitle":"Alas_Poor_Yagan","byteoffset":5732,"anchor":"Analysis"},{"toclevel":1,"level":"2","line":"References","number":"5","index":"5","fromtitle":"Alas_Poor_Yagan","byteoffset":7586,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Alas Poor Yagan","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q4708411"}]}}