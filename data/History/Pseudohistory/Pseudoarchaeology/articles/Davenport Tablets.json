:828,"exists":"","*":"Module:Check isxn"},{"ns":828,"exists":"","*":"Module:Error"}],"images":["Text_document_with_red_question_mark.svg","PSM_V51_D101_Slate_tablet_from_davenport.jpg","PSM_V51_D102_Slate_tablet_from_davenport.jpg"],"externallinks":["http://pseudoarchaeology.org/a05/a05-pinsky.htm","http://www.umvphotoarchive.org/cdm4/item_viewer.php?CISOROOT=/putnm&CISOPTR=56&CISOBOX=1&REC=20","http://www.umvphotoarchive.org/cdm4/item_viewer.php?CISOROOT=/putnm&CISOPTR=55&CISOBOX=1&REC=19"],"sections":[{"toclevel":1,"level":"2","line":"Discovery","number":"1","index":"1","fromtitle":"Davenport_Tablets","byteoffset":262,"anchor":"Discovery"},{"toclevel":1,"level":"2","line":"Interpretations","number":"2","index":"2","fromtitle":"Davenport_Tablets","byteoffset":1163,"anchor":"Interpretations"},{"toclevel":1,"level":"2","line":"See also","number":"3","index":"3","fromtitle":"Davenport_Tablets","byteoffset":4475,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"4","index":"4","fromtitle":"Davenport_Tablets","byteoffset":4805,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"5","index":"5","fromtitle":"Davenport_Tablets","byteoffset":5627,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"Davenport Tablets","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q5230266"}]}}