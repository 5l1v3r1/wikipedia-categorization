level":"2","line":"Criticism","number":"2","index":"2","fromtitle":"The_Jesus_Papers","byteoffset":3026,"anchor":"Criticism"},{"toclevel":1,"level":"2","line":"Interviews","number":"3","index":"3","fromtitle":"The_Jesus_Papers","byteoffset":5549,"anchor":"Interviews"},{"toclevel":1,"level":"2","line":"Editions","number":"4","index":"4","fromtitle":"The_Jesus_Papers","byteoffset":6298,"anchor":"Editions"},{"toclevel":1,"level":"2","line":"See also","number":"5","index":"5","fromtitle":"The_Jesus_Papers","byteoffset":6448,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"6","index":"6","fromtitle":"The_Jesus_Papers","byteoffset":6511,"anchor":"References"},{"toclevel":1,"level":"2","line":"Further reading","number":"7","index":"7","fromtitle":"The_Jesus_Papers","byteoffset":6539,"anchor":"Further_reading"},{"toclevel":1,"level":"2","line":"External links","number":"8","index":"8","fromtitle":"The_Jesus_Papers","byteoffset":6673,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"The Jesus Papers","iwlinks":[],"properties":[{"name":"defaultsort","*":"Jesus Papers, The"},{"name":"wikibase_item","*":"Q3848848"}]}}