dex":"4","fromtitle":"Blond_Eskimos","byteoffset":4685,"anchor":"Scientific_investigation"},{"toclevel":1,"level":"2","line":"See also","number":"5","index":"5","fromtitle":"Blond_Eskimos","byteoffset":5619,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"6","index":"6","fromtitle":"Blond_Eskimos","byteoffset":5696,"anchor":"References"},{"toclevel":1,"level":"2","line":"Further reading","number":"7","index":"7","fromtitle":"Blond_Eskimos","byteoffset":5729,"anchor":"Further_reading"}],"parsewarnings":[],"displaytitle":"Blond Eskimos","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q4927438"}]}}