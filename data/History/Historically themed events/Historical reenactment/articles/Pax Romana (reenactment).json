ts":"","*":"Murmillo"},{"ns":0,"exists":"","*":"Nero Claudius Drusus"},{"ns":0,"exists":"","*":"Netherlands"},{"ns":0,"exists":"","*":"North Sea"},{"ns":0,"exists":"","*":"Retiarius"},{"ns":0,"exists":"","*":"Revolt of Saturninus"},{"ns":0,"exists":"","*":"Rhine"},{"ns":0,"exists":"","*":"Rhine river"},{"ns":0,"exists":"","*":"Romans in the Netherlands"},{"ns":0,"exists":"","*":"Secutor"},{"ns":0,"exists":"","*":"Thraex"},{"ns":0,"exists":"","*":"Utrecht (city)"},{"ns":0,"exists":"","*":"Woerden"},{"ns":0,"exists":"","*":"Xanten"},{"ns":0,"*":"LPLG"},{"ns":0,"*":"Cohors XV Voluntariorum Civium Romanorum"}],"templates":[{"ns":10,"exists":"","*":"Template:Reenactment group"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":10,"exists":"","*":"Template:Reflist"},{"ns":10,"exists":"","*":"Template:Column-width"},{"ns":10,"exists":"","*":"Template:Refbegin"},{"ns":10,"exists":"","*":"Template:Refbegin/styles.css"},{"ns":10,"exists":"","*":"Template:Refend"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"}],"images":["Cohors_xv.jpg","Classis.jpg"],"externallinks":["http://www.livius.org/cn-cs/cologne/alteburg.html","http://www.paxromana.eu"],"sections":[{"toclevel":1,"level":"2","line":"Origin","number":"1","index":"1","fromtitle":"Pax_Romana_(reenactment)","byteoffset":512,"anchor":"Origin"},{"toclevel":1,"level":"2","line":"Nature and purpose","number":"2","index":"2","fromtitle":"Pax_Romana_(reenactment)","byteoffset":756,"anchor":"Nature_and_purpose"},{"toclevel":1,"level":"2","line":"Units portrayed","number":"3","index":"3","fromtitle":"Pax_Romana_(reenactment)","byteoffset":1688,"anchor":"Units_portrayed"},{"toclevel":2,"level":"3","line":"Cohors XV Voluntariorum Civium Romanorum","number":"3.1","index":"4","fromtitle":"Pax_Romana_(reenactment)","byteoffset":1859,"anchor":"Cohors_XV_Voluntariorum_Civium_Romanorum"},{"toclevel":2,"level":"3","line":"Civilians","number":"3.2","index":"5","fromtitle":"Pax_Romana_(reenactment)","byteoffset":2672,"anchor":"Civilians"},{"toclevel":1,"level":"2","line":"Projects","number":"4","index":"6","fromtitle":"Pax_Romana_(reenactment)","byteoffset":3172,"anchor":"Projects"},{"toclevel":2,"level":"3","line":"Classis Augusta Germanica","number":"4.1","index":"7","fromtitle":"Pax_Romana_(reenactment)","byteoffset":3271,"anchor":"Classis_Augusta_Germanica"},{"toclevel":2,"level":"3","line":"Gladiators","number":"4.2","index":"8","fromtitle":"Pax_Romana_(reenactment)","byteoffset":4694,"anchor":"Gladiators"},{"toclevel":2,"level":"3","line":"Cohortes Urbanae","number":"4.3","index":"9","fromtitle":"Pax_Romana_(reenactment)","byteoffset":4897,"anchor":"Cohortes_Urbanae"},{"toclevel":1,"level":"2","line":"Notes","number":"5","index":"10","fromtitle":"Pax_Romana_(reenactment)","byteoffset":5021,"anchor":"Notes"},{"toclevel":1,"level":"2","line":"References","number":"6","index":"11","fromtitle":"Pax_Romana_(reenactment)","byteoffset":5046,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Pax Romana (reenactment)","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q17092358"}]}}