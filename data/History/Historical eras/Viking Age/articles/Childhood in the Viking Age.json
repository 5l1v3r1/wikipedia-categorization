el":1,"level":"2","line":"Agency","number":"2","index":"2","fromtitle":"Childhood_in_the_Viking_Age","byteoffset":2217,"anchor":"Agency"},{"toclevel":1,"level":"2","line":"Inheritance","number":"3","index":"3","fromtitle":"Childhood_in_the_Viking_Age","byteoffset":2625,"anchor":"Inheritance"},{"toclevel":1,"level":"2","line":"Fun and Games","number":"4","index":"4","fromtitle":"Childhood_in_the_Viking_Age","byteoffset":3468,"anchor":"Fun_and_Games"},{"toclevel":1,"level":"2","line":"See also","number":"5","index":"5","fromtitle":"Childhood_in_the_Viking_Age","byteoffset":4221,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"6","index":"6","fromtitle":"Childhood_in_the_Viking_Age","byteoffset":4359,"anchor":"References"},{"toclevel":1,"level":"2","line":"Sources","number":"7","index":"7","fromtitle":"Childhood_in_the_Viking_Age","byteoffset":4389,"anchor":"Sources"}],"parsewarnings":[],"displaytitle":"Childhood in the Viking Age","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q28224848"}]}}