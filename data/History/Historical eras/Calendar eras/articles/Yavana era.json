ates":[{"ns":10,"exists":"","*":"Template:Main"},{"ns":10,"exists":"","*":"Template:Reflist"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":828,"exists":"","*":"Module:Main"},{"ns":828,"exists":"","*":"Module:Hatnote"},{"ns":828,"exists":"","*":"Module:Hatnote list"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"}],"images":["Demetrios_I_Baktria_Tetradrachm_200-185_BC.jpg","Plato_Tetradrachm_MZ.jpg","Yavanarajya_inscription.jpg"],"externallinks":["https://books.google.com/books?id=_TIU_jp93xUC&pg=PA103","https://books.google.com/books?id=X7Cb8IkZVSMC&pg=PA254","https://books.google.com/books?id=X7Cb8IkZVSMC&pg=PA8"],"sections":[{"toclevel":1,"level":"2","line":"Coins of Plato","number":"1","index":"1","fromtitle":"Yavana_era","byteoffset":1313,"anchor":"Coins_of_Plato"},{"toclevel":1,"level":"2","line":"Bajaur reliquary","number":"2","index":"2","fromtitle":"Yavana_era","byteoffset":2794,"anchor":"Bajaur_reliquary"},{"toclevel":1,"level":"2","line":"Yavanarajya inscription","number":"3","index":"3","fromtitle":"Yavana_era","byteoffset":4222,"anchor":"Yavanarajya_inscription"},{"toclevel":1,"level":"2","line":"References","number":"4","index":"4","fromtitle":"Yavana_era","byteoffset":5699,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Yavana era","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q28173769"}]}}