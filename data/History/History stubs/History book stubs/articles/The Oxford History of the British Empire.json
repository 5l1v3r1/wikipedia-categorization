sts":"","*":"Module:Navbar"}],"images":["Oxford_History_of_the_British_Empire_Vol._I.jpg","Hourglass.svg"],"externallinks":["https://global.oup.com/academic/content/series/o/oxford-history-of-the-british-empire-ohbe/?cc=gb&lang=en&","http://ukcatalogue.oup.com/product/9780198205623.do","https://global.oup.com/academic/content/series/o/oxford-history-of-the-british-empire-companion-series-ohbecs/?cc=gb&lang=en&","http://www.historytoday.com/max-beloff/oxford-history-british-empire-volume-i","http://www.h-net.org/reviews/showrev.php?id=6149."],"sections":[{"toclevel":1,"level":"2","line":"Volumes","number":"1","index":"1","fromtitle":"The_Oxford_History_of_the_British_Empire","byteoffset":1548,"anchor":"Volumes"},{"toclevel":1,"level":"2","line":"Reviews","number":"2","index":"2","fromtitle":"The_Oxford_History_of_the_British_Empire","byteoffset":3711,"anchor":"Reviews"},{"toclevel":1,"level":"2","line":"See also","number":"3","index":"3","fromtitle":"The_Oxford_History_of_the_British_Empire","byteoffset":5455,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"4","index":"4","fromtitle":"The_Oxford_History_of_the_British_Empire","byteoffset":5511,"anchor":"References"}],"parsewarnings":[],"displaytitle":"<i>The Oxford History of the British Empire</i>","iwlinks":[],"properties":[{"name":"displaytitle","*":"<i>The Oxford History of the British Empire</i>"},{"name":"defaultsort","*":"Oxford History of the British Empire"},{"name":"wikibase_item","*":"Q20055579"}]}}