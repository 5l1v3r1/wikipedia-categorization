sts":"","*":"State collapse"},{"ns":0,"exists":"","*":"Steve Fielding"},{"ns":0,"exists":"","*":"The Guardian"},{"ns":0,"exists":"","*":"Tsar Alexander II"},{"ns":0,"exists":"","*":"Two-state solution"},{"ns":0,"exists":"","*":"Vaclav Havel"},{"ns":0,"exists":"","*":"Vietnam syndrome"},{"ns":0,"exists":"","*":"Weimar republic"},{"ns":0,"exists":"","*":"White revolution"},{"ns":0,"exists":"","*":"William Roger Louis"},{"ns":0,"exists":"","*":"Ze'ev Sternhell"},{"ns":0,"*":"Marta Latek"},{"ns":0,"*":"Robert C. Rowland"},{"ns":0,"*":"David Frank (writer)"},{"ns":0,"*":"Andrea Shalal"},{"ns":0,"*":"David Schenker"},{"ns":0,"*":"The Washington Institute"},{"ns":0,"*":"Abbas Alnasrawi"},{"ns":0,"*":"The Rise and Fall of The Great Powers"},{"ns":0,"*":"Yakir Plesner"},{"ns":0,"*":"Dahlia Scheindlin"}],"templates":[{"ns":10,"exists":"","*":"Template:Reflist"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"}],"images":[],"externallinks":[],"sections":[{"toclevel":1,"level":"2","line":"History of the concept","number":"1","index":"1","fromtitle":"Political_stagnation","byteoffset":2166,"anchor":"History_of_the_concept"},{"toclevel":1,"level":"2","line":"Examples","number":"2","index":"2","fromtitle":"Political_stagnation","byteoffset":3590,"anchor":"Examples"},{"toclevel":1,"level":"2","line":"See also","number":"3","index":"3","fromtitle":"Political_stagnation","byteoffset":5457,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"4","index":"4","fromtitle":"Political_stagnation","byteoffset":5656,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Political stagnation","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q48999307"}]}}