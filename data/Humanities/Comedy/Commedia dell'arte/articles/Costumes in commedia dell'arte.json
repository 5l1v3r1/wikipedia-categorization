parated entries"},{"ns":10,"exists":"","*":"Template:Reflist"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":828,"exists":"","*":"Module:Separated entries"},{"ns":828,"exists":"","*":"Module:TableTools"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"}],"images":["Commedia_dell'arte_-_troupe_Gelosi.JPG","Francesco_Andreini_as_Capitano_Spavento_after_Poccetti_-_I_Comici_Italiani_1897_p57.jpg","Dottore4.png","SAND_Maurice_Masques_et_bouffons_07.jpg","Andr\u00e9_Derain_-_Arlequin_et_Pierrot.jpg"],"externallinks":[],"sections":[{"toclevel":1,"level":"2","line":"References","number":"1","index":"1","fromtitle":"Costumes_in_commedia_dell'arte","byteoffset":6325,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Costumes in commedia dell'arte","iwlinks":[{"prefix":"it","url":"https://it.wikipedia.org/wiki/Peppe_Nappa","*":"it:Peppe Nappa"}],"properties":[{"name":"wikibase_item","*":"Q28229130"}]}}