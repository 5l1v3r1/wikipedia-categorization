"2","index":"2","fromtitle":"Landscape_history","byteoffset":2893,"anchor":"Techniques"},{"toclevel":1,"level":"2","line":"Academic status","number":"3","index":"3","fromtitle":"Landscape_history","byteoffset":4473,"anchor":"Academic_status"},{"toclevel":1,"level":"2","line":"Journals","number":"4","index":"4","fromtitle":"Landscape_history","byteoffset":5125,"anchor":"Journals"},{"toclevel":1,"level":"2","line":"References","number":"5","index":"5","fromtitle":"Landscape_history","byteoffset":5356,"anchor":"References"},{"toclevel":1,"level":"2","line":"Further reading","number":"6","index":"6","fromtitle":"Landscape_history","byteoffset":5386,"anchor":"Further_reading"}],"parsewarnings":[],"displaytitle":"Landscape history","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q6485101"}]}}