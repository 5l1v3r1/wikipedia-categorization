["https://books.google.co.uk/books?id=MpIBAAAAYAAJ&pg=PA84#v=onepage&q&f=true","http://thescholarship.ecu.edu/handle/10342/3929","https://www.cambridge.org/core/journals/medical-history/article/collateral-sciences-in-the-work-of-golding-bird-18141854/2021D22CFD1EEA57CC72F25F4A4AC905","https://doi.org/10.1017%2FS0025727300014794","http://explore.bl.uk/BLVU1:LSCOP-ALL:BLL01018207409"],"sections":[{"toclevel":1,"level":"2","line":"Description","number":"1","index":"1","fromtitle":"Electric_bath_(electrotherapy)","byteoffset":685,"anchor":"Description"},{"toclevel":1,"level":"2","line":"History","number":"2","index":"2","fromtitle":"Electric_bath_(electrotherapy)","byteoffset":2303,"anchor":"History"},{"toclevel":1,"level":"2","line":"References","number":"3","index":"3","fromtitle":"Electric_bath_(electrotherapy)","byteoffset":5088,"anchor":"References"},{"toclevel":1,"level":"2","line":"Bibliography","number":"4","index":"4","fromtitle":"Electric_bath_(electrotherapy)","byteoffset":5123,"anchor":"Bibliography"}],"parsewarnings":[],"displaytitle":"Electric bath (electrotherapy)","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q55610702"},{"name":"wikibase-badge-Q17437798","*":""}]}}