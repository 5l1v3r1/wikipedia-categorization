ts":"","*":"Kingdom of Denmark"},{"ns":0,"exists":"","*":"Kingdom of England"},{"ns":0,"exists":"","*":"Kingdom of France"},{"ns":0,"exists":"","*":"Latin"},{"ns":0,"exists":"","*":"Martin Heidegger"},{"ns":0,"exists":"","*":"Martin of Dacia"},{"ns":0,"exists":"","*":"Metaphysics"},{"ns":0,"exists":"","*":"Middle Ages"},{"ns":0,"exists":"","*":"Nominalism"},{"ns":0,"exists":"","*":"Ontological"},{"ns":0,"exists":"","*":"Parts of speech"},{"ns":0,"exists":"","*":"Peter Helias"},{"ns":0,"exists":"","*":"Phenomenology (philosophy)"},{"ns":0,"exists":"","*":"Philosophical language"},{"ns":0,"exists":"","*":"Radulphus Brito"},{"ns":0,"exists":"","*":"Reality"},{"ns":0,"exists":"","*":"Roger Bacon"},{"ns":0,"exists":"","*":"Stanford Encyclopedia of Philosophy"},{"ns":0,"exists":"","*":"Summa Grammatica"},{"ns":0,"exists":"","*":"Summa Grammatica (John of Dacia)"},{"ns":0,"exists":"","*":"Thomas of Erfurt"},{"ns":0,"exists":"","*":"Universal grammar"},{"ns":0,"exists":"","*":"William of Conches"},{"ns":0,"*":"John of Dacia"},{"ns":0,"*":"Simonis of Dacia"},{"ns":0,"*":"Siger of Courtrai"}],"templates":[{"ns":10,"exists":"","*":"Template:TOC limit"},{"ns":10,"exists":"","*":"Template:Reflist"},{"ns":10,"exists":"","*":"Template:Column-width"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":10,"exists":"","*":"Template:Cite SEP"},{"ns":10,"exists":"","*":"Template:Cite encyclopaedia"},{"ns":10,"exists":"","*":"Template:Cite encyclopedia"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"},{"ns":828,"exists":"","*":"Module:Citation/CS1"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Configuration"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Whitelist"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Utilities"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Date validation"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Identifiers"},{"ns":828,"exists":"","*":"Module:Citation/CS1/COinS"}],"images":[],"externallinks":["https://plato.stanford.edu/entrie