lish"},{"ns":0,"exists":"","*":"Digraph (orthography)"},{"ns":0,"exists":"","*":"Dz (digraph)"},{"ns":0,"exists":"","*":"D\u017a (digraph)"},{"ns":0,"exists":"","*":"Evolution of the Polish language"},{"ns":0,"exists":"","*":"History of Polish"},{"ns":0,"exists":"","*":"Holy Cross Sermons"},{"ns":0,"exists":"","*":"Jan Kochanowski"},{"ns":0,"exists":"","*":"Komisja Edukacji Narodowej"},{"ns":0,"exists":"","*":"Latin alphabet"},{"ns":0,"exists":"","*":"List of Latin-script digraphs"},{"ns":0,"exists":"","*":"Middle Ages"},{"ns":0,"exists":"","*":"Old Polish language"},{"ns":0,"exists":"","*":"Onufry Kopczy\u0144ski"},{"ns":0,"exists":"","*":"Palatal consonant"},{"ns":0,"exists":"","*":"Partition of Poland"},{"ns":0,"exists":"","*":"Polish Braille"},{"ns":0,"exists":"","*":"Polish alphabet"},{"ns":0,"exists":"","*":"Polish grammar"},{"ns":0,"exists":"","*":"Polish language"},{"ns":0,"exists":"","*":"Polish morphology"},{"ns":0,"exists":"","*":"Polish orthography"},{"ns":0,"exists":"","*":"Polish phonology"},{"ns":0,"exists":"","*":"Polska Akademia Umiej\u0119tno\u015bci"},{"ns":0,"exists":"","*":"Retroflex consonant"},{"ns":0,"exists":"","*":"Sz (digraph)"},{"ns":0,"exists":"","*":"\u0104"},{"ns":0,"exists":"","*":"\u0106"},{"ns":0,"exists":"","*":"\u0118"},{"ns":0,"exists":"","*":"\u015a"},{"ns":0,"exists":"","*":"\u0179"},{"ns":0,"exists":"","*":"\u017b"},{"ns":0,"*":"Stanis\u0142aw Zaborowski"},{"ns":11,"exists":"","*":"Template talk:Polish language"},{"ns":12,"exists":"","*":"Help:IPA/Polish"}],"templates":[{"ns":10,"exists":"","*":"Template:Cite book"},{"ns":10,"exists":"","*":"Template:Polish language"},{"ns":10,"exists":"","*":"Template:Navbox"},{"ns":828,"exists":"","*":"Module:Citation/CS1"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Configuration"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Whitelist"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Utilities"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Date validation"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Identifiers"},{"ns":828,"exists":"","*":"Module:Citation/CS1/COinS"},{"ns":828,"exists":"","*":"Module:Navbox"},{"ns":828,"exists":"","*":"Module:Navbar"},{"ns":828,"exists":"","*":"Module:Arguments"}],"images":["Ortographia_by_Stanislaw_Zaborowski-title_page.jpg"],"externallinks":["https://books.google.com/books?id=Z3ojAAAAMAAJ&printsec=frontcover&hl=pl&source=gbs_ge_summary_r&cad=0#v=onepage&q&f=false","http://www.dbc.wroc.pl/publication/4239","http://rcin.org.pl/publication/82568"],"sections":[{"toclevel":1,"level":"2","line":"Subsequent reforms","number":"1","index":"1","fromtitle":"History_of_Polish_orthography","byteoffset":2795,"anchor":"Subsequent_reforms"},{"toclevel":1,"level":"2","line":"See also","number":"2","index":"2","fromtitle":"History_of_Polish_orthography","byteoffset":3262,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"Literature","number":"3","index":"3","fromtitle":"History_of_Polish_orthography","byteoffset":3406,"anchor":"Literature"}],"parsewarnings":[],"displaytitle":"History of Polish orthography","iwlinks":[],"properties":[{"name":"defaultsort","*":"History Of Polish Orthography"},{"name":"wikibase_item","*":"Q5866028"}]}}