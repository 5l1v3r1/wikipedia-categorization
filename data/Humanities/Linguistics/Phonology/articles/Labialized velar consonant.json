,"*":"Template:Ambox"},{"ns":10,"exists":"","*":"Template:IPA"},{"ns":10,"exists":"","*":"Template:See also"},{"ns":828,"exists":"","*":"Module:Distinguish"},{"ns":828,"exists":"","*":"Module:Hatnote"},{"ns":828,"exists":"","*":"Module:Hatnote list"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:TableTools"},{"ns":828,"exists":"","*":"Module:Unsubst"},{"ns":828,"exists":"","*":"Module:Message box"},{"ns":828,"exists":"","*":"Module:No globals"},{"ns":828,"exists":"","*":"Module:Yesno"},{"ns":828,"exists":"","*":"Module:Message box/configuration"},{"ns":828,"exists":"","*":"Module:Category handler"},{"ns":828,"exists":"","*":"Module:Category handler/data"},{"ns":828,"exists":"","*":"Module:Category handler/config"},{"ns":828,"exists":"","*":"Module:Category handler/shared"},{"ns":828,"exists":"","*":"Module:Category handler/blacklist"},{"ns":828,"exists":"","*":"Module:Namespace detect/data"},{"ns":828,"exists":"","*":"Module:Namespace detect/config"},{"ns":828,"exists":"","*":"Module:Labelled list hatnote"}],"images":["Question_book-new.svg","Xsampa-X.png","Xsampa-w2.png"],"externallinks":[],"sections":[{"toclevel":1,"level":"2","line":"Labialized velar approximants","number":"1","index":"1","fromtitle":"Labialized_velar_consonant","byteoffset":471,"anchor":"Labialized_velar_approximants"},{"toclevel":1,"level":"2","line":"Historical development","number":"2","index":"2","fromtitle":"Labialized_velar_consonant","byteoffset":2471,"anchor":"Historical_development"},{"toclevel":1,"level":"2","line":"See also","number":"3","index":"3","fromtitle":"Labialized_velar_consonant","byteoffset":3949,"anchor":"See_also"}],"parsewarnings":[],"displaytitle":"Labialized velar consonant","iwlinks":[{"prefix":"wiktionary","url":"https://en.wiktionary.org/wiki/which","*":"wiktionary:which"},{"prefix":"wiktionary","url":"https://en.wiktionary.org/wiki/witch","*":"wiktionary:witch"},{"prefix":"wikt","url":"https://en.wiktionary.org/wiki/kwik","*":"wikt