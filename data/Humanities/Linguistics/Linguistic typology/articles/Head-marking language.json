ists":"","*":"Dependency grammar"},{"ns":0,"exists":"","*":"Dependent-marking language"},{"ns":0,"exists":"","*":"Dependent (grammar)"},{"ns":0,"exists":"","*":"Double-marking language"},{"ns":0,"exists":"","*":"Eastern Africa"},{"ns":0,"exists":"","*":"English language"},{"ns":0,"exists":"","*":"Equator"},{"ns":0,"exists":"","*":"Eurasia"},{"ns":0,"exists":"","*":"Finnish language"},{"ns":0,"exists":"","*":"Founder effect"},{"ns":0,"exists":"","*":"Grammar"},{"ns":0,"exists":"","*":"Head (linguistics)"},{"ns":0,"exists":"","*":"Himalayas"},{"ns":0,"exists":"","*":"Johanna Nichols"},{"ns":0,"exists":"","*":"Kusunda language"},{"ns":0,"exists":"","*":"Linguistic Diversity in Space and Time"},{"ns":0,"exists":"","*":"Linguistic typology"},{"ns":0,"exists":"","*":"Models of migration to the New World"},{"ns":0,"exists":"","*":"Modifier (grammar)"},{"ns":0,"exists":"","*":"Morphology (linguistics)"},{"ns":0,"exists":"","*":"New Guinea"},{"ns":0,"exists":"","*":"North America"},{"ns":0,"exists":"","*":"Northern Africa"},{"ns":0,"exists":"","*":"Noun"},{"ns":0,"exists":"","*":"Noun phrase"},{"ns":0,"exists":"","*":"Pacific Coast"},{"ns":0,"exists":"","*":"Pacific Rim"},{"ns":0,"exists":"","*":"Pama-Nyungan"},{"ns":0,"exists":"","*":"Phrase"},{"ns":0,"exists":"","*":"Phrase structure grammar"},{"ns":0,"exists":"","*":"Possessive"},{"ns":0,"exists":"","*":"Prepositional phrase"},{"ns":0,"exists":"","*":"Relative clause"},{"ns":0,"exists":"","*":"South America"},{"ns":0,"exists":"","*":"Southeast Asia"},{"ns":0,"exists":"","*":"Subject (grammar)"},{"ns":0,"exists":"","*":"Verb"},{"ns":0,"exists":"","*":"Verb argument"},{"ns":0,"exists":"","*":"Verb phrase"},{"ns":0,"exists":"","*":"Western Africa"},{"ns":0,"exists":"","*":"World Atlas of Language Structures"},{"ns":0,"exists":"","*":"Zero-marking language"}],"templates":[{"ns":10,"exists":"","*":"Template:Div col"},{"ns":10,"exists":"","*":"Template:Column-width"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":10,"exists":"","*":"Template:Div col end"},{"ns":10,"exists":"","*":"Template:Reflist"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"}],"images":["Head-marking-he-not-I.png"],"externallinks":["http://wals.info/feature/description/24","http://wals.info/feature/description/23","http://wals.info/feature/description/25"],"sections":[{"toclevel":1,"level":"2","line":"In English","number":"1","index":"1","fromtitle":"Head-marking_language","byteoffset":697,"anchor":"In_English"},{"toclevel":1,"level":"2","line":"Noun phrases and verb phrases","number":"2","index":"2","fromtitle":"Head-marking_language","byteoffset":2284,"anchor":"Noun_phrases_and_verb_phrases"},{"toclevel":1,"level":"2","line":"Geographical distribution","number":"3","index":"3","fromtitle":"Head-marking_language","byteoffset":3700,"anchor":"Geographical_distribution"},{"toclevel":1,"level":"2","line":"See also","number":"4","index":"4","fromtitle":"Head-marking_language","byteoffset":6557,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"Notes","number":"5","index":"5","fromtitle":"Head-marking_language","byteoffset":6858,"anchor":"Notes"},{"toclevel":1,"level":"2","line":"References","number":"6","index":"6","fromtitle":"Head-marking_language","byteoffset":6886,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Head-marking language","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q2683359"}]}}