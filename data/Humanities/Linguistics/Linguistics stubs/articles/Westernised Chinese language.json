ns":10,"exists":"","*":"Template:Category handler"},{"ns":10,"exists":"","*":"Template:DMC"},{"ns":10,"exists":"","*":"Template:Dated maintenance category"},{"ns":10,"exists":"","*":"Template:FULLROOTPAGENAME"},{"ns":10,"exists":"","*":"Template:Ns has subpages"},{"ns":10,"exists":"","*":"Template:DMCA"},{"ns":10,"exists":"","*":"Template:Zh"},{"ns":10,"exists":"","*":"Template:Lang-zh"},{"ns":10,"exists":"","*":"Template:Reflist"},{"ns":10,"exists":"","*":"Template:Column-width"},{"ns":10,"exists":"","*":"Template:Ling-stub"},{"ns":10,"exists":"","*":"Template:Asbox"},{"ns":828,"exists":"","*":"Module:Unsubst"},{"ns":828,"exists":"","*":"Module:Message box"},{"ns":828,"exists":"","*":"Module:No globals"},{"ns":828,"exists":"","*":"Module:Yesno"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:Message box/configuration"},{"ns":828,"exists":"","*":"Module:Category handler"},{"ns":828,"exists":"","*":"Module:Category handler/data"},{"ns":828,"exists":"","*":"Module:Category handler/config"},{"ns":828,"exists":"","*":"Module:Category handler/shared"},{"ns":828,"exists":"","*":"Module:Category handler/blacklist"},{"ns":828,"exists":"","*":"Module:Namespace detect/data"},{"ns":828,"exists":"","*":"Module:Namespace detect/config"},{"ns":828,"exists":"","*":"Module:Ns has subpages"},{"ns":828,"exists":"","*":"Module:Zh"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"},{"ns":828,"exists":"","*":"Module:Asbox"},{"ns":828,"exists":"","*":"Module:Buffer"},{"ns":828,"exists":"","*":"Module:Navbar"}],"images":["Edit-clear.svg","Linguistics_stub.svg"],"externallinks":["http://www.thinkingtaiwan.com/content/3525","http://opinion.cw.com.tw/blog/profile/351/article/3737","http://fb.ezp8.com/article/233531.asp","http://www.cw.com.tw/article/article.action?id=5063162","http://www.todaymacao.com/2016/06/26/%E3%80%8C%E8%AA%9E%E8%A8%80%E7%99%8C%E3%80%8D%E6%98%AF%E6%83%A1%E6%80%A7%E8%85%AB%E7%98%A4%E5%97%8E%EF%BC%9F/","https://buzzorange.com/2014/12/19/language-cancer/"],"sections":[{"toclevel":1,"level":"2","line":"Examples","number":"1","index":"1","fromtitle":"Westernised_Chinese_language","byteoffset":2038,"anchor":"Examples"},{"toclevel":1,"level":"2","line":"See also","number":"2","index":"2","fromtitle":"Westernised_Chinese_language","byteoffset":5618,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"3","index":"3","fromtitle":"Westernised_Chinese_language","byteoffset":5647,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Westernised Chinese language","iwlinks":[],"properties":[{"name":"defaultsort","*":"Westernised Chinese Language"},{"name":"wikibase_item","*":"Q7988572"}]}}