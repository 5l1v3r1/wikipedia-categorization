es"},{"toclevel":1,"level":"2","line":"External links","number":"3","index":"3","fromtitle":"Metaphor_and_metonymy","byteoffset":6245,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"Metaphor and metonymy","iwlinks":[{"prefix":"wikt","url":"https://en.wiktionary.org/wiki/contiguity","*":"wikt:contiguity"},{"prefix":"de","url":"https://de.wikipedia.org/wiki/Verdichtung","*":"de:Verdichtung"}],"properties":[{"name":"wikibase_item","*":"Q6823116"}]}}