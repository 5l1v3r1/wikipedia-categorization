el":"2","line":"Emotional impact","number":"1","index":"1","fromtitle":"Barrel_children","byteoffset":1106,"anchor":"Emotional_impact"},{"toclevel":1,"level":"2","line":"Reportage","number":"2","index":"2","fromtitle":"Barrel_children","byteoffset":4776,"anchor":"Reportage"},{"toclevel":1,"level":"2","line":"References","number":"3","index":"3","fromtitle":"Barrel_children","byteoffset":5812,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Barrel children","iwlinks":[],"properties":[{"name":"defaultsort","*":"Barrel Children"},{"name":"wikibase_item","*":"Q4863330"}]}}