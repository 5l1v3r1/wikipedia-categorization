kerr-posy-simmonds-and-more#img-5","http://www.scratchboardsociety.com/"],"sections":[{"toclevel":1,"level":"2","line":"History","number":"1","index":"1","fromtitle":"Scratchboard","byteoffset":859,"anchor":"History"},{"toclevel":1,"level":"2","line":"Technique","number":"2","index":"2","fromtitle":"Scratchboard","byteoffset":1835,"anchor":"Technique"},{"toclevel":1,"level":"2","line":"Materials and tools","number":"3","index":"3","fromtitle":"Scratchboard","byteoffset":3390,"anchor":"Materials_and_tools"},{"toclevel":1,"level":"2","line":"Artists","number":"4","index":"4","fromtitle":"Scratchboard","byteoffset":5222,"anchor":"Artists"},{"toclevel":1,"level":"2","line":"References","number":"5","index":"5","fromtitle":"Scratchboard","byteoffset":6077,"anchor":"References"},{"toclevel":1,"level":"2","line":"Further reading","number":"6","index":"6","fromtitle":"Scratchboard","byteoffset":6105,"anchor":"Further_reading"}],"parsewarnings":[],"displaytitle":"Scratchboard","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q2508219"}]}}