.txt","https://beta.worldcat.org/archivegrid/collection/data/68695768","https://www.wikidata.org/wiki/Q26997966","https://www.gutenberg.org/ebooks/search/?query=George+Charles+Williamson&go=Go","https://archive.org/search.php?query=creator:(George%20Charles%20Williamson)","http://isni.org/isni/0000000110277642"],"sections":[{"toclevel":1,"level":"2","line":"Biography","number":"1","index":"1","fromtitle":"G._C._Williamson","byteoffset":298,"anchor":"Biography"},{"toclevel":1,"level":"2","line":"Publications","number":"2","index":"2","fromtitle":"G._C._Williamson","byteoffset":2433,"anchor":"Publications"},{"toclevel":1,"level":"2","line":"References","number":"3","index":"3","fromtitle":"G._C._Williamson","byteoffset":5511,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"4","index":"4","fromtitle":"G._C._Williamson","byteoffset":6809,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"G. C. Williamson","iwlinks":[],"properties":[{"name":"defaultsort","*":"Williamson, George Charles"},{"name":"wikibase_item","*":"Q26997966"}]}}