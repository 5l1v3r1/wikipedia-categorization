/10.1002%2F14356007.b01_08","https://doi.org/10.1016%2Fj.corsci.2010.05.002","https://web.archive.org/web/20050218035909/http://www.octel-starreon.com/Products_RFA_Corrosion.htm","https://web.archive.org/web/20080724193257/http://www.corrosionist.com/corrosion_control_methods.htm","http://www.frenchcreeksoftware.com/wt93.htm","https://web.archive.org/web/20090703235638/http://electrochem.cwru.edu/encycl/art-i01-inhibitors.htm"],"sections":[{"toclevel":1,"level":"2","line":"Types","number":"1","index":"1","fromtitle":"Corrosion_inhibitor","byteoffset":1074,"anchor":"Types"},{"toclevel":1,"level":"2","line":"Illustrative applications","number":"2","index":"2","fromtitle":"Corrosion_inhibitor","byteoffset":2382,"anchor":"Illustrative_applications"},{"toclevel":2,"level":"3","line":"Fuels industry","number":"2.1","index":"3","fromtitle":"Corrosion_inhibitor","byteoffset":3584,"anchor":"Fuels_industry"},{"toclevel":1,"level":"2","line":"See also","number":"3","index":"4","fromtitle":"Corrosion_inhibitor","byteoffset":4768,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"4","index":"5","fromtitle":"Corrosion_inhibitor","byteoffset":4933,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"5","index":"6","fromtitle":"Corrosion_inhibitor","byteoffset":4963,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"Corrosion inhibitor","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q2468815"}]}}