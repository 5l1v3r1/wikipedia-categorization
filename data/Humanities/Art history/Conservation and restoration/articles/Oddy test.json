langname":"Croatian","autonym":"hrvatski","*":"Oddy proba"}],"categories":[{"sortkey":"","*":"Chemical_tests"},{"sortkey":"","*":"Conservation_and_restoration"},{"sortkey":"","hidden":"","*":"All_stub_articles"},{"sortkey":"","*":"Biochemistry_stubs"}],"links":[{"ns":10,"exists":"","*":"Template:Biochem-stub"},{"ns":0,"exists":"","*":"Acid"},{"ns":0,"exists":"","*":"Aldehyde"},{"ns":0,"exists":"","*":"Biochemistry"},{"ns":0,"exists":"","*":"British Museum"},{"ns":0,"exists":"","*":"Carbonyl sulfide"},{"ns":0,"exists":"","*":"Celsius"},{"ns":0,"exists":"","*":"Chemical test"},{"ns":0,"exists":"","*":"Chloride"},{"ns":0,"exists":"","*":"Copper"},{"ns":0,"exists":"","*":"Corrosion"},{"ns":0,"exists":"","*":"Corrosive substance"},{"ns":0,"exists":"","*":"Formaldehyde"},{"ns":0,"exists":"","*":"Humidity"},{"ns":0,"exists":"","*":"J. Paul Getty Museum"},{"ns":0,"exists":"","*":"Lead"},{"ns":0,"exists":"","*":"Materials testing"},{"ns":0,"exists":"","*":"Metropolitan Museum of Art"},{"ns":0,"exists":"","*":"Nelson-Atkins Museum of Art"},{"ns":0,"exists":"","*":"Outgassing"},{"ns":0,"exists":"","*":"Physical test"},{"ns":0,"exists":"","*":"Scientific control"},{"ns":0,"exists":"","*":"Silver"},{"ns":0,"exists":"","*":"William Andrew Oddy"},{"ns":0,"*":"Coupon (materials science)"},{"ns":4,"exists":"","*":"Wikipedia:Stub"},{"ns":11,"exists":"","*":"Template talk:Biochem-stub"}],"templates":[{"ns":10,"exists":"","*":"Template:Reflist"},{"ns":10,"exists":"","*":"Template:Cite web"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":10,"exists":"","*":"Template:Biochem-stub"},{"ns":10,"exists":"","*":"Template:Asbox"},{"ns":828,"exists":"","*":"Module:Citation/CS1"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Configuration"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Whitelist"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Utilities"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Date validation"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Identifiers"},{"ns":8