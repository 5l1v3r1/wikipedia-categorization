"}],"templates":[{"ns":10,"exists":"","*":"Template:Clear"},{"ns":10,"exists":"","*":"Template:ISBN"},{"ns":10,"exists":"","*":"Template:Catalog lookup link"},{"ns":10,"exists":"","*":"Template:Trim"},{"ns":10,"exists":"","*":"Template:Yesno-no"},{"ns":10,"exists":"","*":"Template:Yesno"},{"ns":10,"exists":"","*":"Template:Error-small"},{"ns":10,"exists":"","*":"Template:Tl"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":828,"exists":"","*":"Module:Check isxn"},{"ns":828,"exists":"","*":"Module:Error"}],"images":["Transasia_trade_routes_1stC_CE_gr2.png","ChineseJadePlaques.JPG","ZhouVase.JPG","WhiteHanBronzeMirror.JPG","BuddhaEvolution.JPG","Seokguram_Buddha.JPG","Heracles-Shukongoshin.JPG","WindGods.JPG","ScrollEvolution.JPG"],"externallinks":["http://libmma.contentdm.oclc.org/cdm/compoundobject/collection/p15324coll10/id/33392/rec/2"],"sections":[{"toclevel":1,"level":"2","line":"Scythian art","number":"1","index":"1","fromtitle":"Silk_Road_transmission_of_art","byteoffset":737,"anchor":"Scythian_art"},{"toclevel":1,"level":"2","line":"Hellenistic art","number":"2","index":"2","fromtitle":"Silk_Road_transmission_of_art","byteoffset":1761,"anchor":"Hellenistic_art"},{"toclevel":1,"level":"2","line":"Greco-Buddhist art","number":"3","index":"3","fromtitle":"Silk_Road_transmission_of_art","byteoffset":3342,"anchor":"Greco-Buddhist_art"},{"toclevel":2,"level":"3","line":"Buddha","number":"3.1","index":"4","fromtitle":"Silk_Road_transmission_of_art","byteoffset":3366,"anchor":"Buddha"},{"toclevel":1,"level":"2","line":"Eastern iconography in the West","number":"4","index":"5","fromtitle":"Silk_Road_transmission_of_art","byteoffset":4823,"anchor":"Eastern_iconography_in_the_West"},{"toclevel":1,"level":"2","line":"Case studies","number":"5","index":"6","fromtitle":"Silk_Road_transmission_of_art","byteoffset":5973,"anchor":"Case_studies"},{"toclevel":2,"level":"3","line":"Shukongoshin","number":"5.1","index":"7","fromtitle":"Silk_Road_transmission_of_art","byteof