ine":"American-style dealing","number":"2.2","index":"4","fromtitle":"Poker_dealer","byteoffset":2645,"anchor":"American-style_dealing"},{"toclevel":2,"level":"3","line":"European-style dealing","number":"2.3","index":"5","fromtitle":"Poker_dealer","byteoffset":3333,"anchor":"European-style_dealing"},{"toclevel":2,"level":"3","line":"Burning and turning","number":"2.4","index":"6","fromtitle":"Poker_dealer","byteoffset":3638,"anchor":"Burning_and_turning"},{"toclevel":1,"level":"2","line":"Responsibilities during a hand","number":"3","index":"7","fromtitle":"Poker_dealer","byteoffset":4392,"anchor":"Responsibilities_during_a_hand"},{"toclevel":1,"level":"2","line":"Online dealing","number":"4","index":"8","fromtitle":"Poker_dealer","byteoffset":6989,"anchor":"Online_dealing"},{"toclevel":1,"level":"2","line":"See also","number":"5","index":"9","fromtitle":"Poker_dealer","byteoffset":7163,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"Footnotes","number":"6","index":"10","fromtitle":"Poker_dealer","byteoffset":7191,"anchor":"Footnotes"},{"toclevel":1,"level":"2","line":"References","number":"7","index":"11","fromtitle":"Poker_dealer","byteoffset":7257,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Poker dealer","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q3036793"}]}}