s":828,"exists":"","*":"Module:Authority control"},{"ns":828,"exists":"","*":"Module:ResolveEntityId"}],"images":[],"externallinks":[],"sections":[{"toclevel":1,"level":"2","line":"Exoticism","number":"1","index":"1","fromtitle":"The_Memorable_Masque_of_the_Middle_Temple_and_Lincoln's_Inn","byteoffset":909,"anchor":"Exoticism"},{"toclevel":1,"level":"2","line":"The show","number":"2","index":"2","fromtitle":"The_Memorable_Masque_of_the_Middle_Temple_and_Lincoln's_Inn","byteoffset":1664,"anchor":"The_show"},{"toclevel":1,"level":"2","line":"Sponsorship","number":"3","index":"3","fromtitle":"The_Memorable_Masque_of_the_Middle_Temple_and_Lincoln's_Inn","byteoffset":2964,"anchor":"Sponsorship"},{"toclevel":1,"level":"2","line":"Publication","number":"4","index":"4","fromtitle":"The_Memorable_Masque_of_the_Middle_Temple_and_Lincoln's_Inn","byteoffset":3718,"anchor":"Publication"},{"toclevel":1,"level":"2","line":"Credits","number":"5","index":"5","fromtitle":"The_Memorable_Masque_of_the_Middle_Temple_and_Lincoln's_Inn","byteoffset":4427,"anchor":"Credits"},{"toclevel":1,"level":"2","line":"Notes","number":"6","index":"6","fromtitle":"The_Memorable_Masque_of_the_Middle_Temple_and_Lincoln's_Inn","byteoffset":5134,"anchor":"Notes"},{"toclevel":1,"level":"2","line":"Sources","number":"7","index":"7","fromtitle":"The_Memorable_Masque_of_the_Middle_Temple_and_Lincoln's_Inn","byteoffset":5157,"anchor":"Sources"}],"parsewarnings":[],"displaytitle":"<i>The Memorable Masque of the Middle Temple and Lincoln's Inn</i>","iwlinks":[],"properties":[{"name":"displaytitle","*":"<i>The Memorable Masque of the Middle Temple and Lincoln's Inn</i>"},{"name":"defaultsort","*":"Memorable Masque of the Middle Temple and Lincoln's Inn, The"},{"name":"wikibase_item","*":"Q7750957"}]}}