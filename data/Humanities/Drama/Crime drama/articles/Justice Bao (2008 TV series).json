ates":[{"ns":10,"exists":"","*":"Template:For"},{"ns":10,"exists":"","*":"Template:Infobox television"},{"ns":10,"exists":"","*":"Template:Infobox"},{"ns":10,"exists":"","*":"Template:Italic title"},{"ns":10,"exists":"","*":"Template:Template other"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":10,"exists":"","*":"Template:Reflist"},{"ns":828,"exists":"","*":"Module:For"},{"ns":828,"exists":"","*":"Module:Hatnote list"},{"ns":828,"exists":"","*":"Module:Hatnote"},{"ns":828,"exists":"","*":"Module:Yesno"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:Infobox"},{"ns":828,"exists":"","*":"Module:Navbar"},{"ns":828,"exists":"","*":"Module:InfoboxImage"},{"ns":828,"exists":"","*":"Module:Infobox/dates"},{"ns":828,"exists":"","*":"Module:Italic title"},{"ns":828,"exists":"","*":"Module:No globals"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"}],"images":["Justice_Bao_2008.jpg"],"externallinks":["http://www.china.org.cn/culture/2009-07/23/content_18188089.htm"],"sections":[{"toclevel":1,"level":"2","line":"Units","number":"1","index":"1","fromtitle":"Justice_Bao_(2008_TV_series)","byteoffset":1793,"anchor":"Units"},{"toclevel":1,"level":"2","line":"Cast","number":"2","index":"2","fromtitle":"Justice_Bao_(2008_TV_series)","byteoffset":2408,"anchor":"Cast"},{"toclevel":1,"level":"2","line":"References","number":"3","index":"3","fromtitle":"Justice_Bao_(2008_TV_series)","byteoffset":4816,"anchor":"References"}],"parsewarnings":[],"displaytitle":"<i>Justice Bao</i> (2008 TV series)","iwlinks":[],"properties":[{"name":"displaytitle","*":"<i>Justice Bao</i> (2008 TV series)"},{"name":"wikibase_item","*":"Q486468"}]}}