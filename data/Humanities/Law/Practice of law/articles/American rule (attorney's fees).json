v. Kent State University and Alexander J. Seed"},{"ns":0,"exists":"","*":"Statute"},{"ns":0,"exists":"","*":"Tulsa World"},{"ns":0,"exists":"","*":"United States Reports"}],"templates":[{"ns":10,"exists":"","*":"Template:Other uses"},{"ns":10,"exists":"","*":"Template:Reflist"},{"ns":10,"exists":"","*":"Template:Ussc"},{"ns":10,"exists":"","*":"Template:Cite web"},{"ns":10,"exists":"","*":"Template:Cite book"},{"ns":10,"exists":"","*":"Template:Cite journal"},{"ns":10,"exists":"","*":"Template:Cite news"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":828,"exists":"","*":"Module:Other uses"},{"ns":828,"exists":"","*":"Module:Hatnote"},{"ns":828,"exists":"","*":"Module:Hatnote list"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:TableTools"},{"ns":828,"exists":"","*":"Module:Citation/CS1"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Configuration"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Whitelist"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Utilities"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Date validation"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Identifiers"},{"ns":828,"exists":"","*":"Module:Citation/CS1/COinS"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"}],"images":[],"externallinks":["https://supreme.justia.com/cases/federal/us/421/240/","https://www.law.cornell.edu/rules/frcp/rule_54","https://books.google.com/books?id=V9oW90RSR7wC","http://www.jtexconsumerlaw.com/V11N3/JCCL_Magnuson.pdf","https://leginfo.legislature.ca.gov/faces/codes_displayText.xhtml?division=3.&chapter=4.&part=4.&lawCode=CIV&title=1.5.","http://online.ceb.com/CalCases/C3/37C3d813.htm","http://www.tulsaworld.com/homepagelatest/unsuccessful-civil-cases-could-cost-thousands-more-as-new-law/article_685a39a2-2cdc-5adc-9964-8b6130b965ea.html"],"sections":[{"toclevel":1,"level":"2","line":"Exceptions","number":"1","index":"1","fromtitle":"American_rule_(attorney's_fees)","byteoffset":945,"anchor":"Exceptions