ary"},{"ns":0,"exists":"","*":"Encyclopedia"},{"ns":0,"exists":"","*":"Information"},{"ns":0,"exists":"","*":"Language"},{"ns":0,"exists":"","*":"Law"},{"ns":0,"exists":"","*":"Legal terminology textbook"},{"ns":0,"exists":"","*":"Lexicographic information costs"},{"ns":0,"exists":"","*":"Lexicography"},{"ns":0,"exists":"","*":"List of legal abbreviations"},{"ns":0,"exists":"","*":"Monolingual"},{"ns":0,"exists":"","*":"Smartphone"},{"ns":0,"exists":"","*":"Sub-field dictionary"},{"ns":0,"exists":"","*":"Wex"},{"ns":0,"*":"Single-field"},{"ns":4,"exists":"","*":"Wikipedia:Verifiability"},{"ns":12,"exists":"","*":"Help:Introduction to referencing with Wiki Markup/1"},{"ns":12,"exists":"","*":"Help:Maintenance template removal"}],"templates":[{"ns":10,"exists":"","*":"Template:Refimprove"},{"ns":10,"exists":"","*":"Template:More citations needed"},{"ns":10,"exists":"","*":"Template:Ambox"},{"ns":10,"exists":"","*":"Template:Reflist"},{"ns":10,"exists":"","*":"Template:Cite book"},{"ns":10,"exists":"","*":"Template:Cite news"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":10,"exists":"","*":"Template:Dmoz"},{"ns":10,"exists":"","*":"Template:Curlie"},{"ns":828,"exists":"","*":"Module:Unsubst"},{"ns":828,"exists":"","*":"Module:Message box"},{"ns":828,"exists":"","*":"Module:No globals"},{"ns":828,"exists":"","*":"Module:Yesno"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:Message box/configuration"},{"ns":828,"exists":"","*":"Module:Category handler"},{"ns":828,"exists":"","*":"Module:Category handler/data"},{"ns":828,"exists":"","*":"Module:Category handler/config"},{"ns":828,"exists":"","*":"Module:Category handler/shared"},{"ns":828,"exists":"","*":"Module:Category handler/blacklist"},{"ns":828,"exists":"","*":"Module:Namespace detect/data"},{"ns":828,"exists":"","*":"Module:Namespace detect/config"},{"ns":828,"exists":"","*":"Module:Citation/CS1"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Configuration"},{"ns":828,"ex