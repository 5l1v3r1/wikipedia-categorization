"6","fromtitle":"Hyperdescent","byteoffset":5555,"anchor":"Iceland"},{"toclevel":1,"level":"2","line":"See also","number":"2","index":"7","fromtitle":"Hyperdescent","byteoffset":5983,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"3","index":"8","fromtitle":"Hyperdescent","byteoffset":6124,"anchor":"References"},{"toclevel":1,"level":"2","line":"Further reading","number":"4","index":"9","fromtitle":"Hyperdescent","byteoffset":6153,"anchor":"Further_reading"}],"parsewarnings":[],"displaytitle":"Hyperdescent","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q5957927"}]}}