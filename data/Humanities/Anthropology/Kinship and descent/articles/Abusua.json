"fromtitle":"Abusua","byteoffset":2748,"anchor":"Asakyiri"},{"toclevel":2,"level":"3","line":"Asona","number":"1.5","index":"6","fromtitle":"Abusua","byteoffset":3112,"anchor":"Asona"},{"toclevel":2,"level":"3","line":"Bretuo","number":"1.6","index":"7","fromtitle":"Abusua","byteoffset":3758,"anchor":"Bretuo"},{"toclevel":2,"level":"3","line":"Ekuona","number":"1.7","index":"8","fromtitle":"Abusua","byteoffset":4310,"anchor":"Ekuona"},{"toclevel":2,"level":"3","line":"Oyoko","number":"1.8","index":"9","fromtitle":"Abusua","byteoffset":4712,"anchor":"Oyoko"},{"toclevel":1,"level":"2","line":"References","number":"2","index":"10","fromtitle":"Abusua","byteoffset":5069,"anchor":"References"},{"toclevel":1,"level":"2","line":"Books","number":"3","index":"11","fromtitle":"Abusua","byteoffset":5234,"anchor":"Books"},{"toclevel":1,"level":"2","line":"External links","number":"4","index":"12","fromtitle":"Abusua","byteoffset":5259,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"Abusua","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q4670683"}]}}