esno"},{"ns":10,"exists":"","*":"Template:Error-small"},{"ns":10,"exists":"","*":"Template:Tl"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":828,"exists":"","*":"Module:Labelled list hatnote"},{"ns":828,"exists":"","*":"Module:Hatnote"},{"ns":828,"exists":"","*":"Module:Hatnote list"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:Lang"},{"ns":828,"exists":"","*":"Module:No globals"},{"ns":828,"exists":"","*":"Module:Language/name/data"},{"ns":828,"exists":"","*":"Module:Language/data/iana languages"},{"ns":828,"exists":"","*":"Module:Language/data/ISO 639-3"},{"ns":828,"exists":"","*":"Module:Language/data/wp languages"},{"ns":828,"exists":"","*":"Module:Language/data/iana scripts"},{"ns":828,"exists":"","*":"Module:Language/data/iana regions"},{"ns":828,"exists":"","*":"Module:Language/data/iana variants"},{"ns":828,"exists":"","*":"Module:Language/data/iana suppressed scripts"},{"ns":828,"exists":"","*":"Module:Lang/ISO 639 synonyms"},{"ns":828,"exists":"","*":"Module:Lang/data"},{"ns":828,"exists":"","*":"Module:Unicode data"},{"ns":828,"exists":"","*":"Module:Check isxn"},{"ns":828,"exists":"","*":"Module:Error"}],"images":["Prussian_Homage.jpg","Flag_of_Ducal_Prussia.svg","Prussian_Homage.PNG"],"externallinks":[],"sections":[{"toclevel":1,"level":"2","line":"Homages of Grand Masters of the Teutonic Knights","number":"1","index":"1","fromtitle":"Prussian_Homage","byteoffset":2051,"anchor":"Homages_of_Grand_Masters_of_the_Teutonic_Knights"},{"toclevel":1,"level":"2","line":"Homages of Dukes of Prussia","number":"2","index":"2","fromtitle":"Prussian_Homage","byteoffset":3412,"anchor":"Homages_of_Dukes_of_Prussia"},{"toclevel":1,"level":"2","line":"References","number":"3","index":"3","fromtitle":"Prussian_Homage","byteoffset":4728,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Prussian Homage","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q7253161"}]}}