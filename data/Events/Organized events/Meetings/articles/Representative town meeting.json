ists":"","*":"Template:Further information"},{"ns":10,"exists":"","*":"Template:Further"},{"ns":10,"exists":"","*":"Template:Refimprove"},{"ns":10,"exists":"","*":"Template:More citations needed"},{"ns":10,"exists":"","*":"Template:Ambox"},{"ns":10,"exists":"","*":"Template:Cn"},{"ns":10,"exists":"","*":"Template:Citation needed"},{"ns":10,"exists":"","*":"Template:Fix"},{"ns":10,"exists":"","*":"Template:Category handler"},{"ns":10,"exists":"","*":"Template:Fix/category"},{"ns":10,"exists":"","*":"Template:Delink"},{"ns":10,"exists":"","*":"Template:Reflist"},{"ns":10,"exists":"","*":"Template:Column-width"},{"ns":10,"exists":"","*":"Template:Cite web"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":828,"exists":"","*":"Module:Labelled list hatnote"},{"ns":828,"exists":"","*":"Module:Hatnote"},{"ns":828,"exists":"","*":"Module:Hatnote list"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:Unsubst"},{"ns":828,"exists":"","*":"Module:Message box"},{"ns":828,"exists":"","*":"Module:No globals"},{"ns":828,"exists":"","*":"Module:Yesno"},{"ns":828,"exists":"","*":"Module:Message box/configuration"},{"ns":828,"exists":"","*":"Module:Category handler"},{"ns":828,"exists":"","*":"Module:Category handler/data"},{"ns":828,"exists":"","*":"Module:Category handler/config"},{"ns":828,"exists":"","*":"Module:Category handler/shared"},{"ns":828,"exists":"","*":"Module:Category handler/blacklist"},{"ns":828,"exists":"","*":"Module:Namespace detect/data"},{"ns":828,"exists":"","*":"Module:Namespace detect/config"},{"ns":828,"exists":"","*":"Module:Delink"},{"ns":828,"exists":"","*":"Module:Citation/CS1"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Configuration"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Whitelist"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Utilities"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Date validation"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Identifiers"},{"ns":828,"exists":"","*":"Module:Citation/CS1/COinS"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"}],"images":["Question_book-new.svg"],"externallinks":["http://www.kjonline.com/news/sanford-becomes-maines-newest-city-two-months-after-vote-by-residents_2013-01-02.html","http://www.sec.state.ma.us/cis/cistwn/twnidx.htm","http://library.municode.com/index.aspx?clientID=14484&stateID=7&statename=Connecticut","http://wethersfieldct.com/government/code-regulations/minority-representation","http://www.town.groton.ct.us/town_gov/EntireCharterRevised.pdf","http://www.gencourt.state.nh.us/rsa/html/III/49-D/49-D-3.htm","http://www.gencourt.state.nh.us/rsa/html/LXIII/669/669-1.htm","https://www.sec.state.ma.us/cis/cistwn/twnidx.htm"],"sections":[{"toclevel":1,"level":"2","line":"Connecticut","number":"1","index":"1","fromtitle":"Representative_town_meeting","byteoffset":698,"anchor":"Connecticut"},{"toclevel":1,"level":"2","line":"Maine","number":"2","index":"2","fromtitle":"Representative_town_meeting","byteoffset":1993,"anchor":"Maine"},{"toclevel":1,"level":"2","line":"Massachusetts","number":"3","index":"3","fromtitle":"Representative_town_meeting","byteoffset":2580,"anchor":"Massachusetts"},{"toclevel":1,"level":"2","line":"New Hampshire","number":"4","index":"4","fromtitle":"Representative_town_meeting","byteoffset":3714,"anchor":"New_Hampshire"},{"toclevel":1,"level":"2","line":"Compare to","number":"5","index":"5","fromtitle":"Representative_town_meeting","byteoffset":5654,"anchor":"Compare_to"},{"toclevel":1,"level":"2","line":"References","number":"6","index":"6","fromtitle":"Representative_town_meeting","byteoffset":5908,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Representative town meeting","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q7314261"}]}}