},{"ns":0,"exists":"","*":"The Times"},{"ns":0,"exists":"","*":"Violin"},{"ns":0,"exists":"","*":"Western concert flute"}],"templates":[{"ns":10,"exists":"","*":"Template:Quote"},{"ns":10,"exists":"","*":"Template:Quote/styles.css"},{"ns":10,"exists":"","*":"Template:Reflist"},{"ns":10,"exists":"","*":"Template:Column-width"},{"ns":10,"exists":"","*":"Template:Cite web"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":10,"exists":"","*":"Template:Italic title"},{"ns":828,"exists":"","*":"Module:Citation/CS1"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Configuration"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Whitelist"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Utilities"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Date validation"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Identifiers"},{"ns":828,"exists":"","*":"Module:Citation/CS1/COinS"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"},{"ns":828,"exists":"","*":"Module:Italic title"},{"ns":828,"exists":"","*":"Module:No globals"},{"ns":828,"exists":"","*":"Module:Yesno"},{"ns":828,"exists":"","*":"Module:Arguments"}],"images":[],"externallinks":["http://www.classical-music.com/news/edward-gardner-eric-whitacre-and-kings-singers-cheltenham","http://charlottebray.co.uk/works/opera/entanglement/","https://www.theguardian.com/music/2015/jul/07/entanglement-that-man-stephen-ward-review","https://web.archive.org/web/20150814171144/http://www.gloucestershireecho.co.uk/arts-diary-Entanglement-Man-Stephen-Ward-Parabola/story-26847658-detail/story.html","http://www.gloucestershireecho.co.uk/arts-diary-Entanglement-Man-Stephen-Ward-Parabola/story-26847658-detail/story.html","http://www.theartsdesk.com/opera/entanglement-man-stephen-ward-presteigne-festival","http://www.thetimes.co.uk/tto/arts/music/classical/article4490295.ece"],"sections":[{"toclevel":1,"level":"2","line":"Background","number":"1","index":"1","fromtitle":"Entanglement_(opera)","byteoffset":1792,"anchor":"Background"},{"toclevel":1,"level":"2","line":"Composition","number":"2","index":"2","fromtitle":"Entanglement_(opera)","byteoffset":2900,"anchor":"Composition"},{"toclevel":2,"level":"3","line":"Instrumentation","number":"2.1","index":"3","fromtitle":"Entanglement_(opera)","byteoffset":3016,"anchor":"Instrumentation"},{"toclevel":1,"level":"2","line":"Roles","number":"3","index":"4","fromtitle":"Entanglement_(opera)","byteoffset":3263,"anchor":"Roles"},{"toclevel":1,"level":"2","line":"Reception","number":"4","index":"5","fromtitle":"Entanglement_(opera)","byteoffset":3546,"anchor":"Reception"},{"toclevel":1,"level":"2","line":"References","number":"5","index":"6","fromtitle":"Entanglement_(opera)","byteoffset":6502,"anchor":"References"}],"parsewarnings":[],"displaytitle":"<i>Entanglement</i> (opera)","iwlinks":[],"properties":[{"name":"displaytitle","*":"<i>Entanglement</i> (opera)"},{"name":"wikibase_item","*":"Q21998487"}]}}