Norway"},{"ns":0,"exists":"","*":"Novgorod"},{"ns":0,"exists":"","*":"Opera"},{"ns":0,"exists":"","*":"Pechenegs"},{"ns":0,"exists":"","*":"Scribe"},{"ns":0,"exists":"","*":"Sergei Prokofiev"},{"ns":0,"exists":"","*":"Sicily"},{"ns":0,"exists":"","*":"Soprano"},{"ns":0,"exists":"","*":"Sweden"},{"ns":0,"exists":"","*":"Tenor"},{"ns":0,"exists":"","*":"Ukrainians"},{"ns":0,"exists":"","*":"Varangians"},{"ns":0,"exists":"","*":"Viking"},{"ns":0,"exists":"","*":"Vikings"},{"ns":0,"exists":"","*":"Yaroslav I the Wise"},{"ns":0,"exists":"","*":"Yaroslav the Wise"},{"ns":0,"*":"Stefan Turchak"}],"templates":[{"ns":10,"exists":"","*":"Template:See also"},{"ns":10,"exists":"","*":"Template:Italic title"},{"ns":10,"exists":"","*":"Template:TOC limit"},{"ns":828,"exists":"","*":"Module:Labelled list hatnote"},{"ns":828,"exists":"","*":"Module:Hatnote"},{"ns":828,"exists":"","*":"Module:Hatnote list"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:Italic title"},{"ns":828,"exists":"","*":"Module:No globals"},{"ns":828,"exists":"","*":"Module:Yesno"}],"images":["Yaroslav_Mudriy.jpg"],"externallinks":[],"sections":[{"toclevel":1,"level":"2","line":"Roles","number":"1","index":"1","fromtitle":"Yaroslav_Mudriy","byteoffset":937,"anchor":"Roles"},{"toclevel":1,"level":"2","line":"Synopsis","number":"2","index":"2","fromtitle":"Yaroslav_Mudriy","byteoffset":1728,"anchor":"Synopsis"},{"toclevel":2,"level":"3","line":"Act I","number":"2.1","index":"3","fromtitle":"Yaroslav_Mudriy","byteoffset":2317,"anchor":"Act_I"},{"toclevel":3,"level":"4","line":"Scene 1","number":"2.1.1","index":"4","fromtitle":"Yaroslav_Mudriy","byteoffset":2329,"anchor":"Scene_1"},{"toclevel":3,"level":"4","line":"Scene 2","number":"2.1.2","index":"5","fromtitle":"Yaroslav_Mudriy","byteoffset":2714,"anchor":"Scene_2"},{"toclevel":2,"level":"3","line":"Act II","number":"2.2","index":"6","fromtitle":"Yaroslav_Mudriy","byteoffset":3109,"anchor":"Act_II"},{"toclevel":3,"level":"4","line":"Scene 3","number":"2.2.1","index":"7","fromtitle":"Yaroslav_Mudriy","byteoffset":3122,"anchor":"Scene_3"},{"toclevel":3,"level":"4","line":"Scene 4","number":"2.2.2","index":"8","fromtitle":"Yaroslav_Mudriy","byteoffset":3427,"anchor":"Scene_4"},{"toclevel":3,"level":"4","line":"Scene 5","number":"2.2.3","index":"9","fromtitle":"Yaroslav_Mudriy","byteoffset":3559,"anchor":"Scene_5"},{"toclevel":2,"level":"3","line":"Act III","number":"2.3","index":"10","fromtitle":"Yaroslav_Mudriy","byteoffset":4147,"anchor":"Act_III"},{"toclevel":3,"level":"4","line":"Scene 6","number":"2.3.1","index":"11","fromtitle":"Yaroslav_Mudriy","byteoffset":4161,"anchor":"Scene_6"},{"toclevel":3,"level":"4","line":"Scene 7","number":"2.3.2","index":"12","fromtitle":"Yaroslav_Mudriy","byteoffset":4772,"anchor":"Scene_7"},{"toclevel":3,"level":"4","line":"Scene 8","number":"2.3.3","index":"13","fromtitle":"Yaroslav_Mudriy","byteoffset":4885,"anchor":"Scene_8"},{"toclevel":1,"level":"2","line":"References","number":"3","index":"14","fromtitle":"Yaroslav_Mudriy","byteoffset":5009,"anchor":"References"}],"parsewarnings":[],"displaytitle":"<i>Yaroslav Mudriy</i>","iwlinks":[],"properties":[{"name":"displaytitle","*":"<i>Yaroslav Mudriy</i>"},{"name":"defaultsort","*":"Yaroslav Mudriy"},{"name":"wikibase_item","*":"Q8012913"}]}}