t","number":"1","index":"1","fromtitle":"Hoodwink_(1981_film)","byteoffset":1345,"anchor":"Plot"},{"toclevel":1,"level":"2","line":"Cast","number":"2","index":"2","fromtitle":"Hoodwink_(1981_film)","byteoffset":1929,"anchor":"Cast"},{"toclevel":1,"level":"2","line":"Production","number":"3","index":"3","fromtitle":"Hoodwink_(1981_film)","byteoffset":2404,"anchor":"Production"},{"toclevel":1,"level":"2","line":"Release","number":"4","index":"4","fromtitle":"Hoodwink_(1981_film)","byteoffset":3402,"anchor":"Release"},{"toclevel":2,"level":"3","line":"Awards and nominations","number":"4.1","index":"5","fromtitle":"Hoodwink_(1981_film)","byteoffset":3638,"anchor":"Awards_and_nominations"},{"toclevel":1,"level":"2","line":"References","number":"5","index":"6","fromtitle":"Hoodwink_(1981_film)","byteoffset":4462,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"6","index":"7","fromtitle":"Hoodwink_(1981_film)","byteoffset":4495,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"<i>Hoodwink</i> (1981 film)","iwlinks":[],"properties":[{"name":"displaytitle","*":"<i>Hoodwink</i> (1981 film)"},{"name":"wikibase_item","*":"Q5897662"}]}}