l-center.html"],"sections":[{"toclevel":1,"level":"2","line":"Production history","number":"1","index":"1","fromtitle":"The_Death_of_Bessie_Smith","byteoffset":530,"anchor":"Production_history"},{"toclevel":1,"level":"2","line":"Plot overview","number":"2","index":"2","fromtitle":"The_Death_of_Bessie_Smith","byteoffset":2914,"anchor":"Plot_overview"},{"toclevel":1,"level":"2","line":"Characters","number":"3","index":"3","fromtitle":"The_Death_of_Bessie_Smith","byteoffset":4384,"anchor":"Characters"},{"toclevel":1,"level":"2","line":"References","number":"4","index":"4","fromtitle":"The_Death_of_Bessie_Smith","byteoffset":4935,"anchor":"References"}],"parsewarnings":[],"displaytitle":"<i>The Death of Bessie Smith</i>","iwlinks":[],"properties":[{"name":"displaytitle","*":"<i>The Death of Bessie Smith</i>"},{"name":"defaultsort","*":"Death of Bessie Smith, The"},{"name":"wikibase_item","*":"Q7729416"}]}}