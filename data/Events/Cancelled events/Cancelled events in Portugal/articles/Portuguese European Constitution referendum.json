ferendum"},{"ns":0,"exists":"","*":"Spanish European Constitution referendum, 2005"},{"ns":0,"exists":"","*":"Treaty establishing a Constitution for Europe"},{"ns":0,"exists":"","*":"Treaty of Lisbon"},{"ns":0,"exists":"","*":"United Kingdom European Constitution referendum"},{"ns":11,"exists":"","*":"Template talk:EU Constitution"}],"templates":[{"ns":10,"exists":"","*":"Template:EU Constitution"},{"ns":10,"exists":"","*":"Template:Sidebar"},{"ns":828,"exists":"","*":"Module:Sidebar"},{"ns":828,"exists":"","*":"Module:No globals"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:Navbar"}],"images":[],"externallinks":["http://edition.cnn.com/2005/WORLD/europe/03/12/portugal.eu.vote.reut/","https://web.archive.org/web/20050224124400/http://www.eubusiness.com/afp/041217203719.0lxc1vrp/","https://www.forbes.com/markets/feeds/afx/2005/06/17/afx2098196.html"],"sections":[{"toclevel":1,"level":"2","line":"Initial plans and delay","number":"1","index":"1","fromtitle":"Portuguese_European_Constitution_referendum","byteoffset":1562,"anchor":"Initial_plans_and_delay"},{"toclevel":1,"level":"2","line":"External links","number":"2","index":"2","fromtitle":"Portuguese_European_Constitution_referendum","byteoffset":2219,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"Portuguese European Constitution referendum","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q3454893"}]}}