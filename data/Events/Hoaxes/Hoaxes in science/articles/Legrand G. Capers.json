ber":"4","index":"4","fromtitle":"Legrand_G._Capers","byteoffset":6909,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"5","index":"5","fromtitle":"Legrand_G._Capers","byteoffset":6939,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"Legrand G. Capers","iwlinks":[{"prefix":"wikisource","url":"https://en.wikisource.org/wiki/Attention_Gynaecologists!_%E2%80%94_Notes_from_the_Diary_of_a_Field_and_Hospital_Surgeon,_C._S._A.","*":"wikisource:Attention Gynaecologists! \u2014 Notes from the Diary of a Field and Hospital Surgeon, C. S. A."}],"properties":[{"name":"defaultsort","*":"Capers, Legrand G."},{"name":"wikibase_item","*":"Q6518660"}]}}