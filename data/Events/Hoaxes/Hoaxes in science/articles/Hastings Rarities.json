e:Main other"},{"ns":828,"exists":"","*":"Module:Convert"},{"ns":828,"exists":"","*":"Module:Convert/data"},{"ns":828,"exists":"","*":"Module:Convert/text"},{"ns":828,"exists":"","*":"Module:Citation/CS1"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Configuration"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Whitelist"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Utilities"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Date validation"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Identifiers"},{"ns":828,"exists":"","*":"Module:Citation/CS1/COinS"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"}],"images":["Snowfinch.jpg","Acrocephalus_melanopogon_ad03_CB.jpg"],"externallinks":["http://www.bbrc.org.uk/805","//www.britishbirds.co.uk/wp-content/uploads/article_files/V55/V55_N08/V55_N08_P283_298_A044.pdf","http://www.hmag.org.uk/collections/rarities/"],"sections":[{"toclevel":1,"level":"2","line":"History","number":"1","index":"1","fromtitle":"Hastings_Rarities","byteoffset":1369,"anchor":"History"},{"toclevel":1,"level":"2","line":"Surviving specimens","number":"2","index":"2","fromtitle":"Hastings_Rarities","byteoffset":4294,"anchor":"Surviving_specimens"},{"toclevel":1,"level":"2","line":"Bird species dropped from the British List in 1962","number":"3","index":"3","fromtitle":"Hastings_Rarities","byteoffset":4611,"anchor":"Bird_species_dropped_from_the_British_List_in_1962"},{"toclevel":1,"level":"2","line":"References","number":"4","index":"4","fromtitle":"Hastings_Rarities","byteoffset":5599,"anchor":"References"},{"toclevel":1,"level":"2","line":"Sources","number":"5","index":"5","fromtitle":"Hastings_Rarities","byteoffset":5627,"anchor":"Sources"},{"toclevel":1,"level":"2","line":"External links","number":"6","index":"6","fromtitle":"Hastings_Rarities","byteoffset":6357,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"Hastings Rarities","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q5680432"}]}}