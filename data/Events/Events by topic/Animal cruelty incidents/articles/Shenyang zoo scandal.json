allinks":["http://www.scmp.com/portal/site/SCMP/menuitem.2af62ecb329d3d7733492d9253a0a0a0/?vgnextoid=7fd94df762d57210VgnVCM100000360a0a0aRCRD&ss=China&s=News","http://www.usqiaobao.com/2010-03/13/content_379512_2.htm","https://web.archive.org/web/20110717180435/http://www.usqiaobao.com/2010-03/13/content_379512_2.htm","http://china.globaltimes.cn/society/2010-03/512959.html","https://web.archive.org/web/20100323113504/http://china.globaltimes.cn/society/2010-03/512959.html","http://www.usqiaobao.com/2010-03/13/content_379512.htm","https://web.archive.org/web/20110717180739/http://www.usqiaobao.com/2010-03/13/content_379512.htm","http://www.chinadaily.com.cn/china/2010-03/19/content_9611254_2.htm","http://www.scmp.com/portal/site/SCMP/menuitem.2af62ecb329d3d7733492d9253a0a0a0/?vgnextoid=2fd996ea1c267210VgnVCM100000360a0a0aRCRD&ss=China&s=News","https://www.google.com/hostednews/ap/article/ALeqM5g1Y6c77AQ_cFOmsVdiFr_Uh4Qm4gD9EEBB6O0"],"sections":[{"toclevel":1,"level":"2","line":"History","number":"1","index":"1","fromtitle":"Shenyang_zoo_scandal","byteoffset":796,"anchor":"History"},{"toclevel":1,"level":"2","line":"2009","number":"2","index":"2","fromtitle":"Shenyang_zoo_scandal","byteoffset":1526,"anchor":"2009"},{"toclevel":1,"level":"2","line":"2010","number":"3","index":"3","fromtitle":"Shenyang_zoo_scandal","byteoffset":2277,"anchor":"2010"},{"toclevel":1,"level":"2","line":"Responses","number":"4","index":"4","fromtitle":"Shenyang_zoo_scandal","byteoffset":4349,"anchor":"Responses"},{"toclevel":1,"level":"2","line":"See also","number":"5","index":"5","fromtitle":"Shenyang_zoo_scandal","byteoffset":5172,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"6","index":"6","fromtitle":"Shenyang_zoo_scandal","byteoffset":5248,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Shenyang zoo scandal","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q7494357"}]}}