llinks":["http://gallica.bnf.fr/ark:/12148/bpt6k103387b/f82.image","http://mystere-paranormal.over-blog.com/2017/08/les-lavandieres-de-la-nuit.html","http://www.biblisem.net/narratio/souvlava.htm"],"sections":[{"toclevel":1,"level":"2","line":"France","number":"1","index":"1","fromtitle":"Les_Lavandi\u00e8res","byteoffset":1020,"anchor":"France"},{"toclevel":2,"level":"3","line":"Brittany","number":"1.1","index":"2","fromtitle":"Les_Lavandi\u00e8res","byteoffset":1474,"anchor":"Brittany"},{"toclevel":1,"level":"2","line":"Ireland","number":"2","index":"3","fromtitle":"Les_Lavandi\u00e8res","byteoffset":4493,"anchor":"Ireland"},{"toclevel":1,"level":"2","line":"Scotland","number":"3","index":"4","fromtitle":"Les_Lavandi\u00e8res","byteoffset":4688,"anchor":"Scotland"},{"toclevel":1,"level":"2","line":"Wales and Cornwall","number":"4","index":"5","fromtitle":"Les_Lavandi\u00e8res","byteoffset":4996,"anchor":"Wales_and_Cornwall"},{"toclevel":1,"level":"2","line":"England","number":"5","index":"6","fromtitle":"Les_Lavandi\u00e8res","byteoffset":5486,"anchor":"England"},{"toclevel":1,"level":"2","line":"See also","number":"6","index":"7","fromtitle":"Les_Lavandi\u00e8res","byteoffset":5664,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"7","index":"8","fromtitle":"Les_Lavandi\u00e8res","byteoffset":5797,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Les Lavandi\u00e8res","iwlinks":[],"properties":[{"name":"defaultsort","*":"Les Lavandieres"},{"name":"wikibase_item","*":"Q3173830"}]}}