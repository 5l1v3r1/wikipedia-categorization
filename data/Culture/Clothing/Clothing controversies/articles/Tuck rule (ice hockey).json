1,"level":"2","line":"Reception","number":"3","index":"3","fromtitle":"Tuck_rule_(ice_hockey)","byteoffset":5477,"anchor":"Reception"},{"toclevel":1,"level":"2","line":"References","number":"4","index":"4","fromtitle":"Tuck_rule_(ice_hockey)","byteoffset":6548,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Tuck rule (ice hockey)","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q25221918"}]}}