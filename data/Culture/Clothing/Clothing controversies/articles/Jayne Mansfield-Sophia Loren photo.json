romtitle":"Jayne_Mansfield-Sophia_Loren_photo","byteoffset":3594,"anchor":"Sources"},{"toclevel":2,"level":"3","line":"Citation","number":"4.2","index":"6","fromtitle":"Jayne_Mansfield-Sophia_Loren_photo","byteoffset":3958,"anchor":"Citation"}],"parsewarnings":[],"displaytitle":"Jayne Mansfield-Sophia Loren photo","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q18208281"}]}}