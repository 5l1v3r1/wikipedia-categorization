ce category"},{"ns":10,"exists":"","*":"Template:FULLROOTPAGENAME"},{"ns":10,"exists":"","*":"Template:Ns has subpages"},{"ns":10,"exists":"","*":"Template:Use dmy dates"},{"ns":10,"exists":"","*":"Template:DNB Cite"},{"ns":10,"exists":"","*":"Template:Cite DNB"},{"ns":10,"exists":"","*":"Template:Cite encyclopedia"},{"ns":10,"exists":"","*":"Template:Reflist"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":10,"exists":"","*":"Template:Authority control"},{"ns":10,"exists":"","*":"Template:EditAtWikidata"},{"ns":828,"exists":"","*":"Module:Unsubst"},{"ns":828,"exists":"","*":"Module:Ns has subpages"},{"ns":828,"exists":"","*":"Module:Citation/CS1"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Configuration"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Whitelist"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Utilities"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Date validation"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Identifiers"},{"ns":828,"exists":"","*":"Module:Citation/CS1/COinS"},{"ns":828,"exists":"","*":"Module:String"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"},{"ns":828,"exists":"","*":"Module:Authority control"},{"ns":828,"exists":"","*":"Module:No globals"},{"ns":828,"exists":"","*":"Module:ResolveEntityId"},{"ns":828,"exists":"","*":"Module:Navbox"},{"ns":828,"exists":"","*":"Module:Navbar"},{"ns":828,"exists":"","*":"Module:EditAtWikidata"}],"images":["Wikisource-logo.svg","Blue_pencil.svg"],"externallinks":["http://www.hrionline.ac.uk/foxe/single/book7/7_1583_0805.html","https://www.wikidata.org/wiki/Q7326605","http://dlib.lib.ohio-state.edu/foxe/Images/1563/P00016.jpeg","https://www.worldcat.org/identities/containsVIAFID/12176386","http://isni.org/isni/0000000048051211","http://id.loc.gov/authorities/names/nr93005741","https://viaf.org/viaf/12176386"],"sections":[{"toclevel":1,"level":"2","line":"Life","number":"1","index":"1","fromtitle":"Richard_Hunne","byteoffset":774,"anchor":"Life"},{"toclevel":1,"l