ed lard"},{"ns":0,"exists":"","*":"Mur River"},{"ns":0,"exists":"","*":"Mura Statistical Region"},{"ns":0,"exists":"","*":"Peter Dajnko"},{"ns":0,"exists":"","*":"Prekmurje"},{"ns":0,"exists":"","*":"Prekmurje Slovene"},{"ns":0,"exists":"","*":"Prlekija dialect"},{"ns":0,"exists":"","*":"Radenci"},{"ns":0,"exists":"","*":"Razkri\u017eje"},{"ns":0,"exists":"","*":"Slavko Osterc"},{"ns":0,"exists":"","*":"Slovene Hills"},{"ns":0,"exists":"","*":"Slovene language"},{"ns":0,"exists":"","*":"Slovenia"},{"ns":0,"exists":"","*":"Stanko Vraz"},{"ns":0,"exists":"","*":"Sveti Jurij ob \u0160\u010davnici"},{"ns":0,"exists":"","*":"Vekoslav Grmi\u010d"},{"ns":0,"exists":"","*":"Ver\u017eej"},{"ns":0,"*":"Prle\u0161ka t\u00fcnka"},{"ns":0,"*":"Prle\u0161ka gibanica"},{"ns":0,"*":"Fran Ile\u0161i\u010d"},{"ns":0,"*":"Franc Ksaver Me\u0161ko"},{"ns":0,"*":"Vlado Miheljak"},{"ns":0,"*":"Vika Poto\u010dnik"},{"ns":0,"*":"Anton Trstenjak (theologian)"},{"ns":0,"*":"France Veber"},{"ns":4,"exists":"","*":"Wikipedia:Stub"},{"ns":11,"exists":"","*":"Template talk:Slovenia-geo-stub"}],"templates":[{"ns":10,"exists":"","*":"Template:Lang-sl"},{"ns":10,"exists":"","*":"Template:Commons category-inline"},{"ns":10,"exists":"","*":"Template:Sister-inline"},{"ns":10,"exists":"","*":"Template:Sl icon"},{"ns":10,"exists":"","*":"Template:Link language"},{"ns":10,"exists":"","*":"Template:ISO 639 name sl"},{"ns":10,"exists":"","*":"Template:Category handler"},{"ns":10,"exists":"","*":"Template:Coord"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":10,"exists":"","*":"Template:Slovenia-geo-stub"},{"ns":10,"exists":"","*":"Template:Asbox"},{"ns":828,"exists":"","*":"Module:Lang"},{"ns":828,"exists":"","*":"Module:No globals"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:Language/name/data"},{"ns":828,"exists":"","*":"Module:Language/data/iana languages"},{"ns":828,"exists":"","*":"Module:Language/data/ISO 639-3"},{"ns":828,"exists":"","*":"Module:Language/d