lso","number":"5","index":"5","fromtitle":"Hip_flask","byteoffset":3751,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"6","index":"6","fromtitle":"Hip_flask","byteoffset":3881,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"7","index":"7","fromtitle":"Hip_flask","byteoffset":3911,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"Hip flask","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q23680"}]}}