r":"3","index":"3","fromtitle":"Mixed-blood","byteoffset":6604,"anchor":"Sources"},{"toclevel":1,"level":"2","line":"External links","number":"4","index":"4","fromtitle":"Mixed-blood","byteoffset":7531,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"Mixed-blood","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q3738643"}]}}