"Matt Groening"},{"ns":0,"exists":"","*":"Nancy Drew"},{"ns":0,"exists":"","*":"Newhart"},{"ns":0,"exists":"","*":"Plot (narrative)"},{"ns":0,"exists":"","*":"Plot device"},{"ns":0,"exists":"","*":"Reboot (fiction)"},{"ns":0,"exists":"","*":"Retroactive continuity"},{"ns":0,"exists":"","*":"Science fiction"},{"ns":0,"exists":"","*":"Scooby-Doo (character)"},{"ns":0,"exists":"","*":"Screenwriter"},{"ns":0,"exists":"","*":"Season premiere"},{"ns":0,"exists":"","*":"Self-referential humor"},{"ns":0,"exists":"","*":"Soap opera"},{"ns":0,"exists":"","*":"South Park"},{"ns":0,"exists":"","*":"Spider-Man"},{"ns":0,"exists":"","*":"Spider-Man: One More Day"},{"ns":0,"exists":"","*":"Status quo"},{"ns":0,"exists":"","*":"Story arc"},{"ns":0,"exists":"","*":"Superman"},{"ns":0,"exists":"","*":"Suzanne Pleshette"},{"ns":0,"exists":"","*":"Television"},{"ns":0,"exists":"","*":"The Bob Newhart Show"},{"ns":0,"exists":"","*":"The Simpsons"},{"ns":0,"exists":"","*":"Time travel"},{"ns":4,"exists":"","*":"Wikipedia:Manual of Style/Words to watch"},{"ns":4,"exists":"","*":"Wikipedia:Verifiability"},{"ns":12,"exists":"","*":"Help:Introduction to referencing with Wiki Markup/1"},{"ns":12,"exists":"","*":"Help:Maintenance template removal"}],"templates":[{"ns":10,"exists":"","*":"Template:Refimprove"},{"ns":10,"exists":"","*":"Template:More citations needed"},{"ns":10,"exists":"","*":"Template:Ambox"},{"ns":10,"exists":"","*":"Template:Who"},{"ns":10,"exists":"","*":"Template:Fix"},{"ns":10,"exists":"","*":"Template:Category handler"},{"ns":10,"exists":"","*":"Template:Fix/category"},{"ns":10,"exists":"","*":"Template:Delink"},{"ns":10,"exists":"","*":"Template:Cite book"},{"ns":10,"exists":"","*":"Template:Cite news"},{"ns":828,"exists":"","*":"Module:Unsubst"},{"ns":828,"exists":"","*":"Module:Message box"},{"ns":828,"exists":"","*":"Module:No globals"},{"ns":828,"exists":"","*":"Module:Yesno"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:Message bo