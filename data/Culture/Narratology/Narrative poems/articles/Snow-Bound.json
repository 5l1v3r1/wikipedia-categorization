exists":"","*":"Template:Main other"},{"ns":10,"exists":"","*":"Template:Wikisource"},{"ns":10,"exists":"","*":"Template:Sister project"},{"ns":10,"exists":"","*":"Template:Side box"},{"ns":828,"exists":"","*":"Module:Check isxn"},{"ns":828,"exists":"","*":"Module:Error"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"},{"ns":828,"exists":"","*":"Module:Side box"},{"ns":828,"exists":"","*":"Module:Yesno"}],"images":["Snow-Bound_1866.jpg","John_Greenleaf_Whittier_1859.png","Whittier_House.jpg","Wikisource-logo.svg"],"externallinks":["http://www.poetryfoundation.org/poem/174758","http://myweb.northshore.edu/users/sherman/whittier/haverhill/snowbound.html"],"sections":[{"toclevel":1,"level":"2","line":"Overview","number":"1","index":"1","fromtitle":"Snow-Bound","byteoffset":223,"anchor":"Overview"},{"toclevel":1,"level":"2","line":"Composition and publication history","number":"2","index":"2","fromtitle":"Snow-Bound","byteoffset":696,"anchor":"Composition_and_publication_history"},{"toclevel":1,"level":"2","line":"Response","number":"3","index":"3","fromtitle":"Snow-Bound","byteoffset":2076,"anchor":"Response"},{"toclevel":1,"level":"2","line":"Analysis","number":"4","index":"4","fromtitle":"Snow-Bound","byteoffset":4070,"anchor":"Analysis"},{"toclevel":1,"level":"2","line":"References","number":"5","index":"5","fromtitle":"Snow-Bound","byteoffset":5485,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"6","index":"6","fromtitle":"Snow-Bound","byteoffset":5515,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"Snow-Bound","iwlinks":[{"prefix":"wikisource","url":"https://en.wikisource.org/wiki/Snow-Bound:_A_Winter_Idyl","*":"wikisource:Snow-Bound: A Winter Idyl"}],"properties":[{"name":"wikibase_item","*":"Q7548132"}]}}