abs.org/geohack/geohack.php?pagename=Enchanted_Mesa&params=34_55_11_N_107_33_05_W_"],"sections":[{"toclevel":1,"level":"2","line":"History","number":"1","index":"1","fromtitle":"Enchanted_Mesa","byteoffset":1160,"anchor":"History"},{"toclevel":1,"level":"2","line":"Geology","number":"2","index":"2","fromtitle":"Enchanted_Mesa","byteoffset":4975,"anchor":"Geology"},{"toclevel":1,"level":"2","line":"Notes","number":"3","index":"3","fromtitle":"Enchanted_Mesa","byteoffset":5585,"anchor":"Notes"}],"parsewarnings":[],"displaytitle":"Enchanted Mesa","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q5375378"}]}}