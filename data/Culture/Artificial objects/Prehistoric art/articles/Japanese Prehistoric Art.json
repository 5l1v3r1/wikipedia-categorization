r":"1","index":"1","fromtitle":"Japanese_Prehistoric_Art","byteoffset":1218,"anchor":"J\u014dmon_art"},{"toclevel":1,"level":"2","line":"Yayoi art","number":"2","index":"2","fromtitle":"Japanese_Prehistoric_Art","byteoffset":3160,"anchor":"Yayoi_art"},{"toclevel":1,"level":"2","line":"See also","number":"3","index":"3","fromtitle":"Japanese_Prehistoric_Art","byteoffset":5185,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References and notes","number":"4","index":"4","fromtitle":"Japanese_Prehistoric_Art","byteoffset":5267,"anchor":"References_and_notes"}],"parsewarnings":[],"displaytitle":"Japanese Prehistoric Art","iwlinks":[],"properties":[{"name":"defaultsort","*":"Japanese Prehistoric Art"},{"name":"wikibase_item","*":"Q6158281"}]}}