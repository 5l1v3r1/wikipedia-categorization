ills"},{"ns":0,"exists":"","*":"Peter Ucko"},{"ns":0,"exists":"","*":"Reindeer in Siberian shamanism"},{"ns":0,"exists":"","*":"Ronald Hutton"},{"ns":0,"exists":"","*":"Shaman"},{"ns":0,"exists":"","*":"Shamanism"},{"ns":0,"exists":"","*":"Sl\u00e1ine (comics)"},{"ns":0,"exists":"","*":"The Story of B"},{"ns":4,"exists":"","*":"Wikipedia:Citation needed"}],"templates":[{"ns":10,"exists":"","*":"Template:Citation needed"},{"ns":10,"exists":"","*":"Template:Fix"},{"ns":10,"exists":"","*":"Template:Category handler"},{"ns":10,"exists":"","*":"Template:Fix/category"},{"ns":10,"exists":"","*":"Template:Delink"},{"ns":10,"exists":"","*":"Template:Reflist"},{"ns":10,"exists":"","*":"Template:Cite web"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":828,"exists":"","*":"Module:Unsubst"},{"ns":828,"exists":"","*":"Module:Category handler"},{"ns":828,"exists":"","*":"Module:Yesno"},{"ns":828,"exists":"","*":"Module:Category handler/data"},{"ns":828,"exists":"","*":"Module:Category handler/config"},{"ns":828,"exists":"","*":"Module:Category handler/shared"},{"ns":828,"exists":"","*":"Module:Category handler/blacklist"},{"ns":828,"exists":"","*":"Module:Namespace detect/data"},{"ns":828,"exists":"","*":"Module:Namespace detect/config"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:Delink"},{"ns":828,"exists":"","*":"Module:No globals"},{"ns":828,"exists":"","*":"Module:Citation/CS1"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Configuration"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Whitelist"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Utilities"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Date validation"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Identifiers"},{"ns":828,"exists":"","*":"Module:Citation/CS1/COinS"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"}],"images":["Pintura_Trois_Freres.jpg"],"externallinks":["http://www.britannica.com/EBchecked/topic/606267/Trois-Freres#ref=ref893311","http://www.strangehistory.net/2011/07/06/cave-art-cobblers/","http://media-2.web.britannica.com/eb-media/63/4763-004-824529EB.jpg","http://www.donsmaps.com/clickphotos/sorcerer.jpg"],"sections":[{"toclevel":1,"level":"2","line":"Breuil's drawing","number":"1","index":"1","fromtitle":"The_Sorcerer_(cave_art)","byteoffset":1742,"anchor":"Breuil's_drawing"},{"toclevel":1,"level":"2","line":"Critique","number":"2","index":"2","fromtitle":"The_Sorcerer_(cave_art)","byteoffset":2504,"anchor":"Critique"},{"toclevel":1,"level":"2","line":"Continuing influence","number":"3","index":"3","fromtitle":"The_Sorcerer_(cave_art)","byteoffset":4169,"anchor":"Continuing_influence"},{"toclevel":1,"level":"2","line":"Popular culture","number":"4","index":"4","fromtitle":"The_Sorcerer_(cave_art)","byteoffset":4596,"anchor":"Popular_culture"},{"toclevel":1,"level":"2","line":"See also","number":"5","index":"5","fromtitle":"The_Sorcerer_(cave_art)","byteoffset":5133,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"6","index":"6","fromtitle":"The_Sorcerer_(cave_art)","byteoffset":5198,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"7","index":"7","fromtitle":"The_Sorcerer_(cave_art)","byteoffset":5226,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"The Sorcerer (cave art)","iwlinks":[],"properties":[{"name":"defaultsort","*":"Sorcerer (cave art), The"},{"name":"wikibase_item","*":"Q3835782"}]}}