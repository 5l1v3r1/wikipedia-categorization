tor","byteoffset":5320,"anchor":"Further_development"},{"toclevel":1,"level":"2","line":"References","number":"4","index":"4","fromtitle":"Repressilator","byteoffset":5497,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"5","index":"5","fromtitle":"Repressilator","byteoffset":5527,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"Repressilator","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q933128"}]}}