Western Malayo-Polynesian languages","number":"1","index":"1","fromtitle":"Vanua","byteoffset":942,"anchor":"In_Western_Malayo-Polynesian_languages"},{"toclevel":2,"level":"3","line":"Philippines","number":"1.1","index":"2","fromtitle":"Vanua","byteoffset":985,"anchor":"Philippines"},{"toclevel":2,"level":"3","line":"Malaysia and Indonesia","number":"1.2","index":"3","fromtitle":"Vanua","byteoffset":1352,"anchor":"Malaysia_and_Indonesia"},{"toclevel":1,"level":"2","line":"In Melanesia","number":"2","index":"4","fromtitle":"Vanua","byteoffset":2301,"anchor":"In_Melanesia"},{"toclevel":2,"level":"3","line":"In Vanuatu","number":"2.1","index":"5","fromtitle":"Vanua","byteoffset":2409,"anchor":"In_Vanuatu"},{"toclevel":2,"level":"3","line":"In Fiji","number":"2.2","index":"6","fromtitle":"Vanua","byteoffset":2891,"anchor":"In_Fiji"},{"toclevel":1,"level":"2","line":"Polynesian languages","number":"3","index":"7","fromtitle":"Vanua","byteoffset":5340,"anchor":"Polynesian_languages"},{"toclevel":1,"level":"2","line":"See also","number":"4","index":"8","fromtitle":"Vanua","byteoffset":5880,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"5","index":"9","fromtitle":"Vanua","byteoffset":5992,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Vanua","iwlinks":[],"properties":[{"name":"notoc","*":""},{"name":"wikibase_item","*":"Q7915227"}]}}