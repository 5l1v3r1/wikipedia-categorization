/td>\n<td>\n</td>\n<td>\n</td>\n<td>\n</td>\n<td>\n</td>\n<td>\n</td></tr>\n<tr>\n<th>Social interaction\n</th>\n<td style=\"background:#9F9;vertical-align:middle;text-align:center;\" class=\"table-yes\">Yes\n</td>\n<td style=\"background:#9F9;vertical-align:middle;text-align:center;\" class=\"table-yes\">Yes\n</td>\n<td style=\"background:#9F9;vertical-align:middle;text-align:center;\" class=\"table-yes\">Yes\n</td>\n<td style=\"background:#9F9;vertical-align:middle;text-align:center;\" class=\"table-yes\">Yes\n</td>\n<td style=\"background:#9F9;vertical-align:middle;text-align:center;\" class=\"table-yes\">Yes\n</td>\n<td style=\"background:#9F9;vertical-align:middle;text-align:center;\" class=\"table-yes\">Yes\n</td>\n<td>\n</td>\n<td>\n</td>\n<td>\n</td>\n<td>\n</td></tr>\n<tr>\n<th>Repeated interaction\n</th>\n<td style=\"background:#9F9;vertical-align:middle;text-align:center;\" class=\"table-yes\">Yes\n</td>\n<td style=\"background:#9F9;vertical-align:middle;text-align:center;\" class=\"table-yes\">Yes\n</td>\n<td style=\"background:#9F9;vertical-align:middle;text-align:center;\" class=\"table-yes\">Yes\n</td>\n<td style=\"background:#9F9;vertical-align:middle;text-align:center;\" class=\"table-yes\">Yes\n</td>\n<td style=\"background:#9F9;vertical-align:middle;text-align:center;\" class=\"table-yes\">Yes\n</td>\n<td style=\"background:#9F9;vertical-align:middle;text-align:center;\" class=\"table-yes\">Yes\n</td>\n<td style=\"background:#9F9;vertical-align:middle;text-align:center;\" class=\"table-yes\">Yes\n</td>\n<td>\n</td>\n<td>\n</td>\n<td>\n</td></tr>\n<tr>\n<th>Regular interaction\n</th>\n<td style=\"background:#9F9;vertical-align:middle;text-align:center;\" class=\"table-yes\">Yes\n</td>\n<td style=\"background:#9F9;vertical-align:middle;text-align:center;\" class=\"table-yes\">Yes\n</td>\n<td style=\"background:#9F9;vertical-align:middle;text-align:center;\" class=\"table-yes\">Yes\n</td>\n<td style=\"background:#9F9;vertical-align:middle;text-align: