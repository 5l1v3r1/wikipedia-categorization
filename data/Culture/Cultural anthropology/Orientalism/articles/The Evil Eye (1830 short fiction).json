w.isfdb.org/cgi-bin/title.cgi?98520"],"sections":[{"toclevel":1,"level":"2","line":"Summary","number":"1","index":"1","fromtitle":"The_Evil_Eye_(1830_short_fiction)","byteoffset":554,"anchor":"Summary"},{"toclevel":1,"level":"2","line":"Publication History","number":"2","index":"2","fromtitle":"The_Evil_Eye_(1830_short_fiction)","byteoffset":3276,"anchor":"Publication_History"},{"toclevel":1,"level":"2","line":"Themes and Influences","number":"3","index":"3","fromtitle":"The_Evil_Eye_(1830_short_fiction)","byteoffset":4354,"anchor":"Themes_and_Influences"},{"toclevel":1,"level":"2","line":"References","number":"4","index":"4","fromtitle":"The_Evil_Eye_(1830_short_fiction)","byteoffset":6418,"anchor":"References"}],"parsewarnings":[],"displaytitle":"The Evil Eye (1830 short fiction)","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q48729851"}]}}