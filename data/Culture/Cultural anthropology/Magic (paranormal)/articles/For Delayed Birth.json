,"*":"Template:Verse translation"},{"ns":10,"exists":"","*":"Template:Lang"},{"ns":10,"exists":"","*":"Template:Pad"},{"ns":10,"exists":"","*":"Template:Reflist"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":10,"exists":"","*":"Template:Mythology-stub"},{"ns":10,"exists":"","*":"Template:Myth-stub"},{"ns":10,"exists":"","*":"Template:Asbox"},{"ns":828,"exists":"","*":"Module:Lang"},{"ns":828,"exists":"","*":"Module:No globals"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:Language/name/data"},{"ns":828,"exists":"","*":"Module:Language/data/iana languages"},{"ns":828,"exists":"","*":"Module:Language/data/ISO 639-3"},{"ns":828,"exists":"","*":"Module:Language/data/wp languages"},{"ns":828,"exists":"","*":"Module:Language/data/iana scripts"},{"ns":828,"exists":"","*":"Module:Language/data/iana regions"},{"ns":828,"exists":"","*":"Module:Language/data/iana variants"},{"ns":828,"exists":"","*":"Module:Language/data/iana suppressed scripts"},{"ns":828,"exists":"","*":"Module:Lang/ISO 639 synonyms"},{"ns":828,"exists":"","*":"Module:Lang/data"},{"ns":828,"exists":"","*":"Module:Unicode data"},{"ns":828,"exists":"","*":"Module:Unicode data/scripts"},{"ns":828,"exists":"","*":"Module:Table"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"},{"ns":828,"exists":"","*":"Module:Asbox"},{"ns":828,"exists":"","*":"Module:Buffer"},{"ns":828,"exists":"","*":"Module:Navbar"}],"images":["Draig.svg"],"externallinks":["https://www.jstor.org/stable/438781","https://dx.doi.org/10.1080/00393278508587899","http://digital.library.leeds.ac.uk/id/eprint/289"],"sections":[{"toclevel":1,"level":"2","line":"Text","number":"1","index":"1","fromtitle":"For_Delayed_Birth","byteoffset":1510,"anchor":"Text"},{"toclevel":1,"level":"2","line":"References","number":"2","index":"2","fromtitle":"For_Delayed_Birth","byteoffset":5362,"anchor":"References"}],"parsewarnings":[],"displaytitle":"For Delayed Birth","iwlinks":[],"properties":[{"name":"wiki