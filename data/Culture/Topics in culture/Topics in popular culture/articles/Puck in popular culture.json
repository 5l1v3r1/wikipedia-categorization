{"ns":828,"exists":"","*":"Module:Message box"},{"ns":828,"exists":"","*":"Module:No globals"},{"ns":828,"exists":"","*":"Module:Yesno"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:Message box/configuration"},{"ns":828,"exists":"","*":"Module:Unsubst"},{"ns":828,"exists":"","*":"Module:Category handler"},{"ns":828,"exists":"","*":"Module:Category handler/data"},{"ns":828,"exists":"","*":"Module:Category handler/config"},{"ns":828,"exists":"","*":"Module:Category handler/shared"},{"ns":828,"exists":"","*":"Module:Category handler/blacklist"},{"ns":828,"exists":"","*":"Module:Namespace detect/data"},{"ns":828,"exists":"","*":"Module:Namespace detect/config"},{"ns":828,"exists":"","*":"Module:String"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"}],"images":["Ambox_important.svg","Wiki_letter_w.svg","Question_book-new.svg","Puck_1629.JPG"],"externallinks":["//edwardbetts.com/find_link?q=Puck_in_popular_culture"],"sections":[{"toclevel":1,"level":"2","line":"Examples","number":"1","index":"1","fromtitle":"Puck_in_popular_culture","byteoffset":927,"anchor":"Examples"}],"parsewarnings":[],"displaytitle":"Puck in popular culture","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q7258243"}]}}