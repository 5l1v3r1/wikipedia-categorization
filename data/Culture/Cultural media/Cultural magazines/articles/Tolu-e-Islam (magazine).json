evel":1,"level":"2","line":"1936 Kitab Khana Tolu-e-Islam","number":"2","index":"2","fromtitle":"Tolu-e-Islam_(magazine)","byteoffset":2272,"anchor":"1936_Kitab_Khana_Tolu-e-Islam"},{"toclevel":1,"level":"2","line":"1938 Ghulam Ahmad Parwez","number":"3","index":"3","fromtitle":"Tolu-e-Islam_(magazine)","byteoffset":2870,"anchor":"1938_Ghulam_Ahmad_Parwez"},{"toclevel":1,"level":"2","line":"After the emergence of Pakistan","number":"4","index":"4","fromtitle":"Tolu-e-Islam_(magazine)","byteoffset":3739,"anchor":"After_the_emergence_of_Pakistan"},{"toclevel":1,"level":"2","line":"See also","number":"5","index":"5","fromtitle":"Tolu-e-Islam_(magazine)","byteoffset":4336,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"6","index":"6","fromtitle":"Tolu-e-Islam_(magazine)","byteoffset":4401,"anchor":"References"}],"parsewarnings":[],"displaytitle":"<i>Tolu-e-Islam</i> (magazine)","iwlinks":[],"properties":[{"name":"displaytitle","*":"<i>Tolu-e-Islam</i> (magazine)"},{"name":"wikibase_item","*":"Q7814652"}]}}