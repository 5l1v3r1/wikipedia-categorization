ist"},{"ns":828,"exists":"","*":"Module:Namespace detect/data"},{"ns":828,"exists":"","*":"Module:Namespace detect/config"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"}],"images":["Question_book-new.svg"],"externallinks":["http://reader.eblib.com/(S(l3pwge1uoxu5akgi5booohoz))/Reader.aspx?p=1099476&o=785&u=mKyUaTgCatU4QFkEYCpDmA%3D%3D&t=1359430452&h=7AC69DD72F8AD341C0215D22214BA5A21AB5CCA9&s=15609797&ut=2445&pg=1&r=img&c=-1&pat=n#","https://books.google.com/books?id=3z9EpgisKOgC&q=%22living+funeral%22#v=snippet&q=%22living%20funeral%22&f=false","http://ink.library.smu.edu.sg/cgi/viewcontent.cgi?article=1077&context=soss_research&sei-redir=1&referer=http://scholar.google.com/scholar?hl%3Den%26q%3Dseizenso%26btnG%3D%26as_sdt%3D1%252C39%26as_sdtp%3D#search=%22seizenso%22","http://www.tandfonline.com/doi/full/10.1080/13576270410001652578#tabModule"],"sections":[{"toclevel":1,"level":"2","line":"Purpose","number":"1","index":"1","fromtitle":"Living_funeral","byteoffset":645,"anchor":"Purpose"},{"toclevel":1,"level":"2","line":"Cost","number":"2","index":"2","fromtitle":"Living_funeral","byteoffset":1302,"anchor":"Cost"},{"toclevel":1,"level":"2","line":"Aspects of a living funeral","number":"3","index":"3","fromtitle":"Living_funeral","byteoffset":1748,"anchor":"Aspects_of_a_living_funeral"},{"toclevel":1,"level":"2","line":"Japan","number":"4","index":"4","fromtitle":"Living_funeral","byteoffset":2939,"anchor":"Japan"},{"toclevel":1,"level":"2","line":"Controversy","number":"5","index":"5","fromtitle":"Living_funeral","byteoffset":3825,"anchor":"Controversy"},{"toclevel":1,"level":"2","line":"Fiction","number":"6","index":"6","fromtitle":"Living_funeral","byteoffset":5126,"anchor":"Fiction"},{"toclevel":1,"level":"2","line":"Non-Fiction","number":"7","index":"7","fromtitle":"Living_funeral","byteoffset":5477,"anchor":"Non-Fiction"},{"toclevel":1,"level":"2","line":"References","number":"8","index":"8","fromtitle":"Living_funeral","byteoffset":5875,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Living funeral","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q11574428"}]}}