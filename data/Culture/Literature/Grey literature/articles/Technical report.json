exists":"","*":"Module:Check for unknown parameters"}],"images":[],"externallinks":["http://www.iso.org/iso/catalogue_detail.htm?csnumber=12160","http://eprints.rclis.org/7469/1/nancy.pdf","http://www.iso.org/iso/catalogue_detail.htm?csnumber=18506","http://www.niso.org/kst/reports/standards?step=2&gid=None&project_key:ustring:iso-8859-1=c8d16b0f23c850c46455e307c4e0b4a25088b2f1","https://web.archive.org/web/20120304082842/http://www.niso.org/kst/reports/standards?step=2&gid=None&project_key:ustring:iso-8859-1=c8d16b0f23c850c46455e307c4e0b4a25088b2f1","http://www.ntis.gov/","http://www.niso.org","http://www.ansi.org","http://www.dtic.mil/dtic/find_a_doc.html","https://ntrs.nasa.gov/search.jsp","http://naca.central.cranfield.ac.uk/","http://www.technicalreports.org/trail/search/","http://www.crl.edu/grn/trail"],"sections":[{"toclevel":1,"level":"2","line":"Description","number":"1","index":"1","fromtitle":"Technical_report","byteoffset":1235,"anchor":"Description"},{"toclevel":1,"level":"2","line":"Production guidelines","number":"2","index":"2","fromtitle":"Technical_report","byteoffset":2214,"anchor":"Production_guidelines"},{"toclevel":1,"level":"2","line":"Publication","number":"3","index":"3","fromtitle":"Technical_report","byteoffset":3077,"anchor":"Publication"},{"toclevel":1,"level":"2","line":"See also","number":"4","index":"4","fromtitle":"Technical_report","byteoffset":4654,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"5","index":"5","fromtitle":"Technical_report","byteoffset":4690,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"6","index":"6","fromtitle":"Technical_report","byteoffset":4718,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"Technical report","iwlinks":[{"prefix":"de","url":"https://de.wikipedia.org/wiki/International_Standard_Technical_Report_Number","*":"de:International Standard Technical Report Number"}],"properties":[{"name":"wikibase_item","*":"Q3099732"}]}}