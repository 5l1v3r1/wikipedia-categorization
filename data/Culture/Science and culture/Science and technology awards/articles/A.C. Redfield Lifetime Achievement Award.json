llinks":["https://aslo.org/p/cm/ld/fid=1066-","http://www.aslo.org/information/awards.html#ACHI-"],"sections":[{"toclevel":1,"level":"2","line":"Recipients","number":"1","index":"1","fromtitle":"A.C._Redfield_Lifetime_Achievement_Award","byteoffset":355,"anchor":"Recipients"},{"toclevel":1,"level":"2","line":"References","number":"2","index":"2","fromtitle":"A.C._Redfield_Lifetime_Achievement_Award","byteoffset":6240,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"3","index":"3","fromtitle":"A.C._Redfield_Lifetime_Achievement_Award","byteoffset":6543,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"A.C. Redfield Lifetime Achievement Award","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q4647076"}]}}