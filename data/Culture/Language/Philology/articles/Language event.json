s":"","*":"Help:Maintenance template removal"},{"ns":1,"*":"Talk:Language event"}],"templates":[{"ns":10,"exists":"","*":"Template:Confusing"},{"ns":10,"exists":"","*":"Template:Ambox"},{"ns":10,"exists":"","*":"Template:Clarifyme"},{"ns":10,"exists":"","*":"Template:Clarify"},{"ns":10,"exists":"","*":"Template:Fix-span"},{"ns":10,"exists":"","*":"Template:Category handler"},{"ns":10,"exists":"","*":"Template:Fix/category"},{"ns":10,"exists":"","*":"Template:Replace"},{"ns":10,"exists":"","*":"Template:Delink"},{"ns":828,"exists":"","*":"Module:Unsubst"},{"ns":828,"exists":"","*":"Module:Message box"},{"ns":828,"exists":"","*":"Module:No globals"},{"ns":828,"exists":"","*":"Module:Yesno"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:Message box/configuration"},{"ns":828,"exists":"","*":"Module:Category handler"},{"ns":828,"exists":"","*":"Module:Category handler/data"},{"ns":828,"exists":"","*":"Module:Category handler/config"},{"ns":828,"exists":"","*":"Module:Category handler/shared"},{"ns":828,"exists":"","*":"Module:Category handler/blacklist"},{"ns":828,"exists":"","*":"Module:Namespace detect/data"},{"ns":828,"exists":"","*":"Module:Namespace detect/config"},{"ns":828,"exists":"","*":"Module:String"},{"ns":828,"exists":"","*":"Module:Delink"}],"images":["Edit-clear.svg"],"externallinks":["https://en.oxforddictionaries.com/definition/language_event"],"sections":[{"toclevel":1,"level":"2","line":"Paul","number":"1","index":"1","fromtitle":"Language_event","byteoffset":1327,"anchor":"Paul"},{"toclevel":1,"level":"2","line":"Martin Luther","number":"2","index":"2","fromtitle":"Language_event","byteoffset":1880,"anchor":"Martin_Luther"},{"toclevel":1,"level":"2","line":"Ernst Fuchs","number":"3","index":"3","fromtitle":"Language_event","byteoffset":3722,"anchor":"Ernst_Fuchs"},{"toclevel":1,"level":"2","line":"Gerhard Ebeling","number":"4","index":"4","fromtitle":"Language_event","byteoffset":5518,"anchor":"Gerhard_Ebeling"},{"toc