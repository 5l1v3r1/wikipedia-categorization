s":0,"exists":"","*":"Personal statement"},{"ns":0,"exists":"","*":"Ponca"},{"ns":0,"exists":"","*":"Racist"},{"ns":0,"exists":"","*":"Slavery"},{"ns":0,"exists":"","*":"Social status"},{"ns":0,"exists":"","*":"Society for Effecting the Abolition of the Slave Trade"},{"ns":0,"exists":"","*":"South Africa"},{"ns":0,"exists":"","*":"Standing Bear"},{"ns":0,"exists":"","*":"The Elephant Man (film)"},{"ns":0,"exists":"","*":"United States"},{"ns":0,"exists":"","*":"United States Supreme Court"}],"templates":[{"ns":10,"exists":"","*":"Template:Reflist"},{"ns":10,"exists":"","*":"Template:Cite book"},{"ns":10,"exists":"","*":"Template:Cite web"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":828,"exists":"","*":"Module:Citation/CS1"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Configuration"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Whitelist"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Utilities"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Date validation"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Identifiers"},{"ns":828,"exists":"","*":"Module:Citation/CS1/COinS"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"}],"images":["I_Am_a_Man_-_Diorama_of_Memphis_Sanitation_Workers_Strike_-_National_Civil_Rights_Museum_-_Downtown_Memphis_-_Tennessee_-_USA.jpg","Am_I_not_a_man.jpg"],"externallinks":["https://books.google.com/books?id=UILcQZS6Bi4C&pg=PA61&lpg=PA61&dq=boy+african+american+pejorative&source=bl&ots=i-89AM4K-5&sig=LIjrydmYVlYYVrVnMG8b94KUCWE&hl=en#v=onepage&q=boy%20african%20american%20pejorative&f=false","http://www.bbc.co.uk/history/british/abolition/africans_in_art_gallery_02.shtml","http://aalbc.com/reviews/am_i_not_a_man.htm","http://www.miamiherald.com/multimedia/news/iamaman/pitts.html","https://www.allaboutjazz.com/i-am-a-man-ron-miles-yellowbird-review-by-dan-bilawsky.php","https://www.wsj.com/articles/SB10001424052702303714704576384023771126308?mod=googlewsj","http://www.aina.org/news/20110520135252.htm"],"secti