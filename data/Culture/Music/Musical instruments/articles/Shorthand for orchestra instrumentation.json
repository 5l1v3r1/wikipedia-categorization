mplate:ISBN"},{"ns":10,"exists":"","*":"Template:Catalog lookup link"},{"ns":10,"exists":"","*":"Template:Trim"},{"ns":10,"exists":"","*":"Template:Yesno-no"},{"ns":10,"exists":"","*":"Template:Yesno"},{"ns":10,"exists":"","*":"Template:Error-small"},{"ns":10,"exists":"","*":"Template:Tl"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":10,"exists":"","*":"Template:De icon"},{"ns":10,"exists":"","*":"Template:Link language"},{"ns":10,"exists":"","*":"Template:ISO 639 name de"},{"ns":10,"exists":"","*":"Template:Category handler"},{"ns":10,"exists":"","*":"Template:Reflist"},{"ns":828,"exists":"","*":"Module:Unsubst"},{"ns":828,"exists":"","*":"Module:Message box"},{"ns":828,"exists":"","*":"Module:No globals"},{"ns":828,"exists":"","*":"Module:Yesno"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:Message box/configuration"},{"ns":828,"exists":"","*":"Module:Category handler"},{"ns":828,"exists":"","*":"Module:Category handler/data"},{"ns":828,"exists":"","*":"Module:Category handler/config"},{"ns":828,"exists":"","*":"Module:Category handler/shared"},{"ns":828,"exists":"","*":"Module:Category handler/blacklist"},{"ns":828,"exists":"","*":"Module:Namespace detect/data"},{"ns":828,"exists":"","*":"Module:Namespace detect/config"},{"ns":828,"exists":"","*":"Module:Check isxn"},{"ns":828,"exists":"","*":"Module:Error"},{"ns":828,"exists":"","*":"Module:Link language"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Configuration"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"}],"images":["Question_book-new.svg"],"externallinks":["http://desatheory.janetpeachey.com/Orchestral%20Instruments.html","https://daniels-orchestral.com/about/this-online-tool/","http://www.boosey.com/cr/perusals/score.asp?id=1096","http://www.stageamusical.com/wp-content/uploads/2012/10/Phantom-Guidelines-2012-1.pdf","http://webdoc.sub.gwdg.de/ebook/aw/2004/ddb/rak_musik_2003.pdf"],"sections":[{"toclevel":1,"level":"2","line":"General approach","number":"1","index":"1","fromtitle":"Shorthand_for_orchestra_instrumentation","byteoffset":532,"anchor":"General_approach"},{"toclevel":1,"level":"2","line":"Degree of standardisation","number":"2","index":"2","fromtitle":"Shorthand_for_orchestra_instrumentation","byteoffset":1660,"anchor":"Degree_of_standardisation"},{"toclevel":1,"level":"2","line":"Examples","number":"3","index":"3","fromtitle":"Shorthand_for_orchestra_instrumentation","byteoffset":3342,"anchor":"Examples"},{"toclevel":1,"level":"2","line":"Sources","number":"4","index":"4","fromtitle":"Shorthand_for_orchestra_instrumentation","byteoffset":6513,"anchor":"Sources"},{"toclevel":1,"level":"2","line":"Notes","number":"5","index":"5","fromtitle":"Shorthand_for_orchestra_instrumentation","byteoffset":6794,"anchor":"Notes"}],"parsewarnings":[],"displaytitle":"Shorthand for orchestra instrumentation","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q15813070"}]}}