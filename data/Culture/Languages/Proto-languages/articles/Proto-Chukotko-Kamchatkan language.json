*":"Referential case"},{"ns":0,"*":"Attributive case"},{"ns":12,"exists":"","*":"Help:IPA"}],"templates":[{"ns":10,"exists":"","*":"Template:IPA"},{"ns":10,"exists":"","*":"Template:Reflist"},{"ns":10,"exists":"","*":"Template:Cite web"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":828,"exists":"","*":"Module:Citation/CS1"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Configuration"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Whitelist"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Utilities"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Date validation"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Identifiers"},{"ns":828,"exists":"","*":"Module:Citation/CS1/COinS"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"}],"images":[],"externallinks":["http://commons.emich.edu/cgi/viewcontent.cgi?article=1333&context=theses","http://www.degruyter.com/view/books/9783110925388/9783110925388.426/9783110925388.426.xml?format=EBOK"],"sections":[{"toclevel":1,"level":"2","line":"Phonology","number":"1","index":"1","fromtitle":"Proto-Chukotko-Kamchatkan_language","byteoffset":850,"anchor":"Phonology"},{"toclevel":2,"level":"3","line":"Consonants","number":"1.1","index":"2","fromtitle":"Proto-Chukotko-Kamchatkan_language","byteoffset":986,"anchor":"Consonants"},{"toclevel":2,"level":"3","line":"Vowels","number":"1.2","index":"3","fromtitle":"Proto-Chukotko-Kamchatkan_language","byteoffset":2069,"anchor":"Vowels"},{"toclevel":1,"level":"2","line":"Grammar","number":"2","index":"4","fromtitle":"Proto-Chukotko-Kamchatkan_language","byteoffset":2333,"anchor":"Grammar"},{"toclevel":1,"level":"2","line":"See also","number":"3","index":"5","fromtitle":"Proto-Chukotko-Kamchatkan_language","byteoffset":4604,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"4","index":"6","fromtitle":"Proto-Chukotko-Kamchatkan_language","byteoffset":4685,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Proto-Chukotko-Kamchatkan language","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q7251837"}]}}