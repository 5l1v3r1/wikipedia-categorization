,{"ns":0,"exists":"","*":"Aeneas"},{"ns":0,"exists":"","*":"Alaksandu treaty"},{"ns":0,"exists":"","*":"Anchises"},{"ns":0,"exists":"","*":"Andrew Dalby"},{"ns":0,"exists":"","*":"Arzawa"},{"ns":0,"exists":"","*":"Calvert Watkins"},{"ns":0,"exists":"","*":"Colchis"},{"ns":0,"exists":"","*":"Flyting"},{"ns":0,"exists":"","*":"Glottolog"},{"ns":0,"exists":"","*":"Greece"},{"ns":0,"exists":"","*":"Historicity of the Iliad"},{"ns":0,"exists":"","*":"Hittite empire"},{"ns":0,"exists":"","*":"Hittite language"},{"ns":0,"exists":"","*":"Homeric Greek"},{"ns":0,"exists":"","*":"ISO 639-3"},{"ns":0,"exists":"","*":"Iliad"},{"ns":0,"exists":"","*":"International Standard Book Number"},{"ns":0,"exists":"","*":"Jaan Puhvel"},{"ns":0,"exists":"","*":"Jason"},{"ns":0,"exists":"","*":"Joachim Latacz"},{"ns":0,"exists":"","*":"Language family"},{"ns":0,"exists":"","*":"Lingua franca"},{"ns":0,"exists":"","*":"Luwian"},{"ns":0,"exists":"","*":"Luwian language"},{"ns":0,"exists":"","*":"Medea"},{"ns":0,"exists":"","*":"Punic"},{"ns":0,"exists":"","*":"Trojan Battle Order"},{"ns":0,"exists":"","*":"Trojan War"},{"ns":0,"exists":"","*":"Troy"},{"ns":0,"exists":"","*":"Troy VII"},{"ns":0,"exists":"","*":"Turnus"},{"ns":0,"exists":"","*":"Unclassified language"},{"ns":0,"exists":"","*":"University of Chicago"},{"ns":0,"exists":"","*":"University of T\u00fcbingen"},{"ns":0,"exists":"","*":"Wilusa"},{"ns":0,"*":"Hilary Mackie"}],"templates":[{"ns":10,"exists":"","*":"Template:Infobox language"},{"ns":10,"exists":"","*":"Template:Infobox"},{"ns":10,"exists":"","*":"Template:Infobox language/family-color"},{"ns":10,"exists":"","*":"Template:Infobox language/genetic"},{"ns":10,"exists":"","*":"Template:Longitem"},{"ns":10,"exists":"","*":"Template:Allow wrap"},{"ns":10,"exists":"","*":"Template:Nowrap"},{"ns":10,"exists":"","*":"Template:Infobox language/linguistlist"},{"ns":10,"exists":"","*":"Template:Template other"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":10,"exists":"","*":