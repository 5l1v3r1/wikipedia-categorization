le:Citation/CS1"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Configuration"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Whitelist"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Utilities"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Date validation"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Identifiers"},{"ns":828,"exists":"","*":"Module:Citation/CS1/COinS"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"}],"images":[],"externallinks":["https://books.google.com/books?id=Sba145B2WXUC&pg=PA125","http://global.britannica.com/EBchecked/topic/90469/Camargo-Society"],"sections":[{"toclevel":1,"level":"2","line":"The scene in 1930","number":"1","index":"1","fromtitle":"Camargo_Society","byteoffset":1142,"anchor":"The_scene_in_1930"},{"toclevel":1,"level":"2","line":"Formation","number":"2","index":"2","fromtitle":"Camargo_Society","byteoffset":2395,"anchor":"Formation"},{"toclevel":1,"level":"2","line":"Productions","number":"3","index":"3","fromtitle":"Camargo_Society","byteoffset":3652,"anchor":"Productions"},{"toclevel":1,"level":"2","line":"The end of the society","number":"4","index":"4","fromtitle":"Camargo_Society","byteoffset":5562,"anchor":"The_end_of_the_society"},{"toclevel":1,"level":"2","line":"References","number":"5","index":"5","fromtitle":"Camargo_Society","byteoffset":6741,"anchor":"References"},{"toclevel":1,"level":"2","line":"Further reading","number":"6","index":"6","fromtitle":"Camargo_Society","byteoffset":6771,"anchor":"Further_reading"}],"parsewarnings":[],"displaytitle":"Camargo Society","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q17155641"}]}}