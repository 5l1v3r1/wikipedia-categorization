":"Module:Language/data/iana regions"},{"ns":828,"exists":"","*":"Module:Language/data/iana variants"},{"ns":828,"exists":"","*":"Module:Language/data/iana suppressed scripts"},{"ns":828,"exists":"","*":"Module:Lang/ISO 639 synonyms"},{"ns":828,"exists":"","*":"Module:Lang/data"},{"ns":828,"exists":"","*":"Module:Unicode data"},{"ns":828,"exists":"","*":"Module:Coordinates"},{"ns":828,"exists":"","*":"Module:Math"}],"images":["Text_document_with_red_question_mark.svg"],"externallinks":["http://www.savn.fo/00271/00298/","http://www.natmus.fo","http://www.empressgemma.com/faroes.html","//tools.wmflabs.org/geohack/geohack.php?pagename=Sandur_Hoard&params=61_50_33_N_6_48_28_W_region:FO_type:landmark_source:kolossus-dewiki"],"sections":[{"toclevel":1,"level":"2","line":"Find location and owner","number":"1","index":"1","fromtitle":"Sandur_Hoard","byteoffset":1007,"anchor":"Find_location_and_owner"},{"toclevel":1,"level":"2","line":"Index of the coins","number":"2","index":"2","fromtitle":"Sandur_Hoard","byteoffset":1826,"anchor":"Index_of_the_coins"},{"toclevel":1,"level":"2","line":"Literature","number":"3","index":"3","fromtitle":"Sandur_Hoard","byteoffset":3706,"anchor":"Literature"},{"toclevel":1,"level":"2","line":"References","number":"4","index":"4","fromtitle":"Sandur_Hoard","byteoffset":4060,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"5","index":"5","fromtitle":"Sandur_Hoard","byteoffset":4090,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"Sandur Hoard","iwlinks":[{"prefix":"de","url":"https://de.wikipedia.org/wiki/M%C3%BCnzfund_von_Sandur","*":"de:M\u00fcnzfund von Sandur"}],"properties":[{"name":"wikibase_item","*":"Q1051134"}]}}