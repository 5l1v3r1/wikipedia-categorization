":828,"exists":"","*":"Module:Lang/ISO 639 synonyms"},{"ns":828,"exists":"","*":"Module:Lang/data"},{"ns":828,"exists":"","*":"Module:Unicode data"},{"ns":828,"exists":"","*":"Module:Unicode data/scripts"},{"ns":828,"exists":"","*":"Module:Table"},{"ns":828,"exists":"","*":"Module:Webarchive"},{"ns":828,"exists":"","*":"Module:Webarchive/data"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"}],"images":["Herod_coin.jpg","HerodObverseChiDiadem.jpg","HerodReverseTripod.jpg","Herod_Archelaus.jpg","Herod_Antipas.jpg","Tiberius_of_a_coin_by_Herod_Philip.jpg","AgrippaPrutahObverse.jpg","AgrippaPrutahReverse.jpg","Salome_coin.jpg"],"externallinks":["http://www.amuseum.org/book/page9.html","http://www.livius.org/he-hg/herodians/herod_archelaus.htm","http://www.amuseum.org/book/page10.html","http://www.amuseum.org/book/page11.html","http://jerusalemperspective.com/%5Cdefault.aspx?tabid=27&ArticleID=1474","https://web.archive.org/web/20071004213641/http://www.jerusalemperspective.com/default.aspx?tabid=27&ArticleID=1474","http://www.jewishvirtuallibrary.org/jsource/History/HerodsCoins.html","http://www.historian.net/coins.htm"],"sections":[{"toclevel":1,"level":"2","line":"Herod the Great 37&ndash;4 BC","number":"1","index":"1","fromtitle":"Herodian_coinage","byteoffset":368,"anchor":"Herod_the_Great_37\u20134_BC"},{"toclevel":1,"level":"2","line":"Herod Archelaus 4 BC&ndash;6 AD","number":"2","index":"2","fromtitle":"Herodian_coinage","byteoffset":3040,"anchor":"Herod_Archelaus_4_BC\u20136_AD"},{"toclevel":1,"level":"2","line":"Herod Antipas 4 BC&ndash;39 AD","number":"3","index":"3","fromtitle":"Herodian_coinage","byteoffset":3970,"anchor":"Herod_Antipas_4_BC\u201339_AD"},{"toclevel":1,"level":"2","line":"Herod Philip II 4 BC&ndash;34 AD","number":"4","index":"4","fromtitle":"Herodian_coinage","byteoffset":5162,"anchor":"Herod_Philip_II_4_BC\u201334_AD"},{"toclevel":1,"level":"2","line":"Herod Agrippa I 37&ndash;44 AD","number":"5","index":"5","fromtitle":"H