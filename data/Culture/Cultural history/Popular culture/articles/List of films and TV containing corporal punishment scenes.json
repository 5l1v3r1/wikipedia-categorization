d"},{"ns":10,"exists":"","*":"Template:Reflist"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":828,"exists":"","*":"Module:Unsubst"},{"ns":828,"exists":"","*":"Module:Message box"},{"ns":828,"exists":"","*":"Module:No globals"},{"ns":828,"exists":"","*":"Module:Yesno"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:Message box/configuration"},{"ns":828,"exists":"","*":"Module:Category handler"},{"ns":828,"exists":"","*":"Module:Category handler/data"},{"ns":828,"exists":"","*":"Module:Category handler/config"},{"ns":828,"exists":"","*":"Module:Category handler/shared"},{"ns":828,"exists":"","*":"Module:Category handler/blacklist"},{"ns":828,"exists":"","*":"Module:Namespace detect/data"},{"ns":828,"exists":"","*":"Module:Namespace detect/config"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"}],"images":["Text_document_with_red_question_mark.svg"],"externallinks":["https://www.imdb.com/title/tt3124186/"],"sections":[{"toclevel":1,"level":"2","line":"References","number":"1","index":"1","fromtitle":"List_of_films_and_TV_containing_corporal_punishment_scenes","byteoffset":5890,"anchor":"References"}],"parsewarnings":[],"displaytitle":"List of films and TV containing corporal punishment scenes","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q55619328"}]}}