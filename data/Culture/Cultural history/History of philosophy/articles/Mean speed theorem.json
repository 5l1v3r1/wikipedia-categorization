s":"","*":"Merton College"},{"ns":0,"exists":"","*":"Nicole Oresme"},{"ns":0,"exists":"","*":"Oresme"},{"ns":0,"exists":"","*":"Oxford Calculators"},{"ns":0,"exists":"","*":"PubMed Identifier"},{"ns":0,"exists":"","*":"Scholasticism"},{"ns":0,"exists":"","*":"Science in the Middle Ages"},{"ns":0,"exists":"","*":"Speed"},{"ns":0,"exists":"","*":"The Merton Rule"},{"ns":0,"exists":"","*":"Trapezoid"},{"ns":0,"exists":"","*":"Uniform acceleration"},{"ns":0,"exists":"","*":"Uniformly accelerated motion"}],"templates":[{"ns":10,"exists":"","*":"Template:Distinguish"},{"ns":10,"exists":"","*":"Template:Blockquote"},{"ns":10,"exists":"","*":"Template:Quote"},{"ns":10,"exists":"","*":"Template:Quote/styles.css"},{"ns":10,"exists":"","*":"Template:Reflist"},{"ns":10,"exists":"","*":"Template:Cite book"},{"ns":10,"exists":"","*":"Template:Cite journal"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":828,"exists":"","*":"Module:Distinguish"},{"ns":828,"exists":"","*":"Module:Hatnote"},{"ns":828,"exists":"","*":"Module:Hatnote list"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:TableTools"},{"ns":828,"exists":"","*":"Module:Citation/CS1"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Configuration"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Whitelist"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Utilities"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Date validation"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Identifiers"},{"ns":828,"exists":"","*":"Module:Citation/CS1/COinS"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"}],"images":["MertonRuleOresme.jpg","Galileo-1638-173.jpg"],"externallinks":["https://books.google.com/books?id=KLQSHUW8FnUC&pg=PA79&lpg=PP1#v=onepage&q&f=false","https://books.google.com/books?id=KLQSHUW8FnUC","http://science.sciencemag.org/content/351/6272/482.full","http://adsabs.harvard.edu/abs/2016Sci...351..482O","//doi.org/10.1126/science.aad8085","//www.ncbi.nlm.nih.gov/pubmed/