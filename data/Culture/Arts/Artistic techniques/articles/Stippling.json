,"exists":"","*":"International Standard Book Number"},{"ns":0,"exists":"","*":"Kevin Sprouls"},{"ns":0,"exists":"","*":"Knurl"},{"ns":0,"exists":"","*":"Lily"},{"ns":0,"exists":"","*":"Line art"},{"ns":0,"exists":"","*":"Machine quilting"},{"ns":0,"exists":"","*":"Medical illustration"},{"ns":0,"exists":"","*":"Metal"},{"ns":0,"exists":"","*":"Painting"},{"ns":0,"exists":"","*":"Pen"},{"ns":0,"exists":"","*":"Photograph"},{"ns":0,"exists":"","*":"Pigment"},{"ns":0,"exists":"","*":"Plastic"},{"ns":0,"exists":"","*":"Pointillism"},{"ns":0,"exists":"","*":"Printmaking"},{"ns":0,"exists":"","*":"Pulcinella"},{"ns":0,"exists":"","*":"Quilting"},{"ns":0,"exists":"","*":"Sculpture"},{"ns":0,"exists":"","*":"Spider mites"},{"ns":0,"exists":"","*":"Stipple (company)"},{"ns":0,"exists":"","*":"Stipple engraving"},{"ns":0,"exists":"","*":"Stippling (dentistry)"},{"ns":0,"exists":"","*":"The Wall Street Journal"},{"ns":0,"exists":"","*":"Wayback Machine"},{"ns":0,"exists":"","*":"Wood"}],"templates":[{"ns":10,"exists":"","*":"Template:Redirect"},{"ns":10,"exists":"","*":"Template:Main"},{"ns":10,"exists":"","*":"Template:Reflist"},{"ns":10,"exists":"","*":"Template:Cite book"},{"ns":10,"exists":"","*":"Template:Webarchive"},{"ns":10,"exists":"","*":"Template:Cite web"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":10,"exists":"","*":"Template:Commons category-inline"},{"ns":10,"exists":"","*":"Template:Sister-inline"},{"ns":828,"exists":"","*":"Module:Redirect hatnote"},{"ns":828,"exists":"","*":"Module:Hatnote"},{"ns":828,"exists":"","*":"Module:Hatnote list"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:Redirect"},{"ns":828,"exists":"","*":"Module:Main"},{"ns":828,"exists":"","*":"Module:Citation/CS1"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Configuration"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Whitelist"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Utilities"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Date v