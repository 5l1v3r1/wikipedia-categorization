ns":0,"exists":"","*":"Facebook"},{"ns":0,"exists":"","*":"Grundrisse"},{"ns":0,"exists":"","*":"JSTOR"},{"ns":0,"exists":"","*":"Jean-Baptiste Say"},{"ns":0,"exists":"","*":"Magazine circulation"},{"ns":0,"exists":"","*":"Michael Warner"},{"ns":0,"exists":"","*":"Newspaper circulation"},{"ns":0,"exists":"","*":"Pronuntiatio"},{"ns":0,"exists":"","*":"Rhetorical situation"},{"ns":0,"exists":"","*":"Richard Lanham"},{"ns":0,"exists":"","*":"Use value"},{"ns":0,"exists":"","*":"YouTube"}],"templates":[{"ns":10,"exists":"","*":"Template:Reflist"},{"ns":10,"exists":"","*":"Template:Cite book"},{"ns":10,"exists":"","*":"Template:Cite journal"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":828,"exists":"","*":"Module:Citation/CS1"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Configuration"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Whitelist"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Utilities"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Date validation"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Identifiers"},{"ns":828,"exists":"","*":"Module:Citation/CS1/COinS"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"}],"images":[],"externallinks":["http://www.oed.com/view/Entry/33232?redirectedFrom=circulation#eid","http://publicculture.org/articles/view/14/1/publics-and-counterpublics","//doi.org/10.1215/08992363-14-1-49","//doi.org/10.1080/02773940509391320","//www.jstor.org/stable/40232607","//doi.org/10.2307/358493","//www.jstor.org/stable/358493","//doi.org/10.1016/j.compcom.2009.09.004","//doi.org/10.1080/07350198.2012.652034","http://10.1353/par.0.0047*","http://10.1353/rap.2012.0049*","https://www.jstor.org/stable/4140651"],"sections":[{"toclevel":1,"level":"2","line":"Rhetorical circulation and the creation of publics","number":"1","index":"1","fromtitle":"Rhetorical_circulation","byteoffset":809,"anchor":"Rhetorical_circulation_and_the_creation_of_publics"},{"toclevel":1,"level":"2","line":"Rhetorical circulation as a new metaphor","number":"2","index":"2","fromtitle":"Rhetorical_circulation","byteoffset":1714,"anchor":"Rhetorical_circulation_as_a_new_metaphor"},{"toclevel":1,"level":"2","line":"Rhetorical circulation and Marx's <i>Grundrisse</i>","number":"3","index":"3","fromtitle":"Rhetorical_circulation","byteoffset":2614,"anchor":"Rhetorical_circulation_and_Marx's_Grundrisse"},{"toclevel":2,"level":"3","line":"Economics","number":"3.1","index":"4","fromtitle":"Rhetorical_circulation","byteoffset":2668,"anchor":"Economics"},{"toclevel":2,"level":"3","line":"Distribution","number":"3.2","index":"5","fromtitle":"Rhetorical_circulation","byteoffset":3773,"anchor":"Distribution"},{"toclevel":2,"level":"3","line":"Transformation","number":"3.3","index":"6","fromtitle":"Rhetorical_circulation","byteoffset":5258,"anchor":"Transformation"},{"toclevel":1,"level":"2","line":"References","number":"4","index":"7","fromtitle":"Rhetorical_circulation","byteoffset":5756,"anchor":"References"},{"toclevel":1,"level":"2","line":"Further reading","number":"5","index":"8","fromtitle":"Rhetorical_circulation","byteoffset":6007,"anchor":"Further_reading"}],"parsewarnings":[],"displaytitle":"Rhetorical circulation","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q17078426"}]}}