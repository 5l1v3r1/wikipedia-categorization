"http://www.dramatoolkit.co.uk/"],"sections":[{"toclevel":1,"level":"2","line":"Drama games","number":"1","index":"1","fromtitle":"Drama_teaching_techniques","byteoffset":178,"anchor":"Drama_games"},{"toclevel":1,"level":"2","line":"Choral speaking","number":"2","index":"2","fromtitle":"Drama_teaching_techniques","byteoffset":952,"anchor":"Choral_speaking"},{"toclevel":1,"level":"2","line":"Tableaux","number":"3","index":"3","fromtitle":"Drama_teaching_techniques","byteoffset":1238,"anchor":"Tableaux"},{"toclevel":1,"level":"2","line":"Improvisation","number":"4","index":"4","fromtitle":"Drama_teaching_techniques","byteoffset":1646,"anchor":"Improvisation"},{"toclevel":1,"level":"2","line":"Role playing","number":"5","index":"5","fromtitle":"Drama_teaching_techniques","byteoffset":2013,"anchor":"Role_playing"},{"toclevel":1,"level":"2","line":"See also...","number":"6","index":"6","fromtitle":"Drama_teaching_techniques","byteoffset":5159,"anchor":"See_also..."},{"toclevel":1,"level":"2","line":"References","number":"7","index":"7","fromtitle":"Drama_teaching_techniques","byteoffset":5206,"anchor":"References"},{"toclevel":1,"level":"2","line":"External sources","number":"8","index":"8","fromtitle":"Drama_teaching_techniques","byteoffset":6269,"anchor":"External_sources"}],"parsewarnings":[],"displaytitle":"Drama teaching techniques","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q17011186"}]}}