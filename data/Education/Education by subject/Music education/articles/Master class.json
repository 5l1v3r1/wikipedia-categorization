"ns":828,"exists":"","*":"Module:Category handler/data"},{"ns":828,"exists":"","*":"Module:Category handler/config"},{"ns":828,"exists":"","*":"Module:Category handler/shared"},{"ns":828,"exists":"","*":"Module:Category handler/blacklist"},{"ns":828,"exists":"","*":"Module:Delink"},{"ns":828,"exists":"","*":"Module:No globals"},{"ns":828,"exists":"","*":"Module:Check isxn"},{"ns":828,"exists":"","*":"Module:Error"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"}],"images":[],"externallinks":["http://www.oxfordmusiconline.com/subscriber/article/opr/t114/e4274","http://www.oxfordmusiconline.com/subscriber/article/grove/music/48265","https://archive.org/details/baltimorelecture00kelviala","http://www.conservatoire.strasbourg.eu/categories/master-classes/","http://www.unistra.fr/uploads/media/masterclass-philo-2014-web_01.pdf","http://labex-irmia.u-strasbg.fr/spip.php?rubrique4"],"sections":[{"toclevel":1,"level":"2","line":"Around music","number":"1","index":"1","fromtitle":"Master_class","byteoffset":457,"anchor":"Around_music"},{"toclevel":1,"level":"2","line":"Kelvin in Baltimore","number":"2","index":"2","fromtitle":"Master_class","byteoffset":2928,"anchor":"Kelvin_in_Baltimore"},{"toclevel":1,"level":"2","line":"Strasbourg","number":"3","index":"3","fromtitle":"Master_class","byteoffset":5038,"anchor":"Strasbourg"},{"toclevel":1,"level":"2","line":"See also","number":"4","index":"4","fromtitle":"Master_class","byteoffset":5638,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"5","index":"5","fromtitle":"Master_class","byteoffset":5788,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Master class","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q1281422"}]}}