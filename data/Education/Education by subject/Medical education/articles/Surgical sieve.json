tive endocarditis"},{"ns":0,"exists":"","*":"Inflammation"},{"ns":0,"exists":"","*":"Intracranial hemorrhage"},{"ns":0,"exists":"","*":"Malaria"},{"ns":0,"exists":"","*":"Medical students"},{"ns":0,"exists":"","*":"Medicine"},{"ns":0,"exists":"","*":"Mental confusion"},{"ns":0,"exists":"","*":"Metabolism"},{"ns":0,"exists":"","*":"Metastases"},{"ns":0,"exists":"","*":"Mononucleosis"},{"ns":0,"exists":"","*":"Mucopolysaccharidoses"},{"ns":0,"exists":"","*":"Myeloproliferative disorder"},{"ns":0,"exists":"","*":"Neoplasm"},{"ns":0,"exists":"","*":"Neurological"},{"ns":0,"exists":"","*":"Paternity (House)"},{"ns":0,"exists":"","*":"Pathology"},{"ns":0,"exists":"","*":"Patients"},{"ns":0,"exists":"","*":"Physical trauma"},{"ns":0,"exists":"","*":"Physician"},{"ns":0,"exists":"","*":"Polypharmacy"},{"ns":0,"exists":"","*":"Portal vein obstruction"},{"ns":0,"exists":"","*":"Rheumatoid arthritis"},{"ns":0,"exists":"","*":"Sarcoidosis"},{"ns":0,"exists":"","*":"Sepsis"},{"ns":0,"exists":"","*":"Septicaemia"},{"ns":0,"exists":"","*":"Shock (circulatory)"},{"ns":0,"exists":"","*":"Sickle-cell disease"},{"ns":0,"exists":"","*":"Splenomegaly"},{"ns":0,"exists":"","*":"Stroke"},{"ns":0,"exists":"","*":"Syndrome of inappropriate antidiuretic hormone"},{"ns":0,"exists":"","*":"Systemic lupus erythematosus"},{"ns":0,"exists":"","*":"Tangier disease"},{"ns":0,"exists":"","*":"Thalassemia"},{"ns":0,"exists":"","*":"Transient ischemic attack"},{"ns":0,"exists":"","*":"Tuberculosis"},{"ns":0,"exists":"","*":"Vascular dementia"},{"ns":0,"exists":"","*":"Vascular disease"},{"ns":0,"exists":"","*":"Wayback Machine"}],"templates":[{"ns":10,"exists":"","*":"Template:Reflist"},{"ns":10,"exists":"","*":"Template:Webarchive"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":828,"exists":"","*":"Module:Webarchive"},{"ns":828,"exists":"","*":"Module:No globals"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:Webarchive/data"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"}],"images":[],"externallinks":["http://student.bmj.com/back_issues/0602/education/ed6.html","http://www.thinkingmedicine.com","http://www.fox.com/house/","https://web.archive.org/web/20120523030504/http://www.fox.com/house/"],"sections":[{"toclevel":1,"level":"2","line":"Overview","number":"1","index":"1","fromtitle":"Surgical_sieve","byteoffset":1068,"anchor":"Overview"},{"toclevel":1,"level":"2","line":"Examples","number":"2","index":"2","fromtitle":"Surgical_sieve","byteoffset":2889,"anchor":"Examples"},{"toclevel":1,"level":"2","line":"Pathologic Basis Of Disease","number":"3","index":"3","fromtitle":"Surgical_sieve","byteoffset":4361,"anchor":"Pathologic_Basis_Of_Disease"},{"toclevel":1,"level":"2","line":"In popular culture","number":"4","index":"4","fromtitle":"Surgical_sieve","byteoffset":4962,"anchor":"In_popular_culture"},{"toclevel":1,"level":"2","line":"References","number":"5","index":"5","fromtitle":"Surgical_sieve","byteoffset":5688,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Surgical sieve","iwlinks":[{"prefix":"wikt","url":"https://en.wiktionary.org/wiki/rupture","*":"wikt:rupture"}],"properties":[{"name":"defaultsort","*":"Surgical Sieve"},{"name":"wikibase_item","*":"Q7646221"}]}}