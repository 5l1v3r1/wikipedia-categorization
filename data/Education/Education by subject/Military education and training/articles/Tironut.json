n_basic_training_1954_(\u05d8\u05d9\u05e8\u05d5\u05e0\u05d9\u05d5\u05ea_\u05d1\u05d1\u05d4).jpg"],"externallinks":["http://www.ynetnews.com/articles/0,7340,L-4261231,00.html","http://www.iaf.org.il/4376-38302-he/IAF.aspx"],"sections":[{"toclevel":1,"level":"2","line":"Course of the training","number":"1","index":"1","fromtitle":"Tironut","byteoffset":1537,"anchor":"Course_of_the_training"},{"toclevel":1,"level":"2","line":"Training bases","number":"2","index":"2","fromtitle":"Tironut","byteoffset":3656,"anchor":"Training_bases"},{"toclevel":1,"level":"2","line":"Certification requirements","number":"3","index":"3","fromtitle":"Tironut","byteoffset":4449,"anchor":"Certification_requirements"},{"toclevel":2,"level":"3","line":"Rifleman 02","number":"3.1","index":"4","fromtitle":"Tironut","byteoffset":5135,"anchor":"Rifleman_02"},{"toclevel":2,"level":"3","line":"Rifleman 02 Cadets/ Extended","number":"3.2","index":"5","fromtitle":"Tironut","byteoffset":5550,"anchor":"Rifleman_02_Cadets/_Extended"},{"toclevel":2,"level":"3","line":"Rifleman 03","number":"3.3","index":"6","fromtitle":"Tironut","byteoffset":5945,"anchor":"Rifleman_03"},{"toclevel":1,"level":"2","line":"References","number":"4","index":"7","fromtitle":"Tironut","byteoffset":6528,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Tironut","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q187229"}]}}