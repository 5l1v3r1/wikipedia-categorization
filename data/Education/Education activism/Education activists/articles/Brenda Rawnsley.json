ditAtWikidata"}],"images":["Text_document_with_red_question_mark.svg","Blue_pencil.svg"],"externallinks":["http://www.fineart.co.uk/Trade/About_Us/The_Master_of_the_Guild.aspx","https://www.wikidata.org/wiki/Q4960754","https://web.archive.org/web/20071001030623/http://news.independent.co.uk/people/obituaries/article2737101.ece","http://www.timesonline.co.uk/tol/comment/obituaries/article2022629.ece","http://www.merivaleeditions.com/schoolprints/","https://archive.is/20130421063924/http://www.schoolprints.co.uk/School","http://www.bbc.co.uk/radio4/womanshour/05/2007_07_wed.shtml","https://www.worldcat.org/identities/containsVIAFID/61492078","https://viaf.org/viaf/61492078"],"sections":[{"toclevel":1,"level":"2","line":"Biography","number":"1","index":"1","fromtitle":"Brenda_Rawnsley","byteoffset":348,"anchor":"Biography"},{"toclevel":2,"level":"3","line":"World War II","number":"1.1","index":"2","fromtitle":"Brenda_Rawnsley","byteoffset":743,"anchor":"World_War_II"},{"toclevel":2,"level":"3","line":"Picture Hire Ltd","number":"1.2","index":"3","fromtitle":"Brenda_Rawnsley","byteoffset":2404,"anchor":"Picture_Hire_Ltd"},{"toclevel":2,"level":"3","line":"Later life and death","number":"1.3","index":"4","fromtitle":"Brenda_Rawnsley","byteoffset":3549,"anchor":"Later_life_and_death"},{"toclevel":1,"level":"2","line":"References","number":"2","index":"5","fromtitle":"Brenda_Rawnsley","byteoffset":4364,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"3","index":"6","fromtitle":"Brenda_Rawnsley","byteoffset":4392,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"Brenda Rawnsley","iwlinks":[],"properties":[{"name":"defaultsort","*":"Rawnsley, Brenda"},{"name":"wikibase_item","*":"Q4960754"}]}}