":"1","fromtitle":"David_A._Kronick","byteoffset":687,"anchor":"Education"},{"toclevel":1,"level":"2","line":"Career","number":"2","index":"2","fromtitle":"David_A._Kronick","byteoffset":1449,"anchor":"Career"},{"toclevel":1,"level":"2","line":"Legacy","number":"3","index":"3","fromtitle":"David_A._Kronick","byteoffset":2790,"anchor":"Legacy"},{"toclevel":1,"level":"2","line":"Selected bibliography","number":"4","index":"4","fromtitle":"David_A._Kronick","byteoffset":3181,"anchor":"Selected_bibliography"},{"toclevel":1,"level":"2","line":"Awards","number":"5","index":"5","fromtitle":"David_A._Kronick","byteoffset":3548,"anchor":"Awards"},{"toclevel":1,"level":"2","line":"References","number":"6","index":"6","fromtitle":"David_A._Kronick","byteoffset":4806,"anchor":"References"}],"parsewarnings":[],"displaytitle":"David A. Kronick","iwlinks":[],"properties":[{"name":"defaultsort","*":"Kronick, David A."},{"name":"wikibase_item","*":"Q23035368"}]}}