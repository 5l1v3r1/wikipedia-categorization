"Template:Reflist"},{"ns":10,"exists":"","*":"Template:Cite book"},{"ns":10,"exists":"","*":"Template:Webarchive"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":10,"exists":"","*":"Template:Commons category"},{"ns":10,"exists":"","*":"Template:Commons"},{"ns":10,"exists":"","*":"Template:Sister project"},{"ns":10,"exists":"","*":"Template:Side box"},{"ns":10,"exists":"","*":"Template:Coord"},{"ns":828,"exists":"","*":"Module:Citation/CS1"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Configuration"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Whitelist"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Utilities"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Date validation"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Identifiers"},{"ns":828,"exists":"","*":"Module:Citation/CS1/COinS"},{"ns":828,"exists":"","*":"Module:Webarchive"},{"ns":828,"exists":"","*":"Module:No globals"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:Webarchive/data"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"},{"ns":828,"exists":"","*":"Module:Side box"},{"ns":828,"exists":"","*":"Module:Yesno"},{"ns":828,"exists":"","*":"Module:Coordinates"},{"ns":828,"exists":"","*":"Module:Math"}],"images":["Dessau,_Palais_Dietrich,_Wissenschaftliche_Bibliothek.jpg","Commons-logo.svg"],"externallinks":["http://www.haraldfischerverlag.de/hfv/dessau_philanthropinum_engl.php","https://web.archive.org/web/20110719034917/http://www.haraldfischerverlag.de/hfv/dessau_philanthropinum_engl.php","https://archive.org/details/educationaltheor00kant","https://archive.org/details/essaysoneducatio00uic","https://books.google.com/books?id=Sp2k_whzdbsC&printsec=frontcover#v=onepage&q&f=false","https://archive.org/details/readingsinhisto02cubbgoog","http://www.dessauweb.de/philan/portal/index.php","//tools.wmflabs.org/geohack/geohack.php?pagename=Philanthropinum&params=51_50_17_N_12_14_44_E_region:DE-ST_type:landmark_source:kolossus-dewiki"],"sections":[{"toclevel":1,"level":"2","line":"History","number":"1","index":"1","fromtitle":"Philanthropinum","byteoffset":461,"anchor":"History"},{"toclevel":1,"level":"2","line":"Curriculum and philosophy","number":"2","index":"2","fromtitle":"Philanthropinum","byteoffset":4224,"anchor":"Curriculum_and_philosophy"},{"toclevel":1,"level":"2","line":"References","number":"3","index":"3","fromtitle":"Philanthropinum","byteoffset":4984,"anchor":"References"},{"toclevel":1,"level":"2","line":"Bibliography","number":"4","index":"4","fromtitle":"Philanthropinum","byteoffset":5132,"anchor":"Bibliography"},{"toclevel":1,"level":"2","line":"External links","number":"5","index":"5","fromtitle":"Philanthropinum","byteoffset":5939,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"Philanthropinum","iwlinks":[{"prefix":"de","url":"https://de.wikipedia.org/wiki/Philanthropinum_Dessau","*":"de:Philanthropinum Dessau"},{"prefix":"commons","url":"https://commons.wikimedia.org/wiki/Category:Philanthropinum_(Dessau)","*":"commons:Category:Philanthropinum (Dessau)"}],"properties":[{"name":"defaultsort","*":"Philanthropinum"},{"name":"wikibase_item","*":"Q2085857"}]}}