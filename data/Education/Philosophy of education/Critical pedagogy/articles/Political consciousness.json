ce detect/config"},{"ns":828,"exists":"","*":"Module:Main"},{"ns":828,"exists":"","*":"Module:Hatnote"},{"ns":828,"exists":"","*":"Module:Hatnote list"},{"ns":828,"exists":"","*":"Module:Citation/CS1"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Configuration"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Whitelist"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Utilities"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Date validation"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Identifiers"},{"ns":828,"exists":"","*":"Module:Citation/CS1/COinS"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"},{"ns":828,"exists":"","*":"Module:Check isxn"},{"ns":828,"exists":"","*":"Module:Error"}],"images":["Question_book-new.svg"],"externallinks":["http://www.marxists.org/glossary/terms/c/o.htm","http://www.marxists.org/archive/marx/works/1845/german-ideology/ch01a.htm","http://www.marxists.org/archive/marx/works/1848/communist-manifesto/ch02.htm","http://www.victorianweb.org/philosophy/phil2.html","http://www.marxists.org/reference/archive/hegel/works/ol/ol_phen.htm"],"sections":[{"toclevel":1,"level":"2","line":"The politics of consciousness","number":"1","index":"1","fromtitle":"Political_consciousness","byteoffset":160,"anchor":"The_politics_of_consciousness"},{"toclevel":1,"level":"2","line":"False consciousness","number":"2","index":"2","fromtitle":"Political_consciousness","byteoffset":1160,"anchor":"False_consciousness"},{"toclevel":1,"level":"2","line":"Consciousness and the political-economy","number":"3","index":"3","fromtitle":"Political_consciousness","byteoffset":2867,"anchor":"Consciousness_and_the_political-economy"},{"toclevel":1,"level":"2","line":"Consciousness and social movements","number":"4","index":"4","fromtitle":"Political_consciousness","byteoffset":3902,"anchor":"Consciousness_and_social_movements"},{"toclevel":1,"level":"2","line":"See also","number":"5","index":"5","fromtitle":"Political_consciousness","byteoffset":6170,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"Notes","number":"6","index":"6","fromtitle":"Political_consciousness","byteoffset":6424,"anchor":"Notes"},{"toclevel":1,"level":"2","line":"References","number":"7","index":"7","fromtitle":"Political_consciousness","byteoffset":6447,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"8","index":"8","fromtitle":"Political_consciousness","byteoffset":6541,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"Political consciousness","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q7210272"}]}}