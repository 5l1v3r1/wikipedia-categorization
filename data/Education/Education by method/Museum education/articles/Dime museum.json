e:Citation/CS1/COinS"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"}],"images":["Question_book-new.svg","NewFakeMuseumsMascot.jpg","RobinsonsMammothDimeMuseumAd.jpg"],"externallinks":["http://www.sitebits.com/2007/american_dime_museum.html","https://repositories.lib.utexas.edu/handle/2152/ETD-UT-2009-08-279"],"sections":[{"toclevel":1,"level":"2","line":"Baltimore","number":"1","index":"1","fromtitle":"Dime_museum","byteoffset":990,"anchor":"Baltimore"},{"toclevel":1,"level":"2","line":"Boston","number":"2","index":"2","fromtitle":"Dime_museum","byteoffset":1556,"anchor":"Boston"},{"toclevel":1,"level":"2","line":"Cincinnati","number":"3","index":"3","fromtitle":"Dime_museum","byteoffset":1864,"anchor":"Cincinnati"},{"toclevel":1,"level":"2","line":"New Orleans","number":"4","index":"4","fromtitle":"Dime_museum","byteoffset":2179,"anchor":"New_Orleans"},{"toclevel":1,"level":"2","line":"New York City","number":"5","index":"5","fromtitle":"Dime_museum","byteoffset":2725,"anchor":"New_York_City"},{"toclevel":1,"level":"2","line":"References","number":"6","index":"6","fromtitle":"Dime_museum","byteoffset":4285,"anchor":"References"},{"toclevel":1,"level":"2","line":"Further reading","number":"7","index":"7","fromtitle":"Dime_museum","byteoffset":4415,"anchor":"Further_reading"}],"parsewarnings":[],"displaytitle":"Dime museum","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q5277223"}]}}