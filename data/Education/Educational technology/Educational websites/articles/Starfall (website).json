ne":"History","number":"1","index":"1","fromtitle":"Starfall_(website)","byteoffset":2481,"anchor":"History"},{"toclevel":1,"level":"2","line":"Four reading levels","number":"2","index":"2","fromtitle":"Starfall_(website)","byteoffset":3376,"anchor":"Four_reading_levels"},{"toclevel":1,"level":"2","line":"References","number":"3","index":"3","fromtitle":"Starfall_(website)","byteoffset":4955,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"4","index":"4","fromtitle":"Starfall_(website)","byteoffset":4983,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"Starfall (website)","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q16951239"}]}}