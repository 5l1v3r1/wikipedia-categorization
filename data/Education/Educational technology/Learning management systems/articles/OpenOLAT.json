wards","number":"3","index":"13","fromtitle":"OpenOLAT","byteoffset":11298,"anchor":"Awards"},{"toclevel":1,"level":"2","line":"Technical specifications","number":"4","index":"14","fromtitle":"OpenOLAT","byteoffset":12454,"anchor":"Technical_specifications"},{"toclevel":1,"level":"2","line":"Literature","number":"5","index":"15","fromtitle":"OpenOLAT","byteoffset":13481,"anchor":"Literature"},{"toclevel":1,"level":"2","line":"Notes and references","number":"6","index":"16","fromtitle":"OpenOLAT","byteoffset":13842,"anchor":"Notes_and_references"},{"toclevel":1,"level":"2","line":"External links","number":"7","index":"17","fromtitle":"OpenOLAT","byteoffset":13885,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"OpenOLAT","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q1729662"}]}}