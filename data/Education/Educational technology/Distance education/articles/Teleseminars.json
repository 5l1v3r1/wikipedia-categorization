"},{"toclevel":1,"level":"2","line":"References","number":"3","index":"6","fromtitle":"Teleseminars","byteoffset":5225,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Teleseminars","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q55951070"}]}}