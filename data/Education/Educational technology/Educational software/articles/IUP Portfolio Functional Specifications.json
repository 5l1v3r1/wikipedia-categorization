"13","fromtitle":"IUP_Portfolio_Functional_Specifications","byteoffset":2958,"anchor":"Back-end"},{"toclevel":3,"level":"4","line":"Entities","number":"4.1.3","index":"14","fromtitle":"IUP_Portfolio_Functional_Specifications","byteoffset":3291,"anchor":"Entities"},{"toclevel":2,"level":"3","line":"Programmer's model","number":"4.2","index":"15","fromtitle":"IUP_Portfolio_Functional_Specifications","byteoffset":3681,"anchor":"Programmer's_model"},{"toclevel":3,"level":"4","line":"Entities","number":"4.2.1","index":"16","fromtitle":"IUP_Portfolio_Functional_Specifications","byteoffset":3709,"anchor":"Entities_2"}],"parsewarnings":[],"displaytitle":"IUP Portfolio Functional Specifications","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q17059131"}]}}