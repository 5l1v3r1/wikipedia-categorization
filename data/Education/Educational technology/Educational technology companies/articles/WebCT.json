,"level":"2","line":"Criticisms","number":"3","index":"3","fromtitle":"WebCT","byteoffset":4116,"anchor":"Criticisms"},{"toclevel":1,"level":"2","line":"See also","number":"4","index":"4","fromtitle":"WebCT","byteoffset":5760,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"5","index":"5","fromtitle":"WebCT","byteoffset":5807,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"6","index":"6","fromtitle":"WebCT","byteoffset":5835,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"WebCT","iwlinks":[{"prefix":"v","url":"https://en.wikiversity.org/wiki/Special:Search/WebCT","*":"v:Special:Search/WebCT"}],"properties":[{"name":"defaultsort","*":"Webct"},{"name":"wikibase_item","*":"Q3174811"}]}}