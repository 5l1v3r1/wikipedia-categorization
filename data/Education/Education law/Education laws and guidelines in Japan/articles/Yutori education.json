Yutori_education","byteoffset":821,"anchor":"History"},{"toclevel":1,"level":"2","line":"Criticism","number":"3","index":"3","fromtitle":"Yutori_education","byteoffset":4412,"anchor":"Criticism"},{"toclevel":1,"level":"2","line":"Responses","number":"4","index":"4","fromtitle":"Yutori_education","byteoffset":5220,"anchor":"Responses"},{"toclevel":1,"level":"2","line":"See also","number":"5","index":"5","fromtitle":"Yutori_education","byteoffset":6574,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"6","index":"6","fromtitle":"Yutori_education","byteoffset":6739,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"7","index":"7","fromtitle":"Yutori_education","byteoffset":7344,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"Yutori education","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q853232"}]}}