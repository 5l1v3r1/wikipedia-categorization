"ns":0,"exists":"","*":"College Board"},{"ns":0,"exists":"","*":"Conscription in Israel"},{"ns":0,"exists":"","*":"Education in Israel"},{"ns":0,"exists":"","*":"English language"},{"ns":0,"exists":"","*":"Hebrew grammar"},{"ns":0,"exists":"","*":"Hebrew language"},{"ns":0,"exists":"","*":"High school diploma"},{"ns":0,"exists":"","*":"History"},{"ns":0,"exists":"","*":"Islam"},{"ns":0,"exists":"","*":"Israel"},{"ns":0,"exists":"","*":"Jewish People"},{"ns":0,"exists":"","*":"Literature"},{"ns":0,"exists":"","*":"Mathematics"},{"ns":0,"exists":"","*":"Matriculation examination"},{"ns":0,"exists":"","*":"Ministry of Education (Israel)"},{"ns":0,"exists":"","*":"Minorities"},{"ns":0,"exists":"","*":"New York (state)"},{"ns":0,"exists":"","*":"Physical Education"},{"ns":0,"exists":"","*":"Psychometric Entrance Test"},{"ns":0,"exists":"","*":"Regents Examinations"},{"ns":0,"exists":"","*":"Scripture"},{"ns":0,"exists":"","*":"Tanakh"},{"ns":0,"exists":"","*":"World History"}],"templates":[{"ns":10,"exists":"","*":"Template:Lang-he-n"},{"ns":10,"exists":"","*":"Template:Script/Hebrew"},{"ns":10,"exists":"","*":"Template:Category handler"},{"ns":10,"exists":"","*":"Template:Reflist"},{"ns":10,"exists":"","*":"Template:Cite web"},{"ns":10,"exists":"","*":"Template:Cite news"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":828,"exists":"","*":"Module:Category handler"},{"ns":828,"exists":"","*":"Module:Yesno"},{"ns":828,"exists":"","*":"Module:Category handler/data"},{"ns":828,"exists":"","*":"Module:Category handler/config"},{"ns":828,"exists":"","*":"Module:Category handler/shared"},{"ns":828,"exists":"","*":"Module:Category handler/blacklist"},{"ns":828,"exists":"","*":"Module:Namespace detect/data"},{"ns":828,"exists":"","*":"Module:Namespace detect/config"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:Citation/CS1"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Configuration"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Whi