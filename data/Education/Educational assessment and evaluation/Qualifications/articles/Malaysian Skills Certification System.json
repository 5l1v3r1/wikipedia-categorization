"Malaysian_Skills_Certification_System","byteoffset":1596,"anchor":"Award_levels"},{"toclevel":2,"level":"3","line":"Delivery of certification","number":"2.2","index":"4","fromtitle":"Malaysian_Skills_Certification_System","byteoffset":3695,"anchor":"Delivery_of_certification"},{"toclevel":2,"level":"3","line":"Classifications","number":"2.3","index":"5","fromtitle":"Malaysian_Skills_Certification_System","byteoffset":4368,"anchor":"Classifications"},{"toclevel":1,"level":"2","line":"Academic equivalency","number":"3","index":"6","fromtitle":"Malaysian_Skills_Certification_System","byteoffset":5095,"anchor":"Academic_equivalency"},{"toclevel":1,"level":"2","line":"See also","number":"4","index":"7","fromtitle":"Malaysian_Skills_Certification_System","byteoffset":5758,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"5","index":"8","fromtitle":"Malaysian_Skills_Certification_System","byteoffset":5841,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"6","index":"9","fromtitle":"Malaysian_Skills_Certification_System","byteoffset":5893,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"Malaysian Skills Certification System","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q6742017"}]}}