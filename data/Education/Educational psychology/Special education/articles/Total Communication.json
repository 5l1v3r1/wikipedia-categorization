therapy.com/child-speech-language/children-special-needs-learning-difficulties/key-guidelines-creating-total-communication-environment/","http://integratedtreatmentservices.co.uk/our-approaches/speech-therapy-approaches/total-communication/","http://handsandvoices.org/","http://ericec.org/digests/e559.html","https://web.archive.org/web/20050310111234/http://ericec.org/digests/e559.html","https://www.hearinglikeme.com/total-communication/","http://www.ericdigests.org/1998-2/total.htm","https://web.archive.org/web/20051230010927/http://www.healthyhearing.com/library/interview_content.asp?interview_id=725"],"sections":[{"toclevel":1,"level":"2","line":"History","number":"1","index":"1","fromtitle":"Total_Communication","byteoffset":3727,"anchor":"History"},{"toclevel":1,"level":"2","line":"See also","number":"2","index":"2","fromtitle":"Total_Communication","byteoffset":6106,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"3","index":"3","fromtitle":"Total_Communication","byteoffset":6157,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"4","index":"4","fromtitle":"Total_Communication","byteoffset":6882,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"Total Communication","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q2650252"}]}}