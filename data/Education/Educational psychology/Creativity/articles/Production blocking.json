dler/blacklist"},{"ns":828,"exists":"","*":"Module:Namespace detect/data"},{"ns":828,"exists":"","*":"Module:Namespace detect/config"},{"ns":828,"exists":"","*":"Module:Citation/CS1"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Configuration"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Whitelist"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Utilities"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Date validation"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Identifiers"},{"ns":828,"exists":"","*":"Module:Citation/CS1/COinS"}],"images":["Question_book-new.svg"],"externallinks":["https://dx.doi.org/10.1207/s15324834basp1201_1","//doi.org/10.1207/s15324834basp1201_1","//www.worldcat.org/issn/0197-3533","//doi.org/10.1177/1046496496271005","http://psp.sagepub.com/content/11/4/457","//doi.org/10.1177/0146167285114011","//www.worldcat.org/issn/0146-1672","//doi.org/10.1016/s0022-1031(03)00040-4","//doi.org/10.1016/j.jesp.2010.03.007"],"sections":[{"toclevel":1,"level":"2","line":"Methods to decrease the production blocking problem","number":"1","index":"1","fromtitle":"Production_blocking","byteoffset":2924,"anchor":"Methods_to_decrease_the_production_blocking_problem"},{"toclevel":1,"level":"2","line":"References","number":"2","index":"2","fromtitle":"Production_blocking","byteoffset":6059,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Production blocking","iwlinks":[],"properties":[{"name":"defaultsort","*":"Production blocking"},{"name":"wikibase_item","*":"Q7247813"}]}}