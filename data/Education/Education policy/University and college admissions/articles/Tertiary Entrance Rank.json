nchor":"Equivalence"},{"toclevel":1,"level":"2","line":"Calculation of the TER in South Australia and the Northern Territory","number":"3","index":"3","fromtitle":"Tertiary_Entrance_Rank","byteoffset":1546,"anchor":"Calculation_of_the_TER_in_South_Australia_and_the_Northern_Territory"},{"toclevel":1,"level":"2","line":"Calculation of the TER in Tasmania","number":"4","index":"4","fromtitle":"Tertiary_Entrance_Rank","byteoffset":2828,"anchor":"Calculation_of_the_TER_in_Tasmania"},{"toclevel":1,"level":"2","line":"Calculation of the TER in Western Australia","number":"5","index":"5","fromtitle":"Tertiary_Entrance_Rank","byteoffset":3370,"anchor":"Calculation_of_the_TER_in_Western_Australia"},{"toclevel":1,"level":"2","line":"See also","number":"6","index":"6","fromtitle":"Tertiary_Entrance_Rank","byteoffset":4044,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"7","index":"7","fromtitle":"Tertiary_Entrance_Rank","byteoffset":4204,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Tertiary Entrance Rank","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q7705258"}]}}