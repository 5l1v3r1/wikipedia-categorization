":"Biography"},{"toclevel":1,"level":"2","line":"Research","number":"2","index":"2","fromtitle":"Eric_Bettinger","byteoffset":1806,"anchor":"Research"},{"toclevel":2,"level":"3","line":"Impact evaluations of educational interventions in primary and secondary school","number":"2.1","index":"3","fromtitle":"Eric_Bettinger","byteoffset":2434,"anchor":"Impact_evaluations_of_educational_interventions_in_primary_and_secondary_school"},{"toclevel":2,"level":"3","line":"Impact evaluations of educational interventions in higher education","number":"2.2","index":"4","fromtitle":"Eric_Bettinger","byteoffset":4451,"anchor":"Impact_evaluations_of_educational_interventions_in_higher_education"},{"toclevel":1,"level":"2","line":"References","number":"3","index":"5","fromtitle":"Eric_Bettinger","byteoffset":7384,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"4","index":"6","fromtitle":"Eric_Bettinger","byteoffset":7414,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"Eric Bettinger","iwlinks":[],"properties":[{"name":"defaultsort","*":"Bettinger, Eric"},{"name":"wikibase_item","*":"Q51337102"}]}}