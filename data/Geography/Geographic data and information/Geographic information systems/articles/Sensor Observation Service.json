ww.opengeospatial.org/projects/initiatives/imisiot","http://52north.org/communities/sensorweb/sos/","http://www.deegree.org/","http://www.oostethys.org/downloads","http://istsos.org/","http://www.opengeospatial.org/resource/products/byspec","http://www.opengeospatial.org/standards/sos"],"sections":[{"toclevel":1,"level":"2","line":"Operations","number":"1","index":"1","fromtitle":"Sensor_Observation_Service","byteoffset":1746,"anchor":"Operations"},{"toclevel":2,"level":"3","line":"Core operations (core profile)","number":"1.1","index":"2","fromtitle":"Sensor_Observation_Service","byteoffset":2262,"anchor":"Core_operations_(core_profile)"},{"toclevel":2,"level":"3","line":"Transactional operations (transactional profile)","number":"1.2","index":"3","fromtitle":"Sensor_Observation_Service","byteoffset":3093,"anchor":"Transactional_operations_(transactional_profile)"},{"toclevel":2,"level":"3","line":"Extended operations (enhanced profile)","number":"1.3","index":"4","fromtitle":"Sensor_Observation_Service","byteoffset":3323,"anchor":"Extended_operations_(enhanced_profile)"},{"toclevel":1,"level":"2","line":"Terminology","number":"2","index":"5","fromtitle":"Sensor_Observation_Service","byteoffset":4355,"anchor":"Terminology"},{"toclevel":1,"level":"2","line":"Software","number":"3","index":"6","fromtitle":"Sensor_Observation_Service","byteoffset":5928,"anchor":"Software"},{"toclevel":1,"level":"2","line":"References","number":"4","index":"7","fromtitle":"Sensor_Observation_Service","byteoffset":6896,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"5","index":"8","fromtitle":"Sensor_Observation_Service","byteoffset":6929,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"Sensor Observation Service","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q1518039"}]}}