"},{"toclevel":1,"level":"2","line":"References","number":"4","index":"4","fromtitle":"Web_Coverage_Processing_Service","byteoffset":4886,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"5","index":"5","fromtitle":"Web_Coverage_Processing_Service","byteoffset":4916,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"Web Coverage Processing Service","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q7978511"}]}}