Module:Namespace detect/data"},{"ns":828,"exists":"","*":"Module:Namespace detect/config"},{"ns":828,"exists":"","*":"Module:Citation/CS1"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Configuration"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Whitelist"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Utilities"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Date validation"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Identifiers"},{"ns":828,"exists":"","*":"Module:Citation/CS1/COinS"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"}],"images":["Question_book-new.svg"],"externallinks":["//doi.org/10.1111/1467-9876.00103","https://books.google.com/books?id=f2fghtp8lA0C","http://press.princeton.edu/titles/8336.html"],"sections":[{"toclevel":1,"level":"2","line":"Examples","number":"1","index":"1","fromtitle":"Overdispersion","byteoffset":1213,"anchor":"Examples"},{"toclevel":2,"level":"3","line":"Poisson","number":"1.1","index":"2","fromtitle":"Overdispersion","byteoffset":1226,"anchor":"Poisson"},{"toclevel":2,"level":"3","line":"Binomial","number":"1.2","index":"3","fromtitle":"Overdispersion","byteoffset":2259,"anchor":"Binomial"},{"toclevel":2,"level":"3","line":"Normal distribution","number":"1.3","index":"4","fromtitle":"Overdispersion","byteoffset":4435,"anchor":"Normal_distribution"},{"toclevel":1,"level":"2","line":"Differences in terminology among disciplines","number":"2","index":"5","fromtitle":"Overdispersion","byteoffset":6080,"anchor":"Differences_in_terminology_among_disciplines"},{"toclevel":1,"level":"2","line":"See also","number":"3","index":"6","fromtitle":"Overdispersion","byteoffset":7418,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"4","index":"7","fromtitle":"Overdispersion","byteoffset":7498,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"5","index":"8","fromtitle":"Overdispersion","byteoffset":7526,"anchor":"External_links"}],"parsewarnings":[],"displ