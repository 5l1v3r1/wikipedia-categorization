"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":10,"exists":"","*":"Template:Stats-stub"},{"ns":10,"exists":"","*":"Template:Statistics-stub"},{"ns":10,"exists":"","*":"Template:Asbox"},{"ns":828,"exists":"","*":"Module:Citation/CS1"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Configuration"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Whitelist"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Utilities"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Date validation"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Identifiers"},{"ns":828,"exists":"","*":"Module:Citation/CS1/COinS"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"},{"ns":828,"exists":"","*":"Module:Asbox"},{"ns":828,"exists":"","*":"Module:Buffer"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:Navbar"}],"images":["Bellcurve.svg"],"externallinks":["//doi.org/10.2307/2986645","//www.jstor.org/stable/2986645"],"sections":[{"toclevel":1,"level":"2","line":"Sources","number":"1","index":"1","fromtitle":"Geary's_C","byteoffset":2011,"anchor":"Sources"}],"parsewarnings":[],"displaytitle":"Geary's <i>C</i>","iwlinks":[],"properties":[{"name":"displaytitle","*":"Geary's <i>C</i>"},{"name":"wikibase_item","*":"Q3150394"}]}}