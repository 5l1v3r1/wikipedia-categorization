ndex":"1","fromtitle":"GPS_navigation_software","byteoffset":618,"anchor":"Terminology"},{"toclevel":2,"level":"3","line":"Track","number":"1.1","index":"2","fromtitle":"GPS_navigation_software","byteoffset":637,"anchor":"Track"},{"toclevel":2,"level":"3","line":"Route","number":"1.2","index":"3","fromtitle":"GPS_navigation_software","byteoffset":1276,"anchor":"Route"},{"toclevel":2,"level":"3","line":"Waypoint","number":"1.3","index":"4","fromtitle":"GPS_navigation_software","byteoffset":1464,"anchor":"Waypoint"},{"toclevel":1,"level":"2","line":"Platforms","number":"2","index":"5","fromtitle":"GPS_navigation_software","byteoffset":2073,"anchor":"Platforms"},{"toclevel":1,"level":"2","line":"Software products","number":"3","index":"6","fromtitle":"GPS_navigation_software","byteoffset":2402,"anchor":"Software_products"},{"toclevel":2,"level":"3","line":"Land-based navigation software","number":"3.1","index":"7","fromtitle":"GPS_navigation_software","byteoffset":2578,"anchor":"Land-based_navigation_software"},{"toclevel":2,"level":"3","line":"Marine navigation software","number":"3.2","index":"8","fromtitle":"GPS_navigation_software","byteoffset":3783,"anchor":"Marine_navigation_software"},{"toclevel":2,"level":"3","line":"Aeronautical navigation software","number":"3.3","index":"9","fromtitle":"GPS_navigation_software","byteoffset":4514,"anchor":"Aeronautical_navigation_software"},{"toclevel":1,"level":"2","line":"See also","number":"4","index":"10","fromtitle":"GPS_navigation_software","byteoffset":4807,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"5","index":"11","fromtitle":"GPS_navigation_software","byteoffset":4985,"anchor":"References"}],"parsewarnings":[],"displaytitle":"GPS navigation software","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q5514340"}]}}