cal and timestamp 20180901133800 and revision id 847520285\n -->\n</div>"},"langlinks":[],"categories":[{"sortkey":"","*":"Geographic_information_systems"}],"links":[{"ns":0,"exists":"","*":"Ambiguity"},{"ns":0,"exists":"","*":"Database"},{"ns":0,"exists":"","*":"Digital object identifier"},{"ns":0,"exists":"","*":"Gazetteer"},{"ns":0,"exists":"","*":"GeoNames"},{"ns":0,"exists":"","*":"Geocoding"},{"ns":0,"exists":"","*":"Geographic Information Systems"},{"ns":0,"exists":"","*":"Geoparsing"},{"ns":0,"exists":"","*":"Geotagging"},{"ns":0,"exists":"","*":"Information extraction"},{"ns":0,"exists":"","*":"International Standard Book Number"},{"ns":0,"exists":"","*":"Latitude"},{"ns":0,"exists":"","*":"Longitude"},{"ns":0,"exists":"","*":"Metonymy"},{"ns":0,"exists":"","*":"Named-entity recognition"},{"ns":0,"exists":"","*":"Natural Language Processing"},{"ns":0,"exists":"","*":"Ontario"},{"ns":0,"exists":"","*":"OpenStreetMap"},{"ns":0,"exists":"","*":"Set cover problem"},{"ns":0,"exists":"","*":"Supervised learning"},{"ns":0,"exists":"","*":"Toponym"},{"ns":0,"exists":"","*":"Unsupervised learning"}],"templates":[{"ns":10,"exists":"","*":"Template:Quote"},{"ns":10,"exists":"","*":"Template:Quote/styles.css"},{"ns":10,"exists":"","*":"Template:Comma separated entries"},{"ns":10,"exists":"","*":"Template:If empty"},{"ns":10,"exists":"","*":"Template:Reflist"},{"ns":10,"exists":"","*":"Template:Cite thesis"},{"ns":10,"exists":"","*":"Template:Cite book"},{"ns":10,"exists":"","*":"Template:Cite conference"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":828,"exists":"","*":"Module:Separated entries"},{"ns":828,"exists":"","*":"Module:TableTools"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:If empty"},{"ns":828,"exists":"","*":"Module:Citation/CS1"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Configuration"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Whitelist"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Utilities"},{"