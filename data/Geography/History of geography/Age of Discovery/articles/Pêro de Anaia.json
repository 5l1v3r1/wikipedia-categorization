":"","*":"Module:Main"},{"ns":828,"exists":"","*":"Module:Hatnote"},{"ns":828,"exists":"","*":"Module:Hatnote list"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"},{"ns":828,"exists":"","*":"Module:MultiReplace"}],"images":["City_of_Sofala,_1572.jpg"],"externallinks":[],"sections":[{"toclevel":1,"level":"2","line":"Background","number":"1","index":"1","fromtitle":"P\u00earo_de_Anaia","byteoffset":431,"anchor":"Background"},{"toclevel":1,"level":"2","line":"Expedition to Sofala","number":"2","index":"2","fromtitle":"P\u00earo_de_Anaia","byteoffset":910,"anchor":"Expedition_to_Sofala"},{"toclevel":1,"level":"2","line":"Captain of Sofala","number":"3","index":"3","fromtitle":"P\u00earo_de_Anaia","byteoffset":2109,"anchor":"Captain_of_Sofala"},{"toclevel":1,"level":"2","line":"Aftermath","number":"4","index":"4","fromtitle":"P\u00earo_de_Anaia","byteoffset":5170,"anchor":"Aftermath"},{"toclevel":1,"level":"2","line":"See also","number":"5","index":"5","fromtitle":"P\u00earo_de_Anaia","byteoffset":6509,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"6","index":"6","fromtitle":"P\u00earo_de_Anaia","byteoffset":6630,"anchor":"References"},{"toclevel":1,"level":"2","line":"Sources","number":"7","index":"7","fromtitle":"P\u00earo_de_Anaia","byteoffset":6660,"anchor":"Sources"}],"parsewarnings":[],"displaytitle":"P\u00earo de Anaia","iwlinks":[],"properties":[{"name":"defaultsort","*":"Pero De Anaia"},{"name":"wikibase_item","*":"Q7264749"}]}}