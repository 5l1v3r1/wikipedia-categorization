":1,"level":"2","line":"See also","number":"4","index":"4","fromtitle":"NavPix","byteoffset":1954,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"External links","number":"5","index":"5","fromtitle":"NavPix","byteoffset":2042,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"NavPix","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q17125862"}]}}