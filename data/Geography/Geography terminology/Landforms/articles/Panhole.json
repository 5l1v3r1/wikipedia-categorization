10,"exists":"","*":"Template:Catalog lookup link"},{"ns":10,"exists":"","*":"Template:Trim"},{"ns":10,"exists":"","*":"Template:Yesno-no"},{"ns":10,"exists":"","*":"Template:Yesno"},{"ns":10,"exists":"","*":"Template:Error-small"},{"ns":10,"exists":"","*":"Template:Tl"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":828,"exists":"","*":"Module:Other uses"},{"ns":828,"exists":"","*":"Module:Hatnote"},{"ns":828,"exists":"","*":"Module:Hatnote list"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:TableTools"},{"ns":828,"exists":"","*":"Module:Convert"},{"ns":828,"exists":"","*":"Module:Convert/data"},{"ns":828,"exists":"","*":"Module:Convert/text"},{"ns":828,"exists":"","*":"Module:Check isxn"},{"ns":828,"exists":"","*":"Module:Error"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"}],"images":["Pothole,_a_pothole_from_Capital_Reef_National_Park_Spring_2013.jpg","Weathering_pits_-_granitic_rock_-_Glacier_Point,_Yosemite_National_Park,_California.jpg"],"externallinks":["http://journals.openedition.org/geomorphologie/11880","http://ir.lib.uwo.ca/etd/543","https://link.springer.com/article/10.1007/s12225-008-9092-0","http://geology.utah.gov/surveynotes/gladasked/gladpotholes.htm"],"sections":[{"toclevel":1,"level":"2","line":"Description of panholes","number":"1","index":"1","fromtitle":"Panhole","byteoffset":2427,"anchor":"Description_of_panholes"},{"toclevel":1,"level":"2","line":"Origin of panholes","number":"2","index":"2","fromtitle":"Panhole","byteoffset":4713,"anchor":"Origin_of_panholes"},{"toclevel":1,"level":"2","line":"References","number":"3","index":"3","fromtitle":"Panhole","byteoffset":6256,"anchor":"References"},{"toclevel":1,"level":"2","line":"Locations where panholes may be seen","number":"4","index":"4","fromtitle":"Panhole","byteoffset":6284,"anchor":"Locations_where_panholes_may_be_seen"},{"toclevel":1,"level":"2","line":"See also","number":"5","index":"5","fromtitle":"Panhole","byteoffset":6488