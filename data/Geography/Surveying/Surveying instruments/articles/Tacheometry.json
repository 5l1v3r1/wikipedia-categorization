"Template:Reflist"},{"ns":828,"exists":"","*":"Module:IPAc-en"},{"ns":828,"exists":"","*":"Module:IPAc-en/data"},{"ns":828,"exists":"","*":"Module:IPAc-en/pronunciation"},{"ns":828,"exists":"","*":"Module:IPAc-en/phonemes"},{"ns":828,"exists":"","*":"Module:Category handler"},{"ns":828,"exists":"","*":"Module:Yesno"},{"ns":828,"exists":"","*":"Module:Category handler/data"},{"ns":828,"exists":"","*":"Module:Category handler/config"},{"ns":828,"exists":"","*":"Module:Category handler/shared"},{"ns":828,"exists":"","*":"Module:Category handler/blacklist"},{"ns":828,"exists":"","*":"Module:Namespace detect/data"},{"ns":828,"exists":"","*":"Module:Namespace detect/config"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:Citation/CS1"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Configuration"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Whitelist"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Utilities"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Date validation"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Identifiers"},{"ns":828,"exists":"","*":"Module:Citation/CS1/COinS"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"}],"images":["Subtense_Bar.jpg","Wikisource-logo.svg"],"externallinks":[],"sections":[{"toclevel":1,"level":"2","line":"Tacheometer","number":"1","index":"1","fromtitle":"Tacheometry","byteoffset":2269,"anchor":"Tacheometer"},{"toclevel":1,"level":"2","line":"Stadia measurements","number":"2","index":"2","fromtitle":"Tacheometry","byteoffset":2526,"anchor":"Stadia_measurements"},{"toclevel":1,"level":"2","line":"Subtense bars","number":"3","index":"3","fromtitle":"Tacheometry","byteoffset":3643,"anchor":"Subtense_bars"},{"toclevel":1,"level":"2","line":"References","number":"4","index":"4","fromtitle":"Tacheometry","byteoffset":4588,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Tacheometry","iwlinks":[{"prefix":"wikisource","url":"https://en.wikisource.org/wiki/1911_Encyclop%C3%