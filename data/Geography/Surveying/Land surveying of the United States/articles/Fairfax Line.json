":"","*":"Module:Math"},{"ns":828,"exists":"","*":"Module:Arguments"}],"images":["FairfaxLine.jpg","JeffFryDetail.jpg"],"externallinks":["http://www.surveyhistory.org/the_fairfax_line1.htm","//tools.wmflabs.org/geohack/geohack.php?pagename=Fairfax_Line&params=39.000_N_79.000_W_"],"sections":[{"toclevel":1,"level":"2","line":"History","number":"1","index":"1","fromtitle":"Fairfax_Line","byteoffset":1004,"anchor":"History"},{"toclevel":2,"level":"3","line":"Background","number":"1.1","index":"2","fromtitle":"Fairfax_Line","byteoffset":1163,"anchor":"Background"},{"toclevel":2,"level":"3","line":"Disputes","number":"1.2","index":"3","fromtitle":"Fairfax_Line","byteoffset":2150,"anchor":"Disputes"},{"toclevel":2,"level":"3","line":"Surveys","number":"1.3","index":"4","fromtitle":"Fairfax_Line","byteoffset":3344,"anchor":"Surveys"},{"toclevel":1,"level":"2","line":"Geography","number":"2","index":"5","fromtitle":"Fairfax_Line","byteoffset":4203,"anchor":"Geography"},{"toclevel":1,"level":"2","line":"Notes","number":"3","index":"6","fromtitle":"Fairfax_Line","byteoffset":5927,"anchor":"Notes"},{"toclevel":1,"level":"2","line":"References","number":"4","index":"7","fromtitle":"Fairfax_Line","byteoffset":5950,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"5","index":"8","fromtitle":"Fairfax_Line","byteoffset":6125,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"Fairfax Line","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q5430185"}]}}