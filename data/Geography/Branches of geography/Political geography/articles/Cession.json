isdiction"},{"ns":0,"exists":"","*":"Kowloon"},{"ns":0,"exists":"","*":"Land borders"},{"ns":0,"exists":"","*":"Law"},{"ns":0,"exists":"","*":"Louisiana"},{"ns":0,"exists":"","*":"Louisiana (New Spain)"},{"ns":0,"exists":"","*":"Louisiana Purchase"},{"ns":0,"exists":"","*":"Maryland"},{"ns":0,"exists":"","*":"Parson"},{"ns":0,"exists":"","*":"Property"},{"ns":0,"exists":"","*":"Qing dynasty"},{"ns":0,"exists":"","*":"Rector (ecclesiastical)"},{"ns":0,"exists":"","*":"Retrocession (District of Columbia)"},{"ns":0,"exists":"","*":"Secession"},{"ns":0,"exists":"","*":"Second Opium War"},{"ns":0,"exists":"","*":"Session (disambiguation)"},{"ns":0,"exists":"","*":"Taiwan"},{"ns":0,"exists":"","*":"Territory (country subdivision)"},{"ns":0,"exists":"","*":"Treaty"},{"ns":0,"exists":"","*":"Treaty of Nanking"},{"ns":0,"exists":"","*":"United Kingdom"},{"ns":0,"exists":"","*":"United States"},{"ns":0,"exists":"","*":"United States Constitution"},{"ns":0,"exists":"","*":"Virginia"},{"ns":0,"exists":"","*":"West Florida"},{"ns":4,"exists":"","*":"Wikipedia:Stub"},{"ns":11,"exists":"","*":"Template talk:Law-term-stub"}],"templates":[{"ns":10,"exists":"","*":"Template:Distinguish"},{"ns":10,"exists":"","*":"Template:Ndash"},{"ns":10,"exists":"","*":"Template:En dash"},{"ns":10,"exists":"","*":"Template:Wiktionary"},{"ns":10,"exists":"","*":"Template:Sister project"},{"ns":10,"exists":"","*":"Template:Side box"},{"ns":10,"exists":"","*":"Template:Law-term-stub"},{"ns":10,"exists":"","*":"Template:Asbox"},{"ns":828,"exists":"","*":"Module:Distinguish"},{"ns":828,"exists":"","*":"Module:Hatnote"},{"ns":828,"exists":"","*":"Module:Hatnote list"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:TableTools"},{"ns":828,"exists":"","*":"Module:Side box"},{"ns":828,"exists":"","*":"Module:Yesno"},{"ns":828,"exists":"","*":"Module:Asbox"},{"ns":828,"exists":"","*":"Module:Buffer"},{"ns":828,"exists":"","*":"Module:Navbar"}],"images":["Wiktionary-logo-en-v2.svg","Scale_of_justice_2.svg"],"externallinks":[],"sections":[{"toclevel":1,"level":"2","line":"Examples","number":"1","index":"1","fromtitle":"Cession","byteoffset":541,"anchor":"Examples"},{"toclevel":1,"level":"2","line":"Specific areas of law","number":"2","index":"2","fromtitle":"Cession","byteoffset":1404,"anchor":"Specific_areas_of_law"},{"toclevel":2,"level":"3","line":"Contract law","number":"2.1","index":"3","fromtitle":"Cession","byteoffset":1430,"anchor":"Contract_law"},{"toclevel":2,"level":"3","line":"Civil law","number":"2.2","index":"4","fromtitle":"Cession","byteoffset":1897,"anchor":"Civil_law"},{"toclevel":2,"level":"3","line":"Ecclesiastical law","number":"2.3","index":"5","fromtitle":"Cession","byteoffset":2468,"anchor":"Ecclesiastical_law"},{"toclevel":1,"level":"2","line":"Retrocession","number":"3","index":"6","fromtitle":"Cession","byteoffset":2708,"anchor":"Retrocession"},{"toclevel":1,"level":"2","line":"See also","number":"4","index":"7","fromtitle":"Cession","byteoffset":3407,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"5","index":"8","fromtitle":"Cession","byteoffset":3509,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Cession","iwlinks":[{"prefix":"wiktionary","url":"https://en.wiktionary.org/wiki/retrocession","*":"wiktionary:retrocession"}],"properties":[{"name":"wikibase_item","*":"Q1143371"}]}}