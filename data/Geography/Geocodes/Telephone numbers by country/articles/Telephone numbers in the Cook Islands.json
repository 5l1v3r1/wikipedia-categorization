td></tr>\n<tr>\n<td>Mauke</td>\n<td>682</td>\n<td>35 XXX</td>\n<td></td>\n<td></td>\n<td>\n</td></tr>\n<tr>\n<td>Mitiaro</td>\n<td>682</td>\n<td>36 XXX</td>\n<td></td>\n<td></td>\n<td>\n</td></tr>\n<tr>\n<td>Palmerston</td>\n<td>682</td>\n<td>37 XXX</td>\n<td></td>\n<td></td>\n<td>\n</td></tr>\n<tr>\n<td>Pukapuka</td>\n<td>682</td>\n<td>41 XXX</td>\n<td></td>\n<td></td>\n<td>\n</td></tr>\n<tr>\n<td>Penrhyn</td>\n<td>682</td>\n<td>42 XXX</td>\n<td></td>\n<td></td>\n<td>\n</td></tr>\n<tr>\n<td>Manihiki</td>\n<td>682</td>\n<td>43 XXX</td>\n<td></td>\n<td></td>\n<td>\n</td></tr>\n<tr>\n<td>Rakahanga</td>\n<td>682</td>\n<td>44 XXX</td>\n<td></td>\n<td></td>\n<td>\n</td></tr>\n<tr>\n<td>Nassau</td>\n<td>682</td>\n<td>45 XXX</td>\n<td></td>\n<td></td>\n<td>\n</td></tr>\n<tr>\n<td>mobile</td>\n<td>682</td>\n<td>50 XXX</td>\n<td></td>\n<td></td>\n<td>\n</td></tr>\n<tr>\n<td>mobile</td>\n<td>682</td>\n<td>51 XXX</td>\n<td></td>\n<td></td>\n<td>\n</td></tr>\n<tr>\n<td>mobile</td>\n<td>682</td>\n<td>52 XXX</td>\n<td></td>\n<td></td>\n<td>\n</td></tr>\n<tr>\n<td>mobile</td>\n<td>682</td>\n<td>53 XXX</td>\n<td></td>\n<td></td>\n<td>\n</td></tr>\n<tr>\n<td>mobile</td>\n<td>682</td>\n<td>54 XXX</td>\n<td></td>\n<td></td>\n<td>\n</td></tr>\n<tr>\n<td>mobile</td>\n<td>682</td>\n<td>55 XXX</td>\n<td></td>\n<td></td>\n<td>\n</td></tr>\n<tr>\n<td>mobile</td>\n<td>682</td>\n<td>56 XXX</td>\n<td></td>\n<td></td>\n<td>\n</td></tr>\n<tr>\n<td>mobile</td>\n<td>682</td>\n<td>57 XXX</td>\n<td></td>\n<td></td>\n<td>\n</td></tr>\n<tr>\n<td>mobile</td>\n<td>682</td>\n<td>58 XXX</td>\n<td></td>\n<td></td>\n<td>\n</td></tr>\n<tr>\n<td>Voicemail and service number only</td>\n<td>682</td>\n<td>59 XXX</td>\n<td></td>\n<td></td>\n<td>\n</td></tr>\n<tr>\n<td>mobile</td>\n<td>682</td>\n<td>7X XXX</td>\n<td></td>\n<td></td>\n<td>\n</td></tr>\n<tr>\n<td>mobile</td>\n<td>682</td>\n<td>8X XXX</td>\n<td></td>\n<td></td>\n<td>\n</td></tr></tbody></table>\n<h2><span class=\"mw-headline\" id=\"See_also\">See als