:[{"ns":10,"exists":"","*":"Template:ISBN"},{"ns":10,"exists":"","*":"Template:Catalog lookup link"},{"ns":10,"exists":"","*":"Template:Trim"},{"ns":10,"exists":"","*":"Template:Yesno-no"},{"ns":10,"exists":"","*":"Template:Yesno"},{"ns":10,"exists":"","*":"Template:Error-small"},{"ns":10,"exists":"","*":"Template:Tl"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":10,"exists":"","*":"Template:Reflist"},{"ns":828,"exists":"","*":"Module:Check isxn"},{"ns":828,"exists":"","*":"Module:Error"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"}],"images":[],"externallinks":[],"sections":[{"toclevel":1,"level":"2","line":"Development","number":"1","index":"1","fromtitle":"Thematic_transformation","byteoffset":1531,"anchor":"Development"},{"toclevel":1,"level":"2","line":"Controversy","number":"2","index":"2","fromtitle":"Thematic_transformation","byteoffset":2571,"anchor":"Controversy"},{"toclevel":1,"level":"2","line":"Legacy","number":"3","index":"3","fromtitle":"Thematic_transformation","byteoffset":3810,"anchor":"Legacy"},{"toclevel":1,"level":"2","line":"See also","number":"4","index":"4","fromtitle":"Thematic_transformation","byteoffset":4535,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"Bibliography","number":"5","index":"5","fromtitle":"Thematic_transformation","byteoffset":4892,"anchor":"Bibliography"},{"toclevel":1,"level":"2","line":"References","number":"6","index":"6","fromtitle":"Thematic_transformation","byteoffset":6153,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Thematic transformation","iwlinks":[],"properties":[{"name":"defaultsort","*":"Thematic Transformation"},{"name":"wikibase_item","*":"Q3069568"}]}}