es"},{"toclevel":1,"level":"2","line":"Authentication and contested artifacts","number":"4","index":"4","fromtitle":"Suffrage_jewellery","byteoffset":2955,"anchor":"Authentication_and_contested_artifacts"},{"toclevel":1,"level":"2","line":"References","number":"5","index":"5","fromtitle":"Suffrage_jewellery","byteoffset":4717,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Suffrage jewellery","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q30324403"}]}}