","fromtitle":"Ramsgate_Sands","byteoffset":1722,"anchor":"Description"},{"toclevel":1,"level":"2","line":"Reception","number":"3","index":"3","fromtitle":"Ramsgate_Sands","byteoffset":3506,"anchor":"Reception"},{"toclevel":1,"level":"2","line":"Notes","number":"4","index":"4","fromtitle":"Ramsgate_Sands","byteoffset":4753,"anchor":"Notes"},{"toclevel":1,"level":"2","line":"References","number":"5","index":"5","fromtitle":"Ramsgate_Sands","byteoffset":4776,"anchor":"References"}],"parsewarnings":[],"displaytitle":"<i>Ramsgate Sands</i>","iwlinks":[],"properties":[{"name":"displaytitle","*":"<i>Ramsgate Sands</i>"},{"name":"wikibase_item","*":"Q18161946"}]}}