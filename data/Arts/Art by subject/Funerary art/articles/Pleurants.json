lookup link"},{"ns":10,"exists":"","*":"Template:Trim"},{"ns":10,"exists":"","*":"Template:Yesno-no"},{"ns":10,"exists":"","*":"Template:Yesno"},{"ns":10,"exists":"","*":"Template:Error-small"},{"ns":10,"exists":"","*":"Template:Tl"},{"ns":10,"exists":"","*":"Template:Commonscat"},{"ns":10,"exists":"","*":"Template:Commons category"},{"ns":10,"exists":"","*":"Template:Commons"},{"ns":10,"exists":"","*":"Template:Sister project"},{"ns":10,"exists":"","*":"Template:Side box"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"},{"ns":828,"exists":"","*":"Module:Check isxn"},{"ns":828,"exists":"","*":"Module:Error"},{"ns":828,"exists":"","*":"Module:Side box"},{"ns":828,"exists":"","*":"Module:Yesno"}],"images":["Grafmonument_Margaretha_van_Bourbon_(1438-1483)_-_Koninklijk_klooster_van_Brou_25-10-2016_11-14-22.jpg","Louvre_Grab.JPG","Tien_pleurants.jpg","Commons-logo.svg"],"externallinks":["https://books.google.co.uk/books?id=aKJbAAAAQAAJ&pg=PA10&lpg=PA10","http://www.louvre.fr/oeuvre-notices/tombeau-de-philippe-pot-1428-1493-grand-senechal-de-bourgogne","http://balat.kikirpa.be/object/100640","https://books.google.co.uk/books?id=aNUUAAAAIAAJ&pg=PA29&lpg=PA29"],"sections":[{"toclevel":1,"level":"2","line":"Britain","number":"1","index":"1","fromtitle":"Pleurants","byteoffset":1877,"anchor":"Britain"},{"toclevel":1,"level":"2","line":"Franco-Burgundian examples","number":"2","index":"2","fromtitle":"Pleurants","byteoffset":2904,"anchor":"Franco-Burgundian_examples"},{"toclevel":1,"level":"2","line":"Notes","number":"3","index":"3","fromtitle":"Pleurants","byteoffset":4473,"anchor":"Notes"},{"toclevel":1,"level":"2","line":"References","number":"4","index":"4","fromtitle":"Pleurants","byteoffset":4496,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Pleurants","iwlinks":[{"prefix":"commons","url":"https://commons.wikimedia.org/wiki/Category:Pleurants","*":"commons:Category:Pleurants"}],"properties":[{"name":"wikibase_item","*":"Q3392369"}]}}