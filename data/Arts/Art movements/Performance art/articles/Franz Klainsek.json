tle":"Franz_Klainsek","byteoffset":5612,"anchor":"Philosophy"},{"toclevel":1,"level":"2","line":"References","number":"4","index":"11","fromtitle":"Franz_Klainsek","byteoffset":6608,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"5","index":"12","fromtitle":"Franz_Klainsek","byteoffset":6803,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"Franz Klainsek","iwlinks":[],"properties":[{"name":"defaultsort","*":"Klainsek, Franz"},{"name":"wikibase_item","*":"Q21663127"}]}}