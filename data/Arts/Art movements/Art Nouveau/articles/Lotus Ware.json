ohio/marker_details.cfm?marker_id=695&file_id=4209"],"sections":[{"toclevel":1,"level":"2","line":"Knowles, Taylor & Knowles","number":"1","index":"1","fromtitle":"Lotus_Ware","byteoffset":811,"anchor":"Knowles,_Taylor_&_Knowles"},{"toclevel":1,"level":"2","line":"Stylistic influences","number":"2","index":"2","fromtitle":"Lotus_Ware","byteoffset":3919,"anchor":"Stylistic_influences"},{"toclevel":1,"level":"2","line":"Technique","number":"3","index":"3","fromtitle":"Lotus_Ware","byteoffset":4548,"anchor":"Technique"},{"toclevel":1,"level":"2","line":"Financial losses and the end of Lotus Ware","number":"4","index":"4","fromtitle":"Lotus_Ware","byteoffset":5530,"anchor":"Financial_losses_and_the_end_of_Lotus_Ware"},{"toclevel":1,"level":"2","line":"Legacy","number":"5","index":"5","fromtitle":"Lotus_Ware","byteoffset":6184,"anchor":"Legacy"},{"toclevel":1,"level":"2","line":"References","number":"6","index":"6","fromtitle":"Lotus_Ware","byteoffset":6502,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"7","index":"7","fromtitle":"Lotus_Ware","byteoffset":6530,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"Lotus Ware","iwlinks":[{"prefix":"commons","url":"https://commons.wikimedia.org/wiki/Category:Lotus_Ware","*":"commons:Category:Lotus Ware"}],"properties":[{"name":"wikibase_item","*":"Q6685213"}]}}