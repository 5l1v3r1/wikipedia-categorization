","*":"Template:Error-small"},{"ns":10,"exists":"","*":"Template:Tl"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"},{"ns":828,"exists":"","*":"Module:Link language"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Configuration"},{"ns":828,"exists":"","*":"Module:Category handler"},{"ns":828,"exists":"","*":"Module:Yesno"},{"ns":828,"exists":"","*":"Module:Category handler/data"},{"ns":828,"exists":"","*":"Module:Category handler/config"},{"ns":828,"exists":"","*":"Module:Category handler/shared"},{"ns":828,"exists":"","*":"Module:Category handler/blacklist"},{"ns":828,"exists":"","*":"Module:Namespace detect/data"},{"ns":828,"exists":"","*":"Module:Namespace detect/config"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:Check isxn"},{"ns":828,"exists":"","*":"Module:Error"}],"images":["Dogmatic_sarcophagus.JPG"],"externallinks":["https://books.google.com/books?id=BmWpMKaDBVUC&pg=PA39","https://books.google.com/books?id=OcRTwsDq_Z4C&pg=PA66","https://archive.is/20121211083831/http://campus.belmont.edu/honors/SarcPix/RomeDogmaticSarcophagus.jpg"],"sections":[{"toclevel":1,"level":"2","line":"Description","number":"1","index":"1","fromtitle":"Dogmatic_Sarcophagus","byteoffset":1415,"anchor":"Description"},{"toclevel":1,"level":"2","line":"Iconography","number":"2","index":"2","fromtitle":"Dogmatic_Sarcophagus","byteoffset":3667,"anchor":"Iconography"},{"toclevel":1,"level":"2","line":"Notes","number":"3","index":"3","fromtitle":"Dogmatic_Sarcophagus","byteoffset":5975,"anchor":"Notes"},{"toclevel":1,"level":"2","line":"References","number":"4","index":"4","fromtitle":"Dogmatic_Sarcophagus","byteoffset":6000,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"5","index":"5","fromtitle":"Dogmatic_Sarcophagus","byteoffset":6728,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"Dogmatic Sarcophagus","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q3950484"}]}}