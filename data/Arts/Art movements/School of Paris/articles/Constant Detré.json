:828,"exists":"","*":"Module:ResolveEntityId"},{"ns":828,"exists":"","*":"Module:Navbox"}],"images":["Constant_Detr\u00e9_(Szil\u00e1rd_Eduard_Diettmann),_photograph.jpg","Constant_Detr\u00e9,_A_La_Lanterne,_ca._1925,_brothel_scene,_pastel_on_paper.jpg"],"externallinks":["http://www.cdetre.com/crbst_14.html","http://www.cdetre.com/crbst_17.html","http://www.cdetre.com/","https://sites.google.com/site/julianbesset/mesconnus3"],"sections":[{"toclevel":1,"level":"2","line":"Early life","number":"1","index":"1","fromtitle":"Constant_Detr\u00e9","byteoffset":1405,"anchor":"Early_life"},{"toclevel":1,"level":"2","line":"Career","number":"2","index":"2","fromtitle":"Constant_Detr\u00e9","byteoffset":3153,"anchor":"Career"},{"toclevel":1,"level":"2","line":"Notes and references","number":"3","index":"3","fromtitle":"Constant_Detr\u00e9","byteoffset":6645,"anchor":"Notes_and_references"},{"toclevel":1,"level":"2","line":"External links","number":"4","index":"4","fromtitle":"Constant_Detr\u00e9","byteoffset":6683,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"Constant Detr\u00e9","iwlinks":[],"properties":[{"name":"defaultsort","*":"Detre, Constant"},{"name":"wikibase_item","*":"Q2994852"}]}}