ary"},{"ns":0,"exists":"","*":"Paris"},{"ns":0,"exists":"","*":"Pedagogue"},{"ns":0,"exists":"","*":"Pierre Boulez"},{"ns":0,"exists":"","*":"Prokofiev"},{"ns":0,"exists":"","*":"Rudolf Laban"},{"ns":0,"exists":"","*":"Scriabin"},{"ns":0,"exists":"","*":"Sir Charles Trevelyan, 3rd Baronet"},{"ns":0,"exists":"","*":"Tanztheater"},{"ns":0,"exists":"","*":"The Daily Telegraph"},{"ns":0,"exists":"","*":"The Sketch"},{"ns":0,"exists":"","*":"The Times"},{"ns":0,"exists":"","*":"Vienna"},{"ns":0,"exists":"","*":"Wiener Zeitung"},{"ns":100,"exists":"","*":"Portal:Dance"}],"templates":[{"ns":10,"exists":"","*":"Template:Reflist"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":10,"exists":"","*":"Template:Portal"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"},{"ns":828,"exists":"","*":"Module:Portal"},{"ns":828,"exists":"","*":"Module:Portal/images/d"}],"images":["Disco-Dancer-4-Remix-by-Merlin2525.svg"],"externallinks":["http://hildeholger.com/index.php?option=com_content&task=view&id=47&Itemid=55"],"sections":[{"toclevel":1,"level":"2","line":"Life and career","number":"1","index":"1","fromtitle":"Lilian_Harmel","byteoffset":238,"anchor":"Life_and_career"},{"toclevel":1,"level":"2","line":"References","number":"2","index":"2","fromtitle":"Lilian_Harmel","byteoffset":5998,"anchor":"References"},{"toclevel":1,"level":"2","line":"See also","number":"3","index":"3","fromtitle":"Lilian_Harmel","byteoffset":6028,"anchor":"See_also"}],"parsewarnings":[],"displaytitle":"Lilian Harmel","iwlinks":[{"prefix":"de","url":"https://de.wikipedia.org/wiki/Neues_Wiener_Tagblatt","*":"de:Neues Wiener Tagblatt"},{"prefix":"de","url":"https://de.wikipedia.org/wiki/Neues_Wiener_Journal","*":"de:Neues Wiener Journal"},{"prefix":"de","url":"https://de.wikipedia.org/wiki/Felix_Albrecht_Harta","*":"de:Felix Albrecht Harta"},{"prefix":"it","url":"https://it.wikipedia.org/wiki/Il_Giornale_d%27Italia","*":"it:Il Giornale d'Italia"}],"properties":[{"name":"defaultsort","*":"Ha