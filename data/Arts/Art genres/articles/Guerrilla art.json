"http://www.banksy.co.uk","http://www.irangraffiti.com","http://faculty.samfox.wustl.edu/bulawsky/guerillalinks.html"],"sections":[{"toclevel":1,"level":"2","line":"Guerrilla artists","number":"1","index":"1","fromtitle":"Guerrilla_art","byteoffset":1243,"anchor":"Guerrilla_artists"},{"toclevel":2,"level":"3","line":"Artists","number":"1.1","index":"2","fromtitle":"Guerrilla_art","byteoffset":2278,"anchor":"Artists"},{"toclevel":1,"level":"2","line":"Early guerrilla art","number":"2","index":"3","fromtitle":"Guerrilla_art","byteoffset":3109,"anchor":"Early_guerrilla_art"},{"toclevel":1,"level":"2","line":"Artistic movement","number":"3","index":"4","fromtitle":"Guerrilla_art","byteoffset":5325,"anchor":"Artistic_movement"},{"toclevel":1,"level":"2","line":"See also","number":"4","index":"5","fromtitle":"Guerrilla_art","byteoffset":6108,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"5","index":"6","fromtitle":"Guerrilla_art","byteoffset":6222,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"6","index":"7","fromtitle":"Guerrilla_art","byteoffset":6252,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"Guerrilla art","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q3778806"}]}}