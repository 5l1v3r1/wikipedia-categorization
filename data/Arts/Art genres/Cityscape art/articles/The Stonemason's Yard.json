d","byteoffset":1331,"anchor":"Painting"},{"toclevel":1,"level":"2","line":"References","number":"2","index":"2","fromtitle":"The_Stonemason's_Yard","byteoffset":6156,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"3","index":"3","fromtitle":"The_Stonemason's_Yard","byteoffset":6184,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"<i>The Stonemason's Yard</i>","iwlinks":[],"properties":[{"name":"displaytitle","*":"<i>The Stonemason's Yard</i>"},{"name":"defaultsort","*":"Stonemason's Yard"},{"name":"wikibase_item","*":"Q2943415"}]}}