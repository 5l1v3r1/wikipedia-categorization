xists":"","*":"Module:Citation/CS1/COinS"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"}],"images":["Hippolyte_Bayard_-_Drownedman_1840.jpg","Integration_conceptual.jpg","The_Sleep.jpg"],"externallinks":["https://www.theguardian.com/artanddesign/2009/nov/16/conceptual-photography-prizes","https://digital-photography-school.com/guide-to-creating-unique-conceptual-photography/","http://www.source.ie/feature/what_is_conceptual.html","http://www.metmuseum.org/toah/hd/cncp/hd_cncp.htm","http://www.wisegeek.com/what-is-conceptual-photography.htm"],"sections":[{"toclevel":1,"level":"2","line":"Conceptual photography as a methodology","number":"1","index":"1","fromtitle":"Conceptual_photography","byteoffset":791,"anchor":"Conceptual_photography_as_a_methodology"},{"toclevel":1,"level":"2","line":"Conceptual photography as a genre","number":"2","index":"2","fromtitle":"Conceptual_photography","byteoffset":1751,"anchor":"Conceptual_photography_as_a_genre"},{"toclevel":1,"level":"2","line":"Conceptual photography and conceptual art","number":"3","index":"3","fromtitle":"Conceptual_photography","byteoffset":2014,"anchor":"Conceptual_photography_and_conceptual_art"},{"toclevel":1,"level":"2","line":"Conceptual photography and fine-art photography","number":"4","index":"4","fromtitle":"Conceptual_photography","byteoffset":2924,"anchor":"Conceptual_photography_and_fine-art_photography"},{"toclevel":1,"level":"2","line":"References","number":"5","index":"5","fromtitle":"Conceptual_photography","byteoffset":4705,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"6","index":"6","fromtitle":"Conceptual_photography","byteoffset":4733,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"Conceptual photography","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q5158441"}]}}