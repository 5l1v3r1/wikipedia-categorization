p?pagename=Water_Sky_Garden&params=49_10_29_N_123_9_5_W_"],"sections":[{"toclevel":1,"level":"2","line":"Design","number":"1","index":"1","fromtitle":"Water_Sky_Garden","byteoffset":890,"anchor":"Design"},{"toclevel":1,"level":"2","line":"References","number":"2","index":"2","fromtitle":"Water_Sky_Garden","byteoffset":4354,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"3","index":"3","fromtitle":"Water_Sky_Garden","byteoffset":4515,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"<i>Water Sky Garden</i>","iwlinks":[],"properties":[{"name":"displaytitle","*":"<i>Water Sky Garden</i>"},{"name":"wikibase_item","*":"Q7973367"}]}}