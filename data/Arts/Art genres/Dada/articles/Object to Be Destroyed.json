"exists":"","*":"Nine Inch Nails"},{"ns":0,"exists":"","*":"Readymades of Marcel Duchamp"},{"ns":0,"exists":"","*":"Tate Modern"},{"ns":0,"exists":"","*":"They Might Be Giants"},{"ns":0,"exists":"","*":"Used good"},{"ns":4,"exists":"","*":"Wikipedia:Citation needed"}],"templates":[{"ns":10,"exists":"","*":"Template:Italic title"},{"ns":10,"exists":"","*":"Template:Citation needed"},{"ns":10,"exists":"","*":"Template:Fix"},{"ns":10,"exists":"","*":"Template:Category handler"},{"ns":10,"exists":"","*":"Template:Fix/category"},{"ns":10,"exists":"","*":"Template:Delink"},{"ns":10,"exists":"","*":"Template:Reflist"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":828,"exists":"","*":"Module:Italic title"},{"ns":828,"exists":"","*":"Module:No globals"},{"ns":828,"exists":"","*":"Module:Yesno"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:Unsubst"},{"ns":828,"exists":"","*":"Module:Category handler"},{"ns":828,"exists":"","*":"Module:Category handler/data"},{"ns":828,"exists":"","*":"Module:Category handler/config"},{"ns":828,"exists":"","*":"Module:Category handler/shared"},{"ns":828,"exists":"","*":"Module:Category handler/blacklist"},{"ns":828,"exists":"","*":"Module:Namespace detect/data"},{"ns":828,"exists":"","*":"Module:Namespace detect/config"},{"ns":828,"exists":"","*":"Module:Delink"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"}],"images":["Objectdestroyed.jpg"],"externallinks":["https://books.google.com/books?id=8mfVBTf1aaYC&q=page+229#v=onepage&q=Qualite%20Excelsior&f=false","http://www.time.com/time/magazine/article/0,9171,809373,00.html","https://www.flickr.com/photos/sixteen-miles/4043020587/","http://www.tate.org.uk/magazine/issue3/eyeofthebeholder.htm","http://www.moma.org/collection/theme.php?theme_id=10468"],"sections":[{"toclevel":1,"level":"2","line":"The Original Readymade","number":"1","index":"1","fromtitle":"Object_to_Be_Destroyed","byteoffset":687,"anchor":"The_Original_Readymade"},{"toc