elin?","number":"1","index":"1","fromtitle":"The_Great_Led_Zeppelin_Wine_Pairing","byteoffset":1725,"anchor":"Why_Led_Zeppelin?"},{"toclevel":1,"level":"2","line":"History","number":"2","index":"2","fromtitle":"The_Great_Led_Zeppelin_Wine_Pairing","byteoffset":2307,"anchor":"History"},{"toclevel":1,"level":"2","line":"Quotes","number":"3","index":"3","fromtitle":"The_Great_Led_Zeppelin_Wine_Pairing","byteoffset":4220,"anchor":"Quotes"},{"toclevel":1,"level":"2","line":"References","number":"4","index":"4","fromtitle":"The_Great_Led_Zeppelin_Wine_Pairing","byteoffset":5335,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"5","index":"5","fromtitle":"The_Great_Led_Zeppelin_Wine_Pairing","byteoffset":5363,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"The Great Led Zeppelin Wine Pairing","iwlinks":[],"properties":[{"name":"defaultsort","*":"Great Led Zeppelin Wine Pairing"},{"name":"wikibase_item","*":"Q7737825"}]}}