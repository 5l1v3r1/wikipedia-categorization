ne":"Biography","number":"1","index":"1","fromtitle":"Bella_Goodall","byteoffset":889,"anchor":"Biography"},{"toclevel":1,"level":"2","line":"Notes","number":"2","index":"2","fromtitle":"Bella_Goodall","byteoffset":5669,"anchor":"Notes"},{"toclevel":1,"level":"2","line":"References","number":"3","index":"3","fromtitle":"Bella_Goodall","byteoffset":5694,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"4","index":"4","fromtitle":"Bella_Goodall","byteoffset":5996,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"Bella Goodall","iwlinks":[],"properties":[{"name":"defaultsort","*":"Goodall, Bella"},{"name":"wikibase_item","*":"Q4883359"}]}}