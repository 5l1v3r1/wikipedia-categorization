\n<th>205</th>\n<th>206</th>\n<th colspan=\"3\">&#160;\n</th></tr>\n<tr bgcolor=\"#ffe0e0\" align=\"center\">\n<td>Pm<span style=\"text-decoration:overline;\">3</span></td>\n<td>Pn<span style=\"text-decoration:overline;\">3</span></td>\n<td>Fm<span style=\"text-decoration:overline;\">3</span></td>\n<td>Fd<span style=\"text-decoration:overline;\">3</span></td>\n<td>I<span style=\"text-decoration:overline;\">3</span></td>\n<td>Pa<span style=\"text-decoration:overline;\">3</span></td>\n<td>Ia<span style=\"text-decoration:overline;\">3</span></td>\n<td colspan=\"3\">&#160;\n</td></tr>\n<tr bgcolor=\"#e0e0ff\" align=\"center\">\n<td>4<sup>\u2212</sup></td>\n<td>4<sup>+o</sup></td>\n<td>2<sup>\u2212</sup></td>\n<td>2<sup>+o</sup></td>\n<td>8<sup>\u2212o</sup></td>\n<td>2<sup>\u2212</sup>/4</td>\n<td>4<sup>\u2212</sup>/4</td>\n<td colspan=\"3\">&#160;\n</td></tr>\n<tr bgcolor=\"#ffffd0\" align=\"center\">\n<td>P4<span style=\"text-decoration:overline;\">3</span></td>\n<td>P<sub>n</sub>4<span style=\"text-decoration:overline;\">3</span></td>\n<td>F4<span style=\"text-decoration:overline;\">3</span></td>\n<td>F<sub>d</sub>4<span style=\"text-decoration:overline;\">3</span></td>\n<td>I4<span style=\"text-decoration:overline;\">3</span></td>\n<td>P<sub>b</sub>4<span style=\"text-decoration:overline;\">3</span></td>\n<td>I<sub>b</sub>4<span style=\"text-decoration:overline;\">3</span></td>\n<td colspan=\"3\">&#160;\n</td></tr>\n<tr bgcolor=\"#e0ffe0\" align=\"center\">\n<td>[4,3<sup>+</sup>,4]</td>\n<td>[[4,3<sup>+</sup>,4]<sup>+</sup>]</td>\n<td>[4,(3<sup>1,1</sup>)<sup>+</sup>]</td>\n<td>[[3<sup>[4]</sup>]]<sup>+</sup></td>\n<td>[[4,3<sup>+</sup>,4]]</td>\n<td></td>\n<td></td>\n<td colspan=\"3\">&#160;\n</td></tr>\n<tr align=\"center\">\n<th rowspan=\"5\">Gyroidal<br />432<br />(432)\n</th>\n<th>207</th>\n<th>208</th>\n<th>209</th>\n<th>210</th>\n<th>211</th>\n<th>212</th>\n<th>213</th>\n<th>214</th>\n<th colspan=\"2\">&#160;\n</th></tr>\n<tr bgcolor=\"#ffe0e0\" align=\"cente