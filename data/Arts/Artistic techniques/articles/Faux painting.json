svg"],"externallinks":["https://books.google.com/books?id=CLRZlNuSTMkC&lpg=PA110&dq=dictionary%20painting%20faux&pg=PA110#v=onepage&q=dictionary%20painting%20faux&f=false"],"sections":[{"toclevel":1,"level":"2","line":"History","number":"1","index":"1","fromtitle":"Faux_painting","byteoffset":958,"anchor":"History"},{"toclevel":1,"level":"2","line":"20th century revival","number":"2","index":"2","fromtitle":"Faux_painting","byteoffset":1938,"anchor":"20th_century_revival"},{"toclevel":1,"level":"2","line":"Faux finishes","number":"3","index":"3","fromtitle":"Faux_painting","byteoffset":3711,"anchor":"Faux_finishes"},{"toclevel":1,"level":"2","line":"References","number":"4","index":"4","fromtitle":"Faux_painting","byteoffset":5286,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Faux painting","iwlinks":[{"prefix":"wikt","url":"https://en.wiktionary.org/wiki/faux","*":"wikt:faux"},{"prefix":"commons","url":"https://commons.wikimedia.org/wiki/Category:Faux_painting","*":"commons:Category:Faux painting"}],"properties":[{"name":"wikibase_item","*":"Q5438401"}]}}