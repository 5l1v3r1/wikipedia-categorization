/data"},{"ns":828,"exists":"","*":"Module:Category handler/config"},{"ns":828,"exists":"","*":"Module:Category handler/shared"},{"ns":828,"exists":"","*":"Module:Category handler/blacklist"},{"ns":828,"exists":"","*":"Module:Namespace detect/data"},{"ns":828,"exists":"","*":"Module:Namespace detect/config"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"}],"images":["Question_book-new.svg","Fluzone.jpg"],"externallinks":["http://whqlibdoc.who.int/bulletin/1985/Vol63-No6/bulletin_1985_63(6)_1055-1068.pdf","//www.ncbi.nlm.nih.gov/pubmed/3879673","https://www.cdc.gov/flu/professionals/vaccination/effectivenessqa.htm#studies-differ","http://www.sciencebasedmedicine.org/flu-vaccine-efficacy"],"sections":[{"toclevel":1,"level":"2","line":"Vaccine Efficacy Formula","number":"1","index":"1","fromtitle":"Vaccine_efficacy","byteoffset":1129,"anchor":"Vaccine_Efficacy_Formula"},{"toclevel":1,"level":"2","line":"How it's tested","number":"2","index":"2","fromtitle":"Vaccine_efficacy","byteoffset":2236,"anchor":"How_it's_tested"},{"toclevel":1,"level":"2","line":"Risks To Be Considered","number":"3","index":"3","fromtitle":"Vaccine_efficacy","byteoffset":4669,"anchor":"Risks_To_Be_Considered"},{"toclevel":1,"level":"2","line":"Cases Studied","number":"4","index":"4","fromtitle":"Vaccine_efficacy","byteoffset":4818,"anchor":"Cases_Studied"},{"toclevel":1,"level":"2","line":"Conclusions","number":"5","index":"5","fromtitle":"Vaccine_efficacy","byteoffset":7016,"anchor":"Conclusions"},{"toclevel":1,"level":"2","line":"References","number":"6","index":"6","fromtitle":"Vaccine_efficacy","byteoffset":7518,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Vaccine efficacy","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q7907952"}]}}