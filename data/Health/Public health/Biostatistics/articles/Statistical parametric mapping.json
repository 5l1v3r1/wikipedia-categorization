"*":"Module:Category handler/data"},{"ns":828,"exists":"","*":"Module:Category handler/config"},{"ns":828,"exists":"","*":"Module:Category handler/shared"},{"ns":828,"exists":"","*":"Module:Category handler/blacklist"},{"ns":828,"exists":"","*":"Module:Namespace detect/data"},{"ns":828,"exists":"","*":"Module:Namespace detect/config"}],"images":["Question_book-new.svg","Functional_magnetic_resonance_imaging.jpg"],"externallinks":["http://www.fil.ion.ucl.ac.uk/spm/","http://www.mccauslandcenter.sc.edu/CRNL/","http://cogprints.org/6193/","http://www.mrc-cbu.cam.ac.uk/Imaging/Common/","http://www.fil.ion.ucl.ac.uk/~mgray/Presentations/Buttons%20in%20SPM5.ppt","http://spect.yale.edu","http://www.imagilys.com/autospm.html"],"sections":[{"toclevel":1,"level":"2","line":"Approach","number":"1","index":"1","fromtitle":"Statistical_parametric_mapping","byteoffset":608,"anchor":"Approach"},{"toclevel":2,"level":"3","line":"Unit of measurement","number":"1.1","index":"2","fromtitle":"Statistical_parametric_mapping","byteoffset":621,"anchor":"Unit_of_measurement"},{"toclevel":2,"level":"3","line":"Experimental design","number":"1.2","index":"3","fromtitle":"Statistical_parametric_mapping","byteoffset":1325,"anchor":"Experimental_design"},{"toclevel":2,"level":"3","line":"Image pre-processing","number":"1.3","index":"4","fromtitle":"Statistical_parametric_mapping","byteoffset":2462,"anchor":"Image_pre-processing"},{"toclevel":2,"level":"3","line":"Statistical comparison","number":"1.4","index":"5","fromtitle":"Statistical_parametric_mapping","byteoffset":4175,"anchor":"Statistical_comparison"},{"toclevel":2,"level":"3","line":"Graphical representations","number":"1.5","index":"6","fromtitle":"Statistical_parametric_mapping","byteoffset":5399,"anchor":"Graphical_representations"},{"toclevel":1,"level":"2","line":"SPM software","number":"2","index":"7","fromtitle":"Statistical_parametric_mapping","byteoffset":6568,"anchor":"SPM_software"},{"toclevel":1,"level":"2","line":"See also","number":"3","index":"8","fromtitle":"Statistical_parametric_mapping","byteoffset":6858,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"External links","number":"4","index":"9","fromtitle":"Statistical_parametric_mapping","byteoffset":7149,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"Statistical parametric mapping","iwlinks":[{"prefix":"b","url":"https://en.wikibooks.org/wiki/SPM","*":"b:SPM"},{"prefix":"b","url":"https://en.wikibooks.org/wiki/SPM-Information_to_include_in_papers","*":"b:SPM-Information to include in papers"}],"properties":[{"name":"wikibase_item","*":"Q2940624"}]}}