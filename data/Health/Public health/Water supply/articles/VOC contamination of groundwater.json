VOCs"},{"toclevel":2,"level":"3","line":"PCE","number":"2.1","index":"3","fromtitle":"VOC_contamination_of_groundwater","byteoffset":597,"anchor":"PCE"},{"toclevel":2,"level":"3","line":"TCE","number":"2.2","index":"4","fromtitle":"VOC_contamination_of_groundwater","byteoffset":751,"anchor":"TCE"},{"toclevel":2,"level":"3","line":"Toluene","number":"2.3","index":"5","fromtitle":"VOC_contamination_of_groundwater","byteoffset":969,"anchor":"Toluene"},{"toclevel":1,"level":"2","line":"History","number":"3","index":"6","fromtitle":"VOC_contamination_of_groundwater","byteoffset":1826,"anchor":"History"},{"toclevel":2,"level":"3","line":"Camp Lejeune","number":"3.1","index":"7","fromtitle":"VOC_contamination_of_groundwater","byteoffset":1838,"anchor":"Camp_Lejeune"},{"toclevel":2,"level":"3","line":"Love Canal","number":"3.2","index":"8","fromtitle":"VOC_contamination_of_groundwater","byteoffset":2593,"anchor":"Love_Canal"},{"toclevel":2,"level":"3","line":"Valley of the Drums","number":"3.3","index":"9","fromtitle":"VOC_contamination_of_groundwater","byteoffset":3037,"anchor":"Valley_of_the_Drums"},{"toclevel":1,"level":"2","line":"See also","number":"4","index":"10","fromtitle":"VOC_contamination_of_groundwater","byteoffset":3826,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"5","index":"11","fromtitle":"VOC_contamination_of_groundwater","byteoffset":3886,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"6","index":"12","fromtitle":"VOC_contamination_of_groundwater","byteoffset":3914,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"VOC contamination of groundwater","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q7907368"}]}}