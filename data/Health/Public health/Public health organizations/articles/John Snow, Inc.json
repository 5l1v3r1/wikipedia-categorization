,"http://www.jsi.com","http://scms.pfscm.org/scms","http://deliver.jsi.com","http://www.aidstar-one.com/about","http://www.astarteproject.org/index.htm"],"sections":[{"toclevel":1,"level":"2","line":"Activities","number":"1","index":"1","fromtitle":"John_Snow,_Inc","byteoffset":1228,"anchor":"Activities"},{"toclevel":1,"level":"2","line":"History","number":"2","index":"2","fromtitle":"John_Snow,_Inc","byteoffset":6031,"anchor":"History"},{"toclevel":1,"level":"2","line":"Fellowships","number":"3","index":"3","fromtitle":"John_Snow,_Inc","byteoffset":6506,"anchor":"Fellowships"},{"toclevel":1,"level":"2","line":"References","number":"4","index":"4","fromtitle":"John_Snow,_Inc","byteoffset":7265,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"5","index":"5","fromtitle":"John_Snow,_Inc","byteoffset":7295,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"John Snow, Inc","iwlinks":[],"properties":[{"name":"defaultsort","*":"John Snow, Inc"},{"name":"wikibase_item","*":"Q6258444"}]}}