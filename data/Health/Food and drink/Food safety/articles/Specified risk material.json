parameters"}],"images":["Question_book-new.svg","BSE_SRMs.svg"],"externallinks":["//doi.org/10.1016/j.procbio.2013.04.015","//doi.org/10.1016/j.procbio.2009.06.002","http://ncseonline.org/nle/crsreports/05jun/97-905.pdf","https://web.archive.org/web/20110810044532/http://ncseonline.org/nle/crsreports/05jun/97-905.pdf"],"sections":[{"toclevel":1,"level":"2","line":"BSE","number":"1","index":"1","fromtitle":"Specified_risk_material","byteoffset":2306,"anchor":"BSE"},{"toclevel":2,"level":"3","line":"BSE SRMs","number":"1.1","index":"2","fromtitle":"Specified_risk_material","byteoffset":2523,"anchor":"BSE_SRMs"},{"toclevel":2,"level":"3","line":"Removal of BSE SRMs","number":"1.2","index":"3","fromtitle":"Specified_risk_material","byteoffset":3487,"anchor":"Removal_of_BSE_SRMs"},{"toclevel":1,"level":"2","line":"See also","number":"2","index":"4","fromtitle":"Specified_risk_material","byteoffset":4136,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"3","index":"5","fromtitle":"Specified_risk_material","byteoffset":4225,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Specified risk material","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q910559"}]}}