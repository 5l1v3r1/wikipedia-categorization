s":"","*":"Calcium oxide"},{"ns":0,"exists":"","*":"Carbon dioxide"},{"ns":0,"exists":"","*":"Carbonation"},{"ns":0,"exists":"","*":"Cement"},{"ns":0,"exists":"","*":"Chemical reaction"},{"ns":0,"exists":"","*":"Clarifier"},{"ns":0,"exists":"","*":"Coagulation"},{"ns":0,"exists":"","*":"Concrete"},{"ns":0,"exists":"","*":"Concrete cancer"},{"ns":0,"exists":"","*":"Crystallization"},{"ns":0,"exists":"","*":"Europe"},{"ns":0,"exists":"","*":"Flocculation"},{"ns":0,"exists":"","*":"Fructose"},{"ns":0,"exists":"","*":"Glucose"},{"ns":0,"exists":"","*":"John V. N. Dorr"},{"ns":0,"exists":"","*":"Lime (material)"},{"ns":0,"exists":"","*":"Lime softening"},{"ns":0,"exists":"","*":"Limewater"},{"ns":0,"exists":"","*":"Molasses"},{"ns":0,"exists":"","*":"Monosaccharide"},{"ns":0,"exists":"","*":"PH"},{"ns":0,"exists":"","*":"Pourbaix diagram"},{"ns":0,"exists":"","*":"Protein"},{"ns":0,"exists":"","*":"Rebar"},{"ns":0,"exists":"","*":"Sedimentation"},{"ns":0,"exists":"","*":"Soda ash"},{"ns":0,"exists":"","*":"Sucrose"},{"ns":0,"exists":"","*":"Sugar"},{"ns":0,"exists":"","*":"Sugar beet"},{"ns":0,"exists":"","*":"Sugar refinery of Tienen"},{"ns":0,"exists":"","*":"Sugarcane"},{"ns":0,"exists":"","*":"Sulfur"},{"ns":0,"exists":"","*":"Suspension (chemistry)"},{"ns":0,"exists":"","*":"Water softening"},{"ns":0,"*":"Dorr-Oliver"},{"ns":0,"*":"Phosphotation"}],"templates":[{"ns":10,"exists":"","*":"Template:Cite web"},{"ns":828,"exists":"","*":"Module:Citation/CS1"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Configuration"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Whitelist"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Utilities"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Date validation"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Identifiers"},{"ns":828,"exists":"","*":"Module:Citation/CS1/COinS"}],"images":["Betonrot_Hippodroom.JPG"],"externallinks":["http://www.corrosion-doctors.org/Thermo/ironE-pH.htm","https://web.archive.org/web/20041217094637/http: