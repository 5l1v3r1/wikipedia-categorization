.php","https://web.archive.org/web/20140810055014/http://www.hai.ie/minkhounds.php","http://www.bbc.co.uk/ethics/animals/using/hunting_1.shtml"],"sections":[{"toclevel":1,"level":"2","line":"Origins","number":"1","index":"1","fromtitle":"Mink_hunting","byteoffset":498,"anchor":"Origins"},{"toclevel":1,"level":"2","line":"Hunting","number":"2","index":"2","fromtitle":"Mink_hunting","byteoffset":1718,"anchor":"Hunting"},{"toclevel":1,"level":"2","line":"See also","number":"3","index":"3","fromtitle":"Mink_hunting","byteoffset":2975,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"4","index":"4","fromtitle":"Mink_hunting","byteoffset":3145,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Mink hunting","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q16997864"}]}}