","//www.ncbi.nlm.nih.gov/pmc/articles/PMC270585","//www.ncbi.nlm.nih.gov/pubmed/1452682"],"sections":[{"toclevel":1,"level":"2","line":"History","number":"1","index":"1","fromtitle":"Dried_blood_spot","byteoffset":323,"anchor":"History"},{"toclevel":1,"level":"2","line":"Procedure","number":"2","index":"2","fromtitle":"Dried_blood_spot","byteoffset":1853,"anchor":"Procedure"},{"toclevel":1,"level":"2","line":"Dried blood spot testing for HIV infection","number":"3","index":"3","fromtitle":"Dried_blood_spot","byteoffset":4259,"anchor":"Dried_blood_spot_testing_for_HIV_infection"},{"toclevel":1,"level":"2","line":"Principle","number":"4","index":"4","fromtitle":"Dried_blood_spot","byteoffset":5532,"anchor":"Principle"},{"toclevel":1,"level":"2","line":"See also","number":"5","index":"5","fromtitle":"Dried_blood_spot","byteoffset":5943,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"6","index":"6","fromtitle":"Dried_blood_spot","byteoffset":6000,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Dried blood spot","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q5307389"}]}}