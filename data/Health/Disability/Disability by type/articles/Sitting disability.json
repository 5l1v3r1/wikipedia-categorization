ts":"","*":"Wayback Machine"},{"ns":0,"exists":"","*":"Wheelbench"}],"templates":[{"ns":10,"exists":"","*":"Template:Webarchive"},{"ns":10,"exists":"","*":"Template:Cite journal"},{"ns":828,"exists":"","*":"Module:Webarchive"},{"ns":828,"exists":"","*":"Module:No globals"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:Webarchive/data"},{"ns":828,"exists":"","*":"Module:Citation/CS1"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Configuration"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Whitelist"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Utilities"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Date validation"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Identifiers"},{"ns":828,"exists":"","*":"Module:Citation/CS1/COinS"}],"images":[],"externallinks":["http://www.dok.no/universell-utforming-og-sittehemmede-.4452524-82143.html","http://www.caringmedical.com/conditions/Ischial_Tuberosity_Pain.htm","http://www.usu.edu/health/HealthInfo/backpain.htm","http://www.dhmc.org/ortho/Spine_Center/lowback_pain.html","https://web.archive.org/web/20060514060227/http://www.dhmc.org/ortho/spine_center/lowback_pain.html","http://www.aafp.org/afp/20000315/1779.html","//www.ncbi.nlm.nih.gov/pubmed/10750882","http://socialsecuritydisability-blog.com/can-i-get-benefits-for-a-sitting-disability/","http://www.ldo.no/no/Klagesaker/Arkiv/2014/131168-TT-ordningen-i-Akershus/","http://linkinghub.elsevier.com/retrieve/pii/S0002937805001729","//doi.org/10.1016/j.ajog.2005.01.051","//www.ncbi.nlm.nih.gov/pubmed/15902174","http://www.spuninfo.org/index_files/PhysiciansPerspectiveTreatment.htm","https://www.youtube.com/watch?v=BORPdYuwnlY","http://invisibledisabilities.org/invisible-no-more/radio-dj-educates-community/","http://jaapa.com/issues/j20040701/articles/lowback.html","http://www.mosken.com/sittingdisability.html","http://www.emedicine.com/pmr/topic242.htm"],"sections":[{"toclevel":1,"level":"2","line":"Possible causes","number":"1","inde