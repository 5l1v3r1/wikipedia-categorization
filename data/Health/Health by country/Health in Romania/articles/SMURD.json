ine":"History","number":"1","index":"1","fromtitle":"SMURD","byteoffset":2403,"anchor":"History"},{"toclevel":2,"level":"3","line":"1991","number":"1.1","index":"2","fromtitle":"SMURD","byteoffset":3319,"anchor":"1991"},{"toclevel":2,"level":"3","line":"1992","number":"1.2","index":"3","fromtitle":"SMURD","byteoffset":4626,"anchor":"1992"},{"toclevel":2,"level":"3","line":"1993","number":"1.3","index":"4","fromtitle":"SMURD","byteoffset":6096,"anchor":"1993"},{"toclevel":2,"level":"3","line":"1994","number":"1.4","index":"5","fromtitle":"SMURD","byteoffset":7539,"anchor":"1994"},{"toclevel":2,"level":"3","line":"1995\u20131996","number":"1.5","index":"6","fromtitle":"SMURD","byteoffset":8635,"anchor":"1995\u20131996"},{"toclevel":2,"level":"3","line":"1997","number":"1.6","index":"7","fromtitle":"SMURD","byteoffset":9606,"anchor":"1997"},{"toclevel":2,"level":"3","line":"1998","number":"1.7","index":"8","fromtitle":"SMURD","byteoffset":10397,"anchor":"1998"},{"toclevel":1,"level":"2","line":"Today","number":"2","index":"9","fromtitle":"SMURD","byteoffset":11285,"anchor":"Today"},{"toclevel":1,"level":"2","line":"References","number":"3","index":"10","fromtitle":"SMURD","byteoffset":12735,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"4","index":"11","fromtitle":"SMURD","byteoffset":12764,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"SMURD","iwlinks":[{"prefix":"commons","url":"https://commons.wikimedia.org/wiki/Category:SMURD","*":"commons:Category:SMURD"}],"properties":[{"name":"defaultsort","*":"Smurd"},{"name":"wikibase_item","*":"Q896477"}]}}