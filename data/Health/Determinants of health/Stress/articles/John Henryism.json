"},{"toclevel":1,"level":"2","line":"Effects","number":"3","index":"3","fromtitle":"John_Henryism","byteoffset":4232,"anchor":"Effects"},{"toclevel":1,"level":"2","line":"See also","number":"4","index":"4","fromtitle":"John_Henryism","byteoffset":5612,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"5","index":"5","fromtitle":"John_Henryism","byteoffset":5744,"anchor":"References"}],"parsewarnings":[],"displaytitle":"John Henryism","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q6239003"}]}}