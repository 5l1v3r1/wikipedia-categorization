,{"ns":828,"exists":"","*":"Module:No globals"},{"ns":828,"exists":"","*":"Module:Yesno"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:Message box/configuration"},{"ns":828,"exists":"","*":"Module:Category handler"},{"ns":828,"exists":"","*":"Module:Category handler/data"},{"ns":828,"exists":"","*":"Module:Category handler/config"},{"ns":828,"exists":"","*":"Module:Category handler/shared"},{"ns":828,"exists":"","*":"Module:Category handler/blacklist"},{"ns":828,"exists":"","*":"Module:Namespace detect/data"},{"ns":828,"exists":"","*":"Module:Namespace detect/config"},{"ns":828,"exists":"","*":"Module:Citation/CS1"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Configuration"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Whitelist"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Utilities"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Date validation"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Identifiers"},{"ns":828,"exists":"","*":"Module:Citation/CS1/COinS"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"}],"images":["Lock-green.svg","Ambox_important.svg","Ambox_current_red.svg"],"externallinks":["//doi.org/10.1038/oby.2012.99","http://www.myhealthnewsdaily.com/body-fat-index-obesity-calculator-110303-1230/","http://www.emaxhealth.com/1024/new-bai-better-old-bmi","http://www.plosone.org/article/info:doi%2F10.1371%2Fjournal.pone.0035281","//www.ncbi.nlm.nih.gov/pmc/articles/PMC3477292","//doi.org/10.1038/oby.2012.81","//www.ncbi.nlm.nih.gov/pubmed/22484365","http://www.plosone.org/article/info:doi%2F10.1371%2Fjournal.pone.0063999","//doi.org/10.1371/journal.pone.0063999","http://www.shapesense.com/fitness-exercise/calculators/body-adiposity-index-calculator.aspx","http://www.theheart.org/article/1193643.do","https://web.archive.org/web/20110306135310/http://www.ottawacitizen.com/health/Better+than+obesity+scale+proposed/4378908/story.html"],"sections":[{"toclevel":1,"level":"2","line":"Formula","number":