:"","*":"Template:Main other"},{"ns":828,"exists":"","*":"Module:Unsubst"},{"ns":828,"exists":"","*":"Module:Message box"},{"ns":828,"exists":"","*":"Module:No globals"},{"ns":828,"exists":"","*":"Module:Yesno"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:Message box/configuration"},{"ns":828,"exists":"","*":"Module:Category handler"},{"ns":828,"exists":"","*":"Module:Category handler/data"},{"ns":828,"exists":"","*":"Module:Category handler/config"},{"ns":828,"exists":"","*":"Module:Category handler/shared"},{"ns":828,"exists":"","*":"Module:Category handler/blacklist"},{"ns":828,"exists":"","*":"Module:Namespace detect/data"},{"ns":828,"exists":"","*":"Module:Namespace detect/config"},{"ns":828,"exists":"","*":"Module:Citation/CS1"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Configuration"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Whitelist"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Utilities"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Date validation"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Identifiers"},{"ns":828,"exists":"","*":"Module:Citation/CS1/COinS"},{"ns":828,"exists":"","*":"Module:Check isxn"},{"ns":828,"exists":"","*":"Module:Error"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"}],"images":["Wiki_letter_w.svg"],"externallinks":["//www.ncbi.nlm.nih.gov/pubmed/23329804","http://linkinghub.elsevier.com/retrieve/pii/S1360859212001684","//doi.org/10.1016/j.jbmt.2012.06.007","//www.ncbi.nlm.nih.gov/pubmed/23294691","//doi.org/10.1249/JSR.0000000000000148","//www.ncbi.nlm.nih.gov/pubmed/25968853","//edwardbetts.com/find_link?q=Fascia_training"],"sections":[{"toclevel":1,"level":"2","line":"Origin","number":"1","index":"1","fromtitle":"Fascia_training","byteoffset":611,"anchor":"Origin"},{"toclevel":1,"level":"2","line":"Principles","number":"2","index":"2","fromtitle":"Fascia_training","byteoffset":2746,"anchor":"Principles"},{"toclevel":1,"level":"2","line":"Evidence","number