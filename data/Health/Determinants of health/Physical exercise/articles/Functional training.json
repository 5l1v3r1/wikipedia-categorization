Funktionstr\u00e4ning"}],"categories":[{"sortkey":"Functional Training","*":"Physical_exercise"},{"sortkey":"Functional Training","*":"Weight_training"},{"sortkey":"Functional Training","*":"Aerobic_exercise"}],"links":[{"ns":0,"exists":"","*":"Back"},{"ns":0,"exists":"","*":"Balance (ability)"},{"ns":0,"exists":"","*":"Barbell"},{"ns":0,"exists":"","*":"Cable machine"},{"ns":0,"exists":"","*":"Chiropractic"},{"ns":0,"exists":"","*":"Core (anatomy)"},{"ns":0,"exists":"","*":"Digital object identifier"},{"ns":0,"exists":"","*":"Dumbbell"},{"ns":0,"exists":"","*":"Everyday life"},{"ns":0,"exists":"","*":"Exercise"},{"ns":0,"exists":"","*":"Human abdomen"},{"ns":0,"exists":"","*":"International Standard Book Number"},{"ns":0,"exists":"","*":"Kettlebell"},{"ns":0,"exists":"","*":"Marathon"},{"ns":0,"exists":"","*":"Medicine ball"},{"ns":0,"exists":"","*":"Muscle"},{"ns":0,"exists":"","*":"Muscle contraction"},{"ns":0,"exists":"","*":"Occupational therapy"},{"ns":0,"exists":"","*":"Physical therapy"},{"ns":0,"exists":"","*":"Stroke"},{"ns":0,"exists":"","*":"Therapy"},{"ns":0,"exists":"","*":"Weight bearing"},{"ns":0,"exists":"","*":"Weight machine"}],"templates":[{"ns":10,"exists":"","*":"Template:Columns-list"},{"ns":10,"exists":"","*":"Template:Div col"},{"ns":10,"exists":"","*":"Template:Column-width"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":10,"exists":"","*":"Template:Reflist"},{"ns":10,"exists":"","*":"Template:Cite book"},{"ns":10,"exists":"","*":"Template:Cite web"},{"ns":10,"exists":"","*":"Template:Cite journal"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"},{"ns":828,"exists":"","*":"Module:Citation/CS1"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Configuration"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Whitelist"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Utilities"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Date validation"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Identifiers"},{"ns":828,"exists":"","*":"Module:Citation/CS1/COinS"}],"images":[],"externallinks":["http://www.bodybuilding.com/fun/jessec4.htm","//doi.org/10.1177/1545968310368963","http://www.ebrsr.com/reviews_details.php?Upper-Extremity-Interventions-31"],"sections":[{"toclevel":1,"level":"2","line":"Origins","number":"1","index":"1","fromtitle":"Functional_training","byteoffset":158,"anchor":"Origins"},{"toclevel":1,"level":"2","line":"Evidence","number":"2","index":"2","fromtitle":"Functional_training","byteoffset":2629,"anchor":"Evidence"},{"toclevel":1,"level":"2","line":"Equipment","number":"3","index":"3","fromtitle":"Functional_training","byteoffset":4460,"anchor":"Equipment"},{"toclevel":2,"level":"3","line":"Cable machines","number":"3.1","index":"4","fromtitle":"Functional_training","byteoffset":5136,"anchor":"Cable_machines"},{"toclevel":1,"level":"2","line":"Components of a functional exercise program","number":"4","index":"5","fromtitle":"Functional_training","byteoffset":5812,"anchor":"Components_of_a_functional_exercise_program"},{"toclevel":1,"level":"2","line":"References","number":"5","index":"6","fromtitle":"Functional_training","byteoffset":7055,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Functional training","iwlinks":[],"properties":[{"name":"defaultsort","*":"Functional Training"},{"name":"wikibase_item","*":"Q1215296"}]}}