tp://www.ncgc.nih.gov/guidance/section3.html","https://web.archive.org/web/20061129102848/http://www.ncgc.nih.gov/guidance/section3.html","http://www.sigmaplot.com/products/sigmaplot/Standard_Curves_Analysis.pdf","https://web.archive.org/web/20120417121807/http://www.sigmaplot.com/products/sigmaplot/Standard_Curves_Analysis.pdf","https://web.archive.org/web/20060517233647/http://www.unmc.edu/Pharmacology/receptortutorial/competition/IC50.htm","http://pharmrev.aspetjournals.org/cgi/content/full/55/4/597"],"sections":[{"toclevel":1,"level":"2","line":"Equation","number":"1","index":"1","fromtitle":"EC50","byteoffset":2884,"anchor":"Equation"},{"toclevel":1,"level":"2","line":"Limitations","number":"2","index":"2","fromtitle":"EC50","byteoffset":3587,"anchor":"Limitations"},{"toclevel":1,"level":"2","line":"See also","number":"3","index":"3","fromtitle":"EC50","byteoffset":4073,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"4","index":"4","fromtitle":"EC50","byteoffset":4331,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"5","index":"5","fromtitle":"EC50","byteoffset":4363,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"EC50","iwlinks":[],"properties":[{"name":"defaultsort","*":"Ec50"},{"name":"wikibase_item","*":"Q286136"}]}}