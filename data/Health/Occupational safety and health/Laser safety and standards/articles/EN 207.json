xists":"","*":"Template:List of EN standards"},{"ns":10,"exists":"","*":"Template:Navbox"},{"ns":828,"exists":"","*":"Module:Unsubst"},{"ns":828,"exists":"","*":"Module:Message box"},{"ns":828,"exists":"","*":"Module:No globals"},{"ns":828,"exists":"","*":"Module:Yesno"},{"ns":828,"exists":"","*":"Module:Arguments"},{"ns":828,"exists":"","*":"Module:Message box/configuration"},{"ns":828,"exists":"","*":"Module:Category handler"},{"ns":828,"exists":"","*":"Module:Category handler/data"},{"ns":828,"exists":"","*":"Module:Category handler/config"},{"ns":828,"exists":"","*":"Module:Category handler/shared"},{"ns":828,"exists":"","*":"Module:Category handler/blacklist"},{"ns":828,"exists":"","*":"Module:Namespace detect/data"},{"ns":828,"exists":"","*":"Module:Namespace detect/config"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"},{"ns":828,"exists":"","*":"Module:Navbox"},{"ns":828,"exists":"","*":"Module:Navbar"}],"images":["Question_book-new.svg","Laser_goggles_en207.jpg"],"externallinks":["http://www.lvg.com/uvex/laservision/intl/en/home.nsf/?Open&DirectURL=7DEA869E7F02D923C12571F4006D3932"],"sections":[{"toclevel":1,"level":"2","line":"Laser working modes","number":"1","index":"1","fromtitle":"EN_207","byteoffset":1470,"anchor":"Laser_working_modes"},{"toclevel":1,"level":"2","line":"Scale numbers","number":"2","index":"2","fromtitle":"EN_207","byteoffset":1741,"anchor":"Scale_numbers"},{"toclevel":1,"level":"2","line":"References","number":"3","index":"3","fromtitle":"EN_207","byteoffset":3902,"anchor":"References"}],"parsewarnings":[],"displaytitle":"EN 207","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q5323763"}]}}