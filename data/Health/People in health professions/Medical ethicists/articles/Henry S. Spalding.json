q=editions:DuHGUcVzYd4C&hl=en&sa=X&ved=0CCwQ6AEwA2oVChMI3ubOp-PCxwIVy1s-Ch0VBggk#v=onepage&q&f=false","http://cdm.slu.edu/cdm/compoundobject/collection/woodstock/id/26560/rec/54","https://www.jstor.org/stable/2765352?seq=1#page_scan_tab_contents"],"sections":[{"toclevel":1,"level":"2","line":"Early life","number":"1","index":"1","fromtitle":"Henry_S._Spalding","byteoffset":347,"anchor":"Early_life"},{"toclevel":1,"level":"2","line":"Jesuit priest and educator","number":"2","index":"2","fromtitle":"Henry_S._Spalding","byteoffset":1515,"anchor":"Jesuit_priest_and_educator"},{"toclevel":1,"level":"2","line":"Catholic author","number":"3","index":"3","fromtitle":"Henry_S._Spalding","byteoffset":3510,"anchor":"Catholic_author"},{"toclevel":1,"level":"2","line":"Illness and death","number":"4","index":"4","fromtitle":"Henry_S._Spalding","byteoffset":5340,"anchor":"Illness_and_death"},{"toclevel":1,"level":"2","line":"Works","number":"5","index":"5","fromtitle":"Henry_S._Spalding","byteoffset":5778,"anchor":"Works"},{"toclevel":2,"level":"3","line":"Articles","number":"5.1","index":"6","fromtitle":"Henry_S._Spalding","byteoffset":7039,"anchor":"Articles"},{"toclevel":1,"level":"2","line":"References","number":"6","index":"7","fromtitle":"Henry_S._Spalding","byteoffset":7640,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Henry S. Spalding","iwlinks":[],"properties":[{"name":"defaultsort","*":"Spalding, Henry"},{"name":"wikibase_item","*":"Q22338529"}]}}