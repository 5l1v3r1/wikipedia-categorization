title":"Walter_John_Kilner","byteoffset":659,"anchor":"The_Human_Atmosphere"},{"toclevel":1,"level":"2","line":"Critical reception","number":"2","index":"2","fromtitle":"Walter_John_Kilner","byteoffset":4277,"anchor":"Critical_reception"},{"toclevel":1,"level":"2","line":"Publications","number":"3","index":"3","fromtitle":"Walter_John_Kilner","byteoffset":5849,"anchor":"Publications"},{"toclevel":1,"level":"2","line":"See also","number":"4","index":"4","fromtitle":"Walter_John_Kilner","byteoffset":5953,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"5","index":"5","fromtitle":"Walter_John_Kilner","byteoffset":6275,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Walter John Kilner","iwlinks":[],"properties":[{"name":"defaultsort","*":"Kilner, Walter John"},{"name":"wikibase_item","*":"Q16043557"}]}}