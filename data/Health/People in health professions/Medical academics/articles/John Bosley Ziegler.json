on"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Whitelist"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Utilities"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Date validation"},{"ns":828,"exists":"","*":"Module:Citation/CS1/Identifiers"},{"ns":828,"exists":"","*":"Module:Citation/CS1/COinS"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"}],"images":[],"externallinks":["http://www.drugabuse.gov/pdf/monographs/102.pdf","https://web.archive.org/web/20080528195958/http://www.la84foundation.org/SportsLibrary/JSH/JSH1993/JSH2001/jsh2001b.pdf","https://web.archive.org/web/20091027142318/http://www.geocities.com/Heartland/4547/trivia.html","http://articles.philly.com/2002-10-20/sports/25352734_1_steroids-york-barbell-chuck-yesalis","https://www.jstor.org/stable/1734568","http://www.slate.com/id/2113752/"],"sections":[{"toclevel":1,"level":"2","line":"Biography","number":"1","index":"1","fromtitle":"John_Bosley_Ziegler","byteoffset":1689,"anchor":"Biography"},{"toclevel":2,"level":"3","line":"Early life","number":"1.1","index":"2","fromtitle":"John_Bosley_Ziegler","byteoffset":1703,"anchor":"Early_life"},{"toclevel":2,"level":"3","line":"Developing anabolic steroids","number":"1.2","index":"3","fromtitle":"John_Bosley_Ziegler","byteoffset":3774,"anchor":"Developing_anabolic_steroids"},{"toclevel":1,"level":"2","line":"See also","number":"2","index":"4","fromtitle":"John_Bosley_Ziegler","byteoffset":6127,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"3","index":"5","fromtitle":"John_Bosley_Ziegler","byteoffset":6220,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"4","index":"6","fromtitle":"John_Bosley_Ziegler","byteoffset":6250,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"John Bosley Ziegler","iwlinks":[],"properties":[{"name":"defaultsort","*":"Ziegler, John"},{"name":"wikibase_item","*":"Q6222514"}]}}