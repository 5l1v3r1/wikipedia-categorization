"],"sections":[{"toclevel":1,"level":"2","line":"Background","number":"1","index":"1","fromtitle":"Physician_Quality_Reporting_System","byteoffset":454,"anchor":"Background"},{"toclevel":1,"level":"2","line":"Definition and implementation","number":"2","index":"2","fromtitle":"Physician_Quality_Reporting_System","byteoffset":2112,"anchor":"Definition_and_implementation"},{"toclevel":2,"level":"3","line":"Measure definition","number":"2.1","index":"3","fromtitle":"Physician_Quality_Reporting_System","byteoffset":2147,"anchor":"Measure_definition"},{"toclevel":2,"level":"3","line":"Eligible providers","number":"2.2","index":"4","fromtitle":"Physician_Quality_Reporting_System","byteoffset":3130,"anchor":"Eligible_providers"},{"toclevel":2,"level":"3","line":"Reporting process","number":"2.3","index":"5","fromtitle":"Physician_Quality_Reporting_System","byteoffset":4315,"anchor":"Reporting_process"},{"toclevel":1,"level":"2","line":"Outcomes","number":"3","index":"6","fromtitle":"Physician_Quality_Reporting_System","byteoffset":4898,"anchor":"Outcomes"},{"toclevel":1,"level":"2","line":"References","number":"4","index":"7","fromtitle":"Physician_Quality_Reporting_System","byteoffset":5368,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Physician Quality Reporting System","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q16999250"}]}}