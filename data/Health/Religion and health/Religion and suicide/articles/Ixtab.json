am"},{"ns":0,"exists":"","*":"Death deity"},{"ns":0,"exists":"","*":"Diego de Landa"},{"ns":0,"exists":"","*":"Dresden Codex"},{"ns":0,"exists":"","*":"Fakelore"},{"ns":0,"exists":"","*":"Psychopomp"},{"ns":0,"exists":"","*":"Religious views on suicide"},{"ns":0,"exists":"","*":"Spanish conquest of Yucat\u00e1n"},{"ns":0,"exists":"","*":"Suicide by hanging"},{"ns":0,"exists":"","*":"Xtabay"}],"templates":[{"ns":10,"exists":"","*":"Template:Quote"},{"ns":10,"exists":"","*":"Template:Quote/styles.css"},{"ns":10,"exists":"","*":"Template:Reflist"},{"ns":10,"exists":"","*":"Template:Main other"},{"ns":828,"exists":"","*":"Module:Check for unknown parameters"}],"images":["Ixtab.jpg"],"externallinks":[],"sections":[{"toclevel":1,"level":"2","line":"Sources","number":"1","index":"1","fromtitle":"Ixtab","byteoffset":332,"anchor":"Sources"},{"toclevel":1,"level":"2","line":"Comparisons","number":"2","index":"2","fromtitle":"Ixtab","byteoffset":1386,"anchor":"Comparisons"},{"toclevel":1,"level":"2","line":"Dresden Codex","number":"3","index":"3","fromtitle":"Ixtab","byteoffset":2474,"anchor":"Dresden_Codex"},{"toclevel":1,"level":"2","line":"As possible fabrication","number":"4","index":"4","fromtitle":"Ixtab","byteoffset":3030,"anchor":"As_possible_fabrication"},{"toclevel":1,"level":"2","line":"See also","number":"5","index":"5","fromtitle":"Ixtab","byteoffset":3624,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"6","index":"6","fromtitle":"Ixtab","byteoffset":3700,"anchor":"References"},{"toclevel":1,"level":"2","line":"Bibliography","number":"7","index":"7","fromtitle":"Ixtab","byteoffset":3728,"anchor":"Bibliography"}],"parsewarnings":[],"displaytitle":"Ixtab","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q1417974"}]}}