],"externallinks":["//doi.org/10.1177/096228020101000503","//doi.org/10.1093/jnci/85.12.979","http://surveillance.cancer.gov/survival/measures.html","//doi.org/10.1002/sim.1597","//doi.org/10.1093/biostatistics/kxl030","//www.ncbi.nlm.nih.gov/pubmed/17021277"],"sections":[{"toclevel":1,"level":"2","line":"Competing risks survival analysis","number":"1","index":"1","fromtitle":"Relative_survival","byteoffset":1200,"anchor":"Competing_risks_survival_analysis"},{"toclevel":1,"level":"2","line":"Hazard rate","number":"2","index":"2","fromtitle":"Relative_survival","byteoffset":2110,"anchor":"Hazard_rate"},{"toclevel":1,"level":"2","line":"Cancer survival","number":"3","index":"3","fromtitle":"Relative_survival","byteoffset":3470,"anchor":"Cancer_survival"},{"toclevel":1,"level":"2","line":"Epidemiology","number":"4","index":"4","fromtitle":"Relative_survival","byteoffset":5364,"anchor":"Epidemiology"},{"toclevel":1,"level":"2","line":"Software","number":"5","index":"5","fromtitle":"Relative_survival","byteoffset":6232,"anchor":"Software"},{"toclevel":1,"level":"2","line":"See also","number":"6","index":"6","fromtitle":"Relative_survival","byteoffset":6954,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"7","index":"7","fromtitle":"Relative_survival","byteoffset":7018,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Relative survival","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q7310803"}]}}