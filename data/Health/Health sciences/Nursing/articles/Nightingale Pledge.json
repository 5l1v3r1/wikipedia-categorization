ully%22&f=false"],"sections":[{"toclevel":1,"level":"2","line":"Versions","number":"1","index":"1","fromtitle":"Nightingale_Pledge","byteoffset":2697,"anchor":"Versions"},{"toclevel":1,"level":"2","line":"See also","number":"2","index":"2","fromtitle":"Nightingale_Pledge","byteoffset":6343,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"3","index":"3","fromtitle":"Nightingale_Pledge","byteoffset":6385,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Nightingale Pledge","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q3341419"}]}}