:"Moisture"},{"toclevel":2,"level":"3","line":"Activity","number":"1.3","index":"4","fromtitle":"Braden_Scale_for_Predicting_Pressure_Ulcer_Risk","byteoffset":1900,"anchor":"Activity"},{"toclevel":2,"level":"3","line":"Mobility","number":"1.4","index":"5","fromtitle":"Braden_Scale_for_Predicting_Pressure_Ulcer_Risk","byteoffset":2432,"anchor":"Mobility"},{"toclevel":2,"level":"3","line":"Nutrition","number":"1.5","index":"6","fromtitle":"Braden_Scale_for_Predicting_Pressure_Ulcer_Risk","byteoffset":2639,"anchor":"Nutrition"},{"toclevel":2,"level":"3","line":"Friction and Shear","number":"1.6","index":"7","fromtitle":"Braden_Scale_for_Predicting_Pressure_Ulcer_Risk","byteoffset":2863,"anchor":"Friction_and_Shear"},{"toclevel":1,"level":"2","line":"Scoring with the Braden Scale","number":"2","index":"8","fromtitle":"Braden_Scale_for_Predicting_Pressure_Ulcer_Risk","byteoffset":3422,"anchor":"Scoring_with_the_Braden_Scale"},{"toclevel":1,"level":"2","line":"See also","number":"3","index":"9","fromtitle":"Braden_Scale_for_Predicting_Pressure_Ulcer_Risk","byteoffset":4326,"anchor":"See_also"},{"toclevel":1,"level":"2","line":"References","number":"4","index":"10","fromtitle":"Braden_Scale_for_Predicting_Pressure_Ulcer_Risk","byteoffset":4402,"anchor":"References"},{"toclevel":1,"level":"2","line":"External links","number":"5","index":"11","fromtitle":"Braden_Scale_for_Predicting_Pressure_Ulcer_Risk","byteoffset":4430,"anchor":"External_links"}],"parsewarnings":[],"displaytitle":"Braden Scale for Predicting Pressure Ulcer Risk","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q4954485"}]}}