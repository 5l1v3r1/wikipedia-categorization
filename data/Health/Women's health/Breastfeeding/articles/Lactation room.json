"Lactation_room","byteoffset":1280,"anchor":"Popularity"},{"toclevel":1,"level":"2","line":"Benefits","number":"3","index":"3","fromtitle":"Lactation_room","byteoffset":2080,"anchor":"Benefits"},{"toclevel":1,"level":"2","line":"Resources","number":"4","index":"4","fromtitle":"Lactation_room","byteoffset":3914,"anchor":"Resources"},{"toclevel":1,"level":"2","line":"Notes","number":"5","index":"5","fromtitle":"Lactation_room","byteoffset":4697,"anchor":"Notes"}],"parsewarnings":[],"displaytitle":"Lactation room","iwlinks":[{"prefix":"commons","url":"https://commons.wikimedia.org/wiki/Category:Lactation_rooms","*":"commons:Category:Lactation rooms"}],"properties":[{"name":"wikibase_item","*":"Q6468983"}]}}