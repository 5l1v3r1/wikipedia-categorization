cal Procedure Use","number":"3","index":"3","fromtitle":"Hegar_dilators","byteoffset":1808,"anchor":"Cervical_Procedure_Use"},{"toclevel":1,"level":"2","line":"Penile Implant Procedure Use","number":"4","index":"4","fromtitle":"Hegar_dilators","byteoffset":3209,"anchor":"Penile_Implant_Procedure_Use"},{"toclevel":1,"level":"2","line":"References","number":"5","index":"5","fromtitle":"Hegar_dilators","byteoffset":3975,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Hegar dilators","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q1593697"}]}}