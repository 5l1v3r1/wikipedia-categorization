},{"ns":0,"exists":"","*":"Time"},{"ns":0,"exists":"","*":"Tomato"},{"ns":0,"exists":"","*":"Underwood Canning Company"},{"ns":0,"exists":"","*":"University"},{"ns":0,"exists":"","*":"Watertown, Massachusetts"},{"ns":0,"exists":"","*":"William Lyman Underwood"},{"ns":0,"exists":"","*":"William Thompson Sedgwick"},{"ns":0,"exists":"","*":"Z-value"}],"templates":[{"ns":10,"exists":"","*":"Template:Convert"},{"ns":828,"exists":"","*":"Module:Convert"},{"ns":828,"exists":"","*":"Module:Convert/data"},{"ns":828,"exists":"","*":"Module:Convert/text"}],"images":[],"externallinks":["http://www.dairyscience.info/newcalculators/d2.asp","https://web.archive.org/web/20010217172541/http://cifmc.foodsci.purdue.edu/","https://web.archive.org/web/20070929140026/http://www.fda.gov/ora/inspect_ref/itg/itg7.html","https://web.archive.org/web/20061019162237/http://www.bgfoods.com/underwood/underwood_about.asp"],"sections":[{"toclevel":1,"level":"2","line":"History","number":"1","index":"1","fromtitle":"Thermal_death_time","byteoffset":292,"anchor":"History"},{"toclevel":1,"level":"2","line":"Mathematical formulas","number":"2","index":"2","fromtitle":"Thermal_death_time","byteoffset":2096,"anchor":"Mathematical_formulas"},{"toclevel":2,"level":"3","line":"Graphical method","number":"2.1","index":"3","fromtitle":"Thermal_death_time","byteoffset":2234,"anchor":"Graphical_method"},{"toclevel":2,"level":"3","line":"Formula method","number":"2.2","index":"4","fromtitle":"Thermal_death_time","byteoffset":2931,"anchor":"Formula_method"},{"toclevel":1,"level":"2","line":"Applications","number":"3","index":"5","fromtitle":"Thermal_death_time","byteoffset":4377,"anchor":"Applications"},{"toclevel":1,"level":"2","line":"References","number":"4","index":"6","fromtitle":"Thermal_death_time","byteoffset":6121,"anchor":"References"}],"parsewarnings":[],"displaytitle":"Thermal death time","iwlinks":[],"properties":[{"name":"wikibase_item","*":"Q17086186"}]}}